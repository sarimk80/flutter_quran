      "number": 85,
      "name": "سورة البروج",
      "englishName": "Al-Burooj",
      "englishNameTranslation": "The Constellations",
      "revelationType": "Meccan",
      "numberOfAyahs": 22,
      "ayahs": [
        {
          "number": 5910,
          "text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ وَٱلسَّمَآءِ ذَاتِ ٱلْبُرُوجِ",
          "numberInSurah": 1,
          "juz": 30,
          "manzil": 7,
          "page": 590,
          "ruku": 527,
          "hizbQuarter": 236,
          "sajda": false
        },
        {
          "number": 5911,
          "text": "وَٱلْيَوْمِ ٱلْمَوْعُودِ",
          "numberInSurah": 2,
          "juz": 30,
          "manzil": 7,
          "page": 590,
          "ruku": 527,
          "hizbQuarter": 236,
          "sajda": false
        },
        {
          "number": 5912,
          "text": "وَشَاهِدٍۢ وَمَشْهُودٍۢ",
          "numberInSurah": 3,
          "juz": 30,
          "manzil": 7,
          "page": 590,
          "ruku": 527,
          "hizbQuarter": 236,
          "sajda": false
        },
        {
          "number": 5913,
          "text": "قُتِلَ أَصْحَٰبُ ٱلْأُخْدُودِ",
          "numberInSurah": 4,
          "juz": 30,
          "manzil": 7,
          "page": 590,
          "ruku": 527,
          "hizbQuarter": 236,
          "sajda": false
        },
        {
          "number": 5914,
          "text": "ٱلنَّارِ ذَاتِ ٱلْوَقُودِ",
          "numberInSurah": 5,
          "juz": 30,
          "manzil": 7,
          "page": 590,
          "ruku": 527,
          "hizbQuarter": 236,
          "sajda": false
        },
        {
          "number": 5915,
          "text": "إِذْ هُمْ عَلَيْهَا قُعُودٌۭ",
          "numberInSurah": 6,
          "juz": 30,
          "manzil": 7,
          "page": 590,
          "ruku": 527,
          "hizbQuarter": 236,
          "sajda": false
        },
        {
          "number": 5916,
          "text": "وَهُمْ عَلَىٰ مَا يَفْعَلُونَ بِٱلْمُؤْمِنِينَ شُهُودٌۭ",
          "numberInSurah": 7,
          "juz": 30,
          "manzil": 7,
          "page": 590,
          "ruku": 527,
          "hizbQuarter": 236,
          "sajda": false
        },
        {
          "number": 5917,
          "text": "وَمَا نَقَمُوا۟ مِنْهُمْ إِلَّآ أَن يُؤْمِنُوا۟ بِٱللَّهِ ٱلْعَزِيزِ ٱلْحَمِيدِ",
          "numberInSurah": 8,
          "juz": 30,
          "manzil": 7,
          "page": 590,
          "ruku": 527,
          "hizbQuarter": 236,
          "sajda": false
        },
        {
          "number": 5918,
          "text": "ٱلَّذِى لَهُۥ مُلْكُ ٱلسَّمَٰوَٰتِ وَٱلْأَرْضِ ۚ وَٱللَّهُ عَلَىٰ كُلِّ شَىْءٍۢ شَهِيدٌ",
          "numberInSurah": 9,
          "juz": 30,
          "manzil": 7,
          "page": 590,
          "ruku": 527,
          "hizbQuarter": 236,
          "sajda": false
        },
        {
          "number": 5919,
          "text": "إِنَّ ٱلَّذِينَ فَتَنُوا۟ ٱلْمُؤْمِنِينَ وَٱلْمُؤْمِنَٰتِ ثُمَّ لَمْ يَتُوبُوا۟ فَلَهُمْ عَذَابُ جَهَنَّمَ وَلَهُمْ عَذَابُ ٱلْحَرِيقِ",
          "numberInSurah": 10,
          "juz": 30,
          "manzil": 7,
          "page": 590,
          "ruku": 527,
          "hizbQuarter": 236,
          "sajda": false
        },
        {
          "number": 5920,
          "text": "إِنَّ ٱلَّذِينَ ءَامَنُوا۟ وَعَمِلُوا۟ ٱلصَّٰلِحَٰتِ لَهُمْ جَنَّٰتٌۭ تَجْرِى مِن تَحْتِهَا ٱلْأَنْهَٰرُ ۚ ذَٰلِكَ ٱلْفَوْزُ ٱلْكَبِيرُ",
          "numberInSurah": 11,
          "juz": 30,
          "manzil": 7,
          "page": 590,
          "ruku": 527,
          "hizbQuarter": 236,
          "sajda": false
        },
        {
          "number": 5921,
          "text": "إِنَّ بَطْشَ رَبِّكَ لَشَدِيدٌ",
          "numberInSurah": 12,
          "juz": 30,
          "manzil": 7,
          "page": 590,
          "ruku": 527,
          "hizbQuarter": 236,
          "sajda": false
        },
        {
          "number": 5922,
          "text": "إِنَّهُۥ هُوَ يُبْدِئُ وَيُعِيدُ",
          "numberInSurah": 13,
          "juz": 30,
          "manzil": 7,
          "page": 590,
          "ruku": 527,
          "hizbQuarter": 236,
          "sajda": false
        },
        {
          "number": 5923,
          "text": "وَهُوَ ٱلْغَفُورُ ٱلْوَدُودُ",
          "numberInSurah": 14,
          "juz": 30,
          "manzil": 7,
          "page": 590,
          "ruku": 527,
          "hizbQuarter": 236,
          "sajda": false
        },
        {
          "number": 5924,
          "text": "ذُو ٱلْعَرْشِ ٱلْمَجِيدُ",
          "numberInSurah": 15,
          "juz": 30,
          "manzil": 7,
          "page": 590,
          "ruku": 527,
          "hizbQuarter": 236,
          "sajda": false
        },
        {
          "number": 5925,
          "text": "فَعَّالٌۭ لِّمَا يُرِيدُ",
          "numberInSurah": 16,
          "juz": 30,
          "manzil": 7,
          "page": 590,
          "ruku": 527,
          "hizbQuarter": 236,
          "sajda": false
        },
        {
          "number": 5926,
          "text": "هَلْ أَتَىٰكَ حَدِيثُ ٱلْجُنُودِ",
          "numberInSurah": 17,
          "juz": 30,
          "manzil": 7,
          "page": 590,
          "ruku": 527,
          "hizbQuarter": 236,
          "sajda": false
        },
        {
          "number": 5927,
          "text": "فِرْعَوْنَ وَثَمُودَ",
          "numberInSurah": 18,
          "juz": 30,
          "manzil": 7,
          "page": 590,
          "ruku": 527,
          "hizbQuarter": 236,
          "sajda": false
        },
        {
          "number": 5928,
          "text": "بَلِ ٱلَّذِينَ كَفَرُوا۟ فِى تَكْذِيبٍۢ",
          "numberInSurah": 19,
          "juz": 30,
          "manzil": 7,
          "page": 590,
          "ruku": 527,
          "hizbQuarter": 236,
          "sajda": false
        },
        {
          "number": 5929,
          "text": "وَٱللَّهُ مِن وَرَآئِهِم مُّحِيطٌۢ",
          "numberInSurah": 20,
          "juz": 30,
          "manzil": 7,
          "page": 590,
          "ruku": 527,
          "hizbQuarter": 236,
          "sajda": false
        },
        {
          "number": 5930,
          "text": "بَلْ هُوَ قُرْءَانٌۭ مَّجِيدٌۭ",
          "numberInSurah": 21,
          "juz": 30,
          "manzil": 7,
          "page": 590,
          "ruku": 527,
          "hizbQuarter": 236,
          "sajda": false
        },
        {
