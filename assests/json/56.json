          "page": 536,
          "ruku": 470,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 5048,
          "text": "ءَأَنتُمْ أَنزَلْتُمُوهُ مِنَ ٱلْمُزْنِ أَمْ نَحْنُ ٱلْمُنزِلُونَ",
          "numberInSurah": 69,
          "juz": 27,
          "manzil": 7,
          "page": 536,
          "ruku": 470,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 5049,
          "text": "لَوْ نَشَآءُ جَعَلْنَٰهُ أُجَاجًۭا فَلَوْلَا تَشْكُرُونَ",
          "numberInSurah": 70,
          "juz": 27,
          "manzil": 7,
          "page": 536,
          "ruku": 470,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 5050,
          "text": "أَفَرَءَيْتُمُ ٱلنَّارَ ٱلَّتِى تُورُونَ",
          "numberInSurah": 71,
          "juz": 27,
          "manzil": 7,
          "page": 536,
          "ruku": 470,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 5051,
          "text": "ءَأَنتُمْ أَنشَأْتُمْ شَجَرَتَهَآ أَمْ نَحْنُ ٱلْمُنشِـُٔونَ",
          "numberInSurah": 72,
          "juz": 27,
          "manzil": 7,
          "page": 536,
          "ruku": 470,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 5052,
          "text": "نَحْنُ جَعَلْنَٰهَا تَذْكِرَةًۭ وَمَتَٰعًۭا لِّلْمُقْوِينَ",
          "numberInSurah": 73,
