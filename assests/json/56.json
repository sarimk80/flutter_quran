          "manzil": 7,
          "page": 536,
          "ruku": 470,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 5047,
          "text": "أَفَرَءَيْتُمُ ٱلْمَآءَ ٱلَّذِى تَشْرَبُونَ",
          "numberInSurah": 68,
