          "page": 536,
          "ruku": 471,
          "hizbQuarter": 215,
          "sajda": false
        },
        {
          "number": 5056,
          "text": "إِنَّهُۥ لَقُرْءَانٌۭ كَرِيمٌۭ",
          "numberInSurah": 77,
          "juz": 27,
          "manzil": 7,
          "page": 537,
          "ruku": 471,
          "hizbQuarter": 215,
          "sajda": false
        },
