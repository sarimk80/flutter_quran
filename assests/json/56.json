  "data": [
    {
      "number": 56,
      "name": "سورة الواقعة",
      "englishName": "Al-Waaqia",
      "englishNameTranslation": "The Inevitable",
      "revelationType": "Meccan",
      "numberOfAyahs": 96,
      "ayahs": [
        {
          "number": 4980,
          "text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ إِذَا وَقَعَتِ ٱلْوَاقِعَةُ",
          "numberInSurah": 1,
          "juz": 27,
          "manzil": 7,
          "page": 534,
          "ruku": 469,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 4981,
          "text": "لَيْسَ لِوَقْعَتِهَا كَاذِبَةٌ",
          "numberInSurah": 2,
          "juz": 27,
          "manzil": 7,
          "page": 534,
          "ruku": 469,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 4982,
          "text": "خَافِضَةٌۭ رَّافِعَةٌ",
          "numberInSurah": 3,
          "juz": 27,
          "manzil": 7,
          "page": 534,
          "ruku": 469,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 4983,
          "text": "إِذَا رُجَّتِ ٱلْأَرْضُ رَجًّۭا",
          "numberInSurah": 4,
          "juz": 27,
          "manzil": 7,
          "page": 534,
          "ruku": 469,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 4984,
          "text": "وَبُسَّتِ ٱلْجِبَالُ بَسًّۭا",
          "numberInSurah": 5,
          "juz": 27,
          "manzil": 7,
          "page": 534,
          "ruku": 469,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 4985,
          "text": "فَكَانَتْ هَبَآءًۭ مُّنۢبَثًّۭا",
          "numberInSurah": 6,
          "juz": 27,
          "manzil": 7,
          "page": 534,
          "ruku": 469,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 4986,
          "text": "وَكُنتُمْ أَزْوَٰجًۭا ثَلَٰثَةًۭ",
          "numberInSurah": 7,
          "juz": 27,
          "manzil": 7,
          "page": 534,
          "ruku": 469,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 4987,
          "text": "فَأَصْحَٰبُ ٱلْمَيْمَنَةِ مَآ أَصْحَٰبُ ٱلْمَيْمَنَةِ",
          "numberInSurah": 8,
          "juz": 27,
          "manzil": 7,
          "page": 534,
          "ruku": 469,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 4988,
          "text": "وَأَصْحَٰبُ ٱلْمَشْـَٔمَةِ مَآ أَصْحَٰبُ ٱلْمَشْـَٔمَةِ",
          "numberInSurah": 9,
          "juz": 27,
          "manzil": 7,
          "page": 534,
          "ruku": 469,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 4989,
          "text": "وَٱلسَّٰبِقُونَ ٱلسَّٰبِقُونَ",
          "numberInSurah": 10,
          "juz": 27,
          "manzil": 7,
          "page": 534,
          "ruku": 469,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 4990,
          "text": "أُو۟لَٰٓئِكَ ٱلْمُقَرَّبُونَ",
          "numberInSurah": 11,
          "juz": 27,
          "manzil": 7,
          "page": 534,
          "ruku": 469,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 4991,
          "text": "فِى جَنَّٰتِ ٱلنَّعِيمِ",
          "numberInSurah": 12,
          "juz": 27,
          "manzil": 7,
          "page": 534,
          "ruku": 469,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 4992,
          "text": "ثُلَّةٌۭ مِّنَ ٱلْأَوَّلِينَ",
          "numberInSurah": 13,
          "juz": 27,
          "manzil": 7,
          "page": 534,
          "ruku": 469,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 4993,
          "text": "وَقَلِيلٌۭ مِّنَ ٱلْءَاخِرِينَ",
          "numberInSurah": 14,
          "juz": 27,
          "manzil": 7,
          "page": 534,
          "ruku": 469,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 4994,
          "text": "عَلَىٰ سُرُرٍۢ مَّوْضُونَةٍۢ",
          "numberInSurah": 15,
          "juz": 27,
          "manzil": 7,
          "page": 534,
          "ruku": 469,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 4995,
          "text": "مُّتَّكِـِٔينَ عَلَيْهَا مُتَقَٰبِلِينَ",
          "numberInSurah": 16,
          "juz": 27,
          "manzil": 7,
          "page": 534,
          "ruku": 469,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 4996,
          "text": "يَطُوفُ عَلَيْهِمْ وِلْدَٰنٌۭ مُّخَلَّدُونَ",
          "numberInSurah": 17,
          "juz": 27,
          "manzil": 7,
          "page": 535,
          "ruku": 469,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 4997,
          "text": "بِأَكْوَابٍۢ وَأَبَارِيقَ وَكَأْسٍۢ مِّن مَّعِينٍۢ",
          "numberInSurah": 18,
          "juz": 27,
          "manzil": 7,
          "page": 535,
          "ruku": 469,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 4998,
          "text": "لَّا يُصَدَّعُونَ عَنْهَا وَلَا يُنزِفُونَ",
          "numberInSurah": 19,
          "juz": 27,
          "manzil": 7,
          "page": 535,
          "ruku": 469,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 4999,
          "text": "وَفَٰكِهَةٍۢ مِّمَّا يَتَخَيَّرُونَ",
          "numberInSurah": 20,
          "juz": 27,
          "manzil": 7,
          "page": 535,
          "ruku": 469,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 5000,
          "text": "وَلَحْمِ طَيْرٍۢ مِّمَّا يَشْتَهُونَ",
          "numberInSurah": 21,
          "juz": 27,
          "manzil": 7,
          "page": 535,
          "ruku": 469,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 5001,
          "text": "وَحُورٌ عِينٌۭ",
          "numberInSurah": 22,
          "juz": 27,
          "manzil": 7,
          "page": 535,
          "ruku": 469,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 5002,
          "text": "كَأَمْثَٰلِ ٱللُّؤْلُؤِ ٱلْمَكْنُونِ",
          "numberInSurah": 23,
          "juz": 27,
          "manzil": 7,
          "page": 535,
          "ruku": 469,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 5003,
          "text": "جَزَآءًۢ بِمَا كَانُوا۟ يَعْمَلُونَ",
          "numberInSurah": 24,
          "juz": 27,
          "manzil": 7,
          "page": 535,
          "ruku": 469,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 5004,
          "text": "لَا يَسْمَعُونَ فِيهَا لَغْوًۭا وَلَا تَأْثِيمًا",
          "numberInSurah": 25,
          "juz": 27,
          "manzil": 7,
          "page": 535,
          "ruku": 469,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 5005,
          "text": "إِلَّا قِيلًۭا سَلَٰمًۭا سَلَٰمًۭا",
          "numberInSurah": 26,
          "juz": 27,
          "manzil": 7,
          "page": 535,
          "ruku": 469,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 5006,
          "text": "وَأَصْحَٰبُ ٱلْيَمِينِ مَآ أَصْحَٰبُ ٱلْيَمِينِ",
          "numberInSurah": 27,
          "juz": 27,
          "manzil": 7,
          "page": 535,
          "ruku": 469,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 5007,
          "text": "فِى سِدْرٍۢ مَّخْضُودٍۢ",
          "numberInSurah": 28,
          "juz": 27,
          "manzil": 7,
          "page": 535,
          "ruku": 469,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 5008,
          "text": "وَطَلْحٍۢ مَّنضُودٍۢ",
          "numberInSurah": 29,
          "juz": 27,
          "manzil": 7,
          "page": 535,
          "ruku": 469,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 5009,
          "text": "وَظِلٍّۢ مَّمْدُودٍۢ",
          "numberInSurah": 30,
          "juz": 27,
          "manzil": 7,
          "page": 535,
          "ruku": 469,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 5010,
          "text": "وَمَآءٍۢ مَّسْكُوبٍۢ",
          "numberInSurah": 31,
          "juz": 27,
          "manzil": 7,
          "page": 535,
          "ruku": 469,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 5011,
          "text": "وَفَٰكِهَةٍۢ كَثِيرَةٍۢ",
          "numberInSurah": 32,
          "juz": 27,
          "manzil": 7,
          "page": 535,
          "ruku": 469,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 5012,
          "text": "لَّا مَقْطُوعَةٍۢ وَلَا مَمْنُوعَةٍۢ",
          "numberInSurah": 33,
          "juz": 27,
          "manzil": 7,
          "page": 535,
          "ruku": 469,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 5013,
          "text": "وَفُرُشٍۢ مَّرْفُوعَةٍ",
          "numberInSurah": 34,
          "juz": 27,
          "manzil": 7,
          "page": 535,
          "ruku": 469,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 5014,
          "text": "إِنَّآ أَنشَأْنَٰهُنَّ إِنشَآءًۭ",
          "numberInSurah": 35,
          "juz": 27,
          "manzil": 7,
          "page": 535,
          "ruku": 469,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 5015,
          "text": "فَجَعَلْنَٰهُنَّ أَبْكَارًا",
          "numberInSurah": 36,
          "juz": 27,
          "manzil": 7,
          "page": 535,
          "ruku": 469,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 5016,
          "text": "عُرُبًا أَتْرَابًۭا",
          "numberInSurah": 37,
          "juz": 27,
          "manzil": 7,
          "page": 535,
          "ruku": 469,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 5017,
          "text": "لِّأَصْحَٰبِ ٱلْيَمِينِ",
          "numberInSurah": 38,
          "juz": 27,
          "manzil": 7,
          "page": 535,
          "ruku": 469,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 5018,
          "text": "ثُلَّةٌۭ مِّنَ ٱلْأَوَّلِينَ",
          "numberInSurah": 39,
          "juz": 27,
          "manzil": 7,
          "page": 535,
          "ruku": 470,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 5019,
          "text": "وَثُلَّةٌۭ مِّنَ ٱلْءَاخِرِينَ",
          "numberInSurah": 40,
          "juz": 27,
          "manzil": 7,
          "page": 535,
          "ruku": 470,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 5020,
          "text": "وَأَصْحَٰبُ ٱلشِّمَالِ مَآ أَصْحَٰبُ ٱلشِّمَالِ",
          "numberInSurah": 41,
          "juz": 27,
          "manzil": 7,
          "page": 535,
          "ruku": 470,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 5021,
          "text": "فِى سَمُومٍۢ وَحَمِيمٍۢ",
          "numberInSurah": 42,
          "juz": 27,
          "manzil": 7,
          "page": 535,
          "ruku": 470,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 5022,
          "text": "وَظِلٍّۢ مِّن يَحْمُومٍۢ",
          "numberInSurah": 43,
          "juz": 27,
          "manzil": 7,
          "page": 535,
          "ruku": 470,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 5023,
          "text": "لَّا بَارِدٍۢ وَلَا كَرِيمٍ",
          "numberInSurah": 44,
          "juz": 27,
          "manzil": 7,
          "page": 535,
          "ruku": 470,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 5024,
          "text": "إِنَّهُمْ كَانُوا۟ قَبْلَ ذَٰلِكَ مُتْرَفِينَ",
          "numberInSurah": 45,
          "juz": 27,
          "manzil": 7,
          "page": 535,
          "ruku": 470,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 5025,
          "text": "وَكَانُوا۟ يُصِرُّونَ عَلَى ٱلْحِنثِ ٱلْعَظِيمِ",
          "numberInSurah": 46,
          "juz": 27,
          "manzil": 7,
          "page": 535,
          "ruku": 470,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 5026,
          "text": "وَكَانُوا۟ يَقُولُونَ أَئِذَا مِتْنَا وَكُنَّا تُرَابًۭا وَعِظَٰمًا أَءِنَّا لَمَبْعُوثُونَ",
          "numberInSurah": 47,
          "juz": 27,
          "manzil": 7,
          "page": 535,
          "ruku": 470,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 5027,
          "text": "أَوَءَابَآؤُنَا ٱلْأَوَّلُونَ",
          "numberInSurah": 48,
          "juz": 27,
          "manzil": 7,
          "page": 535,
          "ruku": 470,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 5028,
          "text": "قُلْ إِنَّ ٱلْأَوَّلِينَ وَٱلْءَاخِرِينَ",
          "numberInSurah": 49,
          "juz": 27,
          "manzil": 7,
          "page": 535,
          "ruku": 470,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 5029,
          "text": "لَمَجْمُوعُونَ إِلَىٰ مِيقَٰتِ يَوْمٍۢ مَّعْلُومٍۢ",
          "numberInSurah": 50,
          "juz": 27,
          "manzil": 7,
          "page": 535,
          "ruku": 470,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 5030,
          "text": "ثُمَّ إِنَّكُمْ أَيُّهَا ٱلضَّآلُّونَ ٱلْمُكَذِّبُونَ",
          "numberInSurah": 51,
          "juz": 27,
          "manzil": 7,
          "page": 536,
          "ruku": 470,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 5031,
          "text": "لَءَاكِلُونَ مِن شَجَرٍۢ مِّن زَقُّومٍۢ",
          "numberInSurah": 52,
          "juz": 27,
          "manzil": 7,
          "page": 536,
          "ruku": 470,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 5032,
          "text": "فَمَالِـُٔونَ مِنْهَا ٱلْبُطُونَ",
          "numberInSurah": 53,
          "juz": 27,
          "manzil": 7,
          "page": 536,
          "ruku": 470,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 5033,
          "text": "فَشَٰرِبُونَ عَلَيْهِ مِنَ ٱلْحَمِيمِ",
          "numberInSurah": 54,
          "juz": 27,
          "manzil": 7,
          "page": 536,
          "ruku": 470,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 5034,
          "text": "فَشَٰرِبُونَ شُرْبَ ٱلْهِيمِ",
          "numberInSurah": 55,
          "juz": 27,
          "manzil": 7,
          "page": 536,
          "ruku": 470,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 5035,
          "text": "هَٰذَا نُزُلُهُمْ يَوْمَ ٱلدِّينِ",
          "numberInSurah": 56,
          "juz": 27,
          "manzil": 7,
          "page": 536,
          "ruku": 470,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 5036,
          "text": "نَحْنُ خَلَقْنَٰكُمْ فَلَوْلَا تُصَدِّقُونَ",
          "numberInSurah": 57,
          "juz": 27,
          "manzil": 7,
          "page": 536,
          "ruku": 470,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 5037,
          "text": "أَفَرَءَيْتُم مَّا تُمْنُونَ",
          "numberInSurah": 58,
          "juz": 27,
          "manzil": 7,
          "page": 536,
          "ruku": 470,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 5038,
          "text": "ءَأَنتُمْ تَخْلُقُونَهُۥٓ أَمْ نَحْنُ ٱلْخَٰلِقُونَ",
          "numberInSurah": 59,
          "juz": 27,
          "manzil": 7,
          "page": 536,
          "ruku": 470,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 5039,
          "text": "نَحْنُ قَدَّرْنَا بَيْنَكُمُ ٱلْمَوْتَ وَمَا نَحْنُ بِمَسْبُوقِينَ",
          "numberInSurah": 60,
          "juz": 27,
          "manzil": 7,
          "page": 536,
          "ruku": 470,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 5040,
          "text": "عَلَىٰٓ أَن نُّبَدِّلَ أَمْثَٰلَكُمْ وَنُنشِئَكُمْ فِى مَا لَا تَعْلَمُونَ",
          "numberInSurah": 61,
          "juz": 27,
          "manzil": 7,
          "page": 536,
          "ruku": 470,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 5041,
          "text": "وَلَقَدْ عَلِمْتُمُ ٱلنَّشْأَةَ ٱلْأُولَىٰ فَلَوْلَا تَذَكَّرُونَ",
          "numberInSurah": 62,
          "juz": 27,
          "manzil": 7,
          "page": 536,
          "ruku": 470,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 5042,
          "text": "أَفَرَءَيْتُم مَّا تَحْرُثُونَ",
          "numberInSurah": 63,
          "juz": 27,
          "manzil": 7,
          "page": 536,
          "ruku": 470,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 5043,
          "text": "ءَأَنتُمْ تَزْرَعُونَهُۥٓ أَمْ نَحْنُ ٱلزَّٰرِعُونَ",
          "numberInSurah": 64,
          "juz": 27,
          "manzil": 7,
          "page": 536,
          "ruku": 470,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 5044,
          "text": "لَوْ نَشَآءُ لَجَعَلْنَٰهُ حُطَٰمًۭا فَظَلْتُمْ تَفَكَّهُونَ",
          "numberInSurah": 65,
          "juz": 27,
          "manzil": 7,
          "page": 536,
          "ruku": 470,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
          "number": 5045,
          "text": "إِنَّا لَمُغْرَمُونَ",
          "numberInSurah": 66,
          "juz": 27,
          "manzil": 7,
          "page": 536,
          "ruku": 470,
          "hizbQuarter": 214,
          "sajda": false
        },
        {
