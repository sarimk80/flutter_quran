          "juz": 27,
          "manzil": 7,
          "page": 537,
          "ruku": 471,
          "hizbQuarter": 215,
          "sajda": false
        },
        {
          "number": 5060,
          "text": "أَفَبِهَٰذَا ٱلْحَدِيثِ أَنتُم مُّدْهِنُونَ",
