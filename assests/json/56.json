          "manzil": 7,
          "page": 537,
          "ruku": 471,
          "hizbQuarter": 215,
          "sajda": false
        },
        {
          "number": 5058,
          "text": "لَّا يَمَسُّهُۥٓ إِلَّا ٱلْمُطَهَّرُونَ",
          "numberInSurah": 79,
          "juz": 27,
          "manzil": 7,
          "page": 537,
          "ruku": 471,
          "hizbQuarter": 215,
          "sajda": false
        },
