          "text": "مِن نُّطْفَةٍ إِذَا تُمْنَىٰ",
          "numberInSurah": 46,
          "juz": 27,
          "manzil": 7,
          "page": 528,
          "ruku": 462,
          "hizbQuarter": 211,
          "sajda": false
        },
        {
          "number": 4831,
          "text": "وَأَنَّ عَلَيْهِ ٱلنَّشْأَةَ ٱلْأُخْرَىٰ",
          "numberInSurah": 47,
          "juz": 27,
          "manzil": 7,
          "page": 528,
          "ruku": 462,
          "hizbQuarter": 211,
          "sajda": false
        },
        {
          "number": 4832,
          "text": "وَأَنَّهُۥ هُوَ أَغْنَىٰ وَأَقْنَىٰ",
          "numberInSurah": 48,
          "juz": 27,
          "manzil": 7,
          "page": 528,
          "ruku": 462,
          "hizbQuarter": 211,
          "sajda": false
        },
        {
          "number": 4833,
          "text": "وَأَنَّهُۥ هُوَ رَبُّ ٱلشِّعْرَىٰ",
          "numberInSurah": 49,
          "juz": 27,
          "manzil": 7,
          "page": 528,
          "ruku": 462,
          "hizbQuarter": 211,
          "sajda": false
        },
        {
          "number": 4834,
          "text": "وَأَنَّهُۥٓ أَهْلَكَ عَادًا ٱلْأُولَىٰ",
          "numberInSurah": 50,
          "juz": 27,
          "manzil": 7,
          "page": 528,
          "ruku": 462,
          "hizbQuarter": 211,
          "sajda": false
        },
        {
          "number": 4835,
          "text": "وَثَمُودَا۟ فَمَآ أَبْقَىٰ",
