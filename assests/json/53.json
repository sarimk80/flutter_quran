    {
      "number": 53,
      "name": "سورة النجم",
      "englishName": "An-Najm",
      "englishNameTranslation": "The Star",
      "revelationType": "Meccan",
      "numberOfAyahs": 62,
      "ayahs": [
        {
          "number": 4785,
          "text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ وَٱلنَّجْمِ إِذَا هَوَىٰ",
          "numberInSurah": 1,
          "juz": 27,
          "manzil": 7,
          "page": 526,
          "ruku": 460,
          "hizbQuarter": 210,
          "sajda": false
        },
        {
          "number": 4786,
          "text": "مَا ضَلَّ صَاحِبُكُمْ وَمَا غَوَىٰ",
          "numberInSurah": 2,
          "juz": 27,
          "manzil": 7,
          "page": 526,
          "ruku": 460,
          "hizbQuarter": 210,
          "sajda": false
        },
        {
          "number": 4787,
          "text": "وَمَا يَنطِقُ عَنِ ٱلْهَوَىٰٓ",
          "numberInSurah": 3,
          "juz": 27,
          "manzil": 7,
          "page": 526,
          "ruku": 460,
          "hizbQuarter": 210,
          "sajda": false
        },
        {
          "number": 4788,
          "text": "إِنْ هُوَ إِلَّا وَحْىٌۭ يُوحَىٰ",
          "numberInSurah": 4,
          "juz": 27,
          "manzil": 7,
          "page": 526,
          "ruku": 460,
          "hizbQuarter": 210,
          "sajda": false
        },
        {
          "number": 4789,
          "text": "عَلَّمَهُۥ شَدِيدُ ٱلْقُوَىٰ",
          "numberInSurah": 5,
          "juz": 27,
          "manzil": 7,
          "page": 526,
          "ruku": 460,
          "hizbQuarter": 210,
          "sajda": false
        },
        {
          "number": 4790,
          "text": "ذُو مِرَّةٍۢ فَٱسْتَوَىٰ",
          "numberInSurah": 6,
          "juz": 27,
          "manzil": 7,
          "page": 526,
          "ruku": 460,
          "hizbQuarter": 210,
          "sajda": false
        },
        {
          "number": 4791,
          "text": "وَهُوَ بِٱلْأُفُقِ ٱلْأَعْلَىٰ",
          "numberInSurah": 7,
          "juz": 27,
          "manzil": 7,
          "page": 526,
          "ruku": 460,
          "hizbQuarter": 210,
          "sajda": false
        },
        {
          "number": 4792,
          "text": "ثُمَّ دَنَا فَتَدَلَّىٰ",
          "numberInSurah": 8,
          "juz": 27,
          "manzil": 7,
          "page": 526,
          "ruku": 460,
          "hizbQuarter": 210,
          "sajda": false
        },
        {
          "number": 4793,
          "text": "فَكَانَ قَابَ قَوْسَيْنِ أَوْ أَدْنَىٰ",
          "numberInSurah": 9,
          "juz": 27,
          "manzil": 7,
          "page": 526,
          "ruku": 460,
          "hizbQuarter": 210,
          "sajda": false
        },
        {
          "number": 4794,
          "text": "فَأَوْحَىٰٓ إِلَىٰ عَبْدِهِۦ مَآ أَوْحَىٰ",
          "numberInSurah": 10,
          "juz": 27,
          "manzil": 7,
          "page": 526,
          "ruku": 460,
          "hizbQuarter": 210,
          "sajda": false
        },
        {
          "number": 4795,
          "text": "مَا كَذَبَ ٱلْفُؤَادُ مَا رَأَىٰٓ",
          "numberInSurah": 11,
          "juz": 27,
          "manzil": 7,
          "page": 526,
          "ruku": 460,
          "hizbQuarter": 210,
          "sajda": false
        },
        {
          "number": 4796,
          "text": "أَفَتُمَٰرُونَهُۥ عَلَىٰ مَا يَرَىٰ",
          "numberInSurah": 12,
          "juz": 27,
          "manzil": 7,
          "page": 526,
          "ruku": 460,
          "hizbQuarter": 210,
          "sajda": false
        },
        {
          "number": 4797,
          "text": "وَلَقَدْ رَءَاهُ نَزْلَةً أُخْرَىٰ",
          "numberInSurah": 13,
          "juz": 27,
          "manzil": 7,
          "page": 526,
          "ruku": 460,
          "hizbQuarter": 210,
          "sajda": false
        },
        {
          "number": 4798,
          "text": "عِندَ سِدْرَةِ ٱلْمُنتَهَىٰ",
          "numberInSurah": 14,
          "juz": 27,
          "manzil": 7,
          "page": 526,
          "ruku": 460,
          "hizbQuarter": 210,
          "sajda": false
        },
        {
          "number": 4799,
          "text": "عِندَهَا جَنَّةُ ٱلْمَأْوَىٰٓ",
          "numberInSurah": 15,
          "juz": 27,
          "manzil": 7,
          "page": 526,
          "ruku": 460,
          "hizbQuarter": 210,
          "sajda": false
        },
        {
          "number": 4800,
          "text": "إِذْ يَغْشَى ٱلسِّدْرَةَ مَا يَغْشَىٰ",
          "numberInSurah": 16,
          "juz": 27,
          "manzil": 7,
          "page": 526,
          "ruku": 460,
          "hizbQuarter": 210,
          "sajda": false
        },
        {
          "number": 4801,
          "text": "مَا زَاغَ ٱلْبَصَرُ وَمَا طَغَىٰ",
          "numberInSurah": 17,
          "juz": 27,
          "manzil": 7,
          "page": 526,
          "ruku": 460,
          "hizbQuarter": 210,
          "sajda": false
        },
        {
          "number": 4802,
          "text": "لَقَدْ رَأَىٰ مِنْ ءَايَٰتِ رَبِّهِ ٱلْكُبْرَىٰٓ",
          "numberInSurah": 18,
          "juz": 27,
          "manzil": 7,
          "page": 526,
          "ruku": 460,
          "hizbQuarter": 210,
          "sajda": false
        },
        {
          "number": 4803,
          "text": "أَفَرَءَيْتُمُ ٱللَّٰتَ وَٱلْعُزَّىٰ",
          "numberInSurah": 19,
          "juz": 27,
          "manzil": 7,
          "page": 526,
          "ruku": 460,
          "hizbQuarter": 210,
          "sajda": false
        },
        {
          "number": 4804,
          "text": "وَمَنَوٰةَ ٱلثَّالِثَةَ ٱلْأُخْرَىٰٓ",
          "numberInSurah": 20,
          "juz": 27,
          "manzil": 7,
          "page": 526,
          "ruku": 460,
          "hizbQuarter": 210,
          "sajda": false
        },
        {
          "number": 4805,
          "text": "أَلَكُمُ ٱلذَّكَرُ وَلَهُ ٱلْأُنثَىٰ",
          "numberInSurah": 21,
          "juz": 27,
          "manzil": 7,
          "page": 526,
          "ruku": 460,
          "hizbQuarter": 210,
          "sajda": false
        },
        {
          "number": 4806,
          "text": "تِلْكَ إِذًۭا قِسْمَةٌۭ ضِيزَىٰٓ",
          "numberInSurah": 22,
          "juz": 27,
          "manzil": 7,
          "page": 526,
          "ruku": 460,
          "hizbQuarter": 210,
          "sajda": false
        },
        {
          "number": 4807,
          "text": "إِنْ هِىَ إِلَّآ أَسْمَآءٌۭ سَمَّيْتُمُوهَآ أَنتُمْ وَءَابَآؤُكُم مَّآ أَنزَلَ ٱللَّهُ بِهَا مِن سُلْطَٰنٍ ۚ إِن يَتَّبِعُونَ إِلَّا ٱلظَّنَّ وَمَا تَهْوَى ٱلْأَنفُسُ ۖ وَلَقَدْ جَآءَهُم مِّن رَّبِّهِمُ ٱلْهُدَىٰٓ",
          "numberInSurah": 23,
          "juz": 27,
          "manzil": 7,
          "page": 526,
          "ruku": 460,
          "hizbQuarter": 210,
          "sajda": false
        },
        {
          "number": 4808,
          "text": "أَمْ لِلْإِنسَٰنِ مَا تَمَنَّىٰ",
          "numberInSurah": 24,
          "juz": 27,
          "manzil": 7,
          "page": 526,
          "ruku": 460,
          "hizbQuarter": 210,
          "sajda": false
        },
        {
          "number": 4809,
          "text": "فَلِلَّهِ ٱلْءَاخِرَةُ وَٱلْأُولَىٰ",
          "numberInSurah": 25,
          "juz": 27,
          "manzil": 7,
          "page": 526,
          "ruku": 460,
          "hizbQuarter": 210,
          "sajda": false
        },
        {
          "number": 4810,
          "text": "۞ وَكَم مِّن مَّلَكٍۢ فِى ٱلسَّمَٰوَٰتِ لَا تُغْنِى شَفَٰعَتُهُمْ شَيْـًٔا إِلَّا مِنۢ بَعْدِ أَن يَأْذَنَ ٱللَّهُ لِمَن يَشَآءُ وَيَرْضَىٰٓ",
          "numberInSurah": 26,
          "juz": 27,
          "manzil": 7,
          "page": 526,
          "ruku": 461,
          "hizbQuarter": 211,
          "sajda": false
        },
        {
          "number": 4811,
          "text": "إِنَّ ٱلَّذِينَ لَا يُؤْمِنُونَ بِٱلْءَاخِرَةِ لَيُسَمُّونَ ٱلْمَلَٰٓئِكَةَ تَسْمِيَةَ ٱلْأُنثَىٰ",
          "numberInSurah": 27,
          "juz": 27,
          "manzil": 7,
          "page": 527,
          "ruku": 461,
          "hizbQuarter": 211,
          "sajda": false
        },
        {
          "number": 4812,
          "text": "وَمَا لَهُم بِهِۦ مِنْ عِلْمٍ ۖ إِن يَتَّبِعُونَ إِلَّا ٱلظَّنَّ ۖ وَإِنَّ ٱلظَّنَّ لَا يُغْنِى مِنَ ٱلْحَقِّ شَيْـًۭٔا",
          "numberInSurah": 28,
          "juz": 27,
          "manzil": 7,
          "page": 527,
          "ruku": 461,
          "hizbQuarter": 211,
          "sajda": false
        },
        {
          "number": 4813,
          "text": "فَأَعْرِضْ عَن مَّن تَوَلَّىٰ عَن ذِكْرِنَا وَلَمْ يُرِدْ إِلَّا ٱلْحَيَوٰةَ ٱلدُّنْيَا",
          "numberInSurah": 29,
          "juz": 27,
          "manzil": 7,
          "page": 527,
          "ruku": 461,
          "hizbQuarter": 211,
          "sajda": false
        },
        {
          "number": 4814,
          "text": "ذَٰلِكَ مَبْلَغُهُم مِّنَ ٱلْعِلْمِ ۚ إِنَّ رَبَّكَ هُوَ أَعْلَمُ بِمَن ضَلَّ عَن سَبِيلِهِۦ وَهُوَ أَعْلَمُ بِمَنِ ٱهْتَدَىٰ",
          "numberInSurah": 30,
          "juz": 27,
          "manzil": 7,
          "page": 527,
          "ruku": 461,
          "hizbQuarter": 211,
          "sajda": false
        },
        {
          "number": 4815,
          "text": "وَلِلَّهِ مَا فِى ٱلسَّمَٰوَٰتِ وَمَا فِى ٱلْأَرْضِ لِيَجْزِىَ ٱلَّذِينَ أَسَٰٓـُٔوا۟ بِمَا عَمِلُوا۟ وَيَجْزِىَ ٱلَّذِينَ أَحْسَنُوا۟ بِٱلْحُسْنَى",
          "numberInSurah": 31,
          "juz": 27,
          "manzil": 7,
          "page": 527,
          "ruku": 461,
          "hizbQuarter": 211,
          "sajda": false
        },
        {
          "number": 4816,
          "text": "ٱلَّذِينَ يَجْتَنِبُونَ كَبَٰٓئِرَ ٱلْإِثْمِ وَٱلْفَوَٰحِشَ إِلَّا ٱللَّمَمَ ۚ إِنَّ رَبَّكَ وَٰسِعُ ٱلْمَغْفِرَةِ ۚ هُوَ أَعْلَمُ بِكُمْ إِذْ أَنشَأَكُم مِّنَ ٱلْأَرْضِ وَإِذْ أَنتُمْ أَجِنَّةٌۭ فِى بُطُونِ أُمَّهَٰتِكُمْ ۖ فَلَا تُزَكُّوٓا۟ أَنفُسَكُمْ ۖ هُوَ أَعْلَمُ بِمَنِ ٱتَّقَىٰٓ",
          "numberInSurah": 32,
          "juz": 27,
          "manzil": 7,
          "page": 527,
          "ruku": 461,
          "hizbQuarter": 211,
          "sajda": false
        },
        {
          "number": 4817,
          "text": "أَفَرَءَيْتَ ٱلَّذِى تَوَلَّىٰ",
          "numberInSurah": 33,
          "juz": 27,
          "manzil": 7,
          "page": 527,
          "ruku": 462,
          "hizbQuarter": 211,
          "sajda": false
        },
        {
          "number": 4818,
          "text": "وَأَعْطَىٰ قَلِيلًۭا وَأَكْدَىٰٓ",
          "numberInSurah": 34,
          "juz": 27,
          "manzil": 7,
          "page": 527,
          "ruku": 462,
          "hizbQuarter": 211,
          "sajda": false
        },
        {
          "number": 4819,
          "text": "أَعِندَهُۥ عِلْمُ ٱلْغَيْبِ فَهُوَ يَرَىٰٓ",
          "numberInSurah": 35,
          "juz": 27,
          "manzil": 7,
          "page": 527,
          "ruku": 462,
          "hizbQuarter": 211,
          "sajda": false
        },
        {
          "number": 4820,
          "text": "أَمْ لَمْ يُنَبَّأْ بِمَا فِى صُحُفِ مُوسَىٰ",
          "numberInSurah": 36,
          "juz": 27,
          "manzil": 7,
          "page": 527,
          "ruku": 462,
          "hizbQuarter": 211,
          "sajda": false
        },
        {
          "number": 4821,
          "text": "وَإِبْرَٰهِيمَ ٱلَّذِى وَفَّىٰٓ",
          "numberInSurah": 37,
          "juz": 27,
          "manzil": 7,
          "page": 527,
          "ruku": 462,
          "hizbQuarter": 211,
          "sajda": false
        },
        {
          "number": 4822,
          "text": "أَلَّا تَزِرُ وَازِرَةٌۭ وِزْرَ أُخْرَىٰ",
          "numberInSurah": 38,
          "juz": 27,
          "manzil": 7,
          "page": 527,
          "ruku": 462,
          "hizbQuarter": 211,
          "sajda": false
        },
        {
          "number": 4823,
          "text": "وَأَن لَّيْسَ لِلْإِنسَٰنِ إِلَّا مَا سَعَىٰ",
          "numberInSurah": 39,
          "juz": 27,
          "manzil": 7,
          "page": 527,
          "ruku": 462,
          "hizbQuarter": 211,
          "sajda": false
        },
        {
          "number": 4824,
          "text": "وَأَنَّ سَعْيَهُۥ سَوْفَ يُرَىٰ",
          "numberInSurah": 40,
          "juz": 27,
          "manzil": 7,
          "page": 527,
          "ruku": 462,
          "hizbQuarter": 211,
          "sajda": false
        },
        {
          "number": 4825,
          "text": "ثُمَّ يُجْزَىٰهُ ٱلْجَزَآءَ ٱلْأَوْفَىٰ",
          "numberInSurah": 41,
          "juz": 27,
          "manzil": 7,
          "page": 527,
          "ruku": 462,
          "hizbQuarter": 211,
          "sajda": false
        },
        {
          "number": 4826,
          "text": "وَأَنَّ إِلَىٰ رَبِّكَ ٱلْمُنتَهَىٰ",
          "numberInSurah": 42,
          "juz": 27,
          "manzil": 7,
          "page": 527,
          "ruku": 462,
          "hizbQuarter": 211,
          "sajda": false
        },
        {
          "number": 4827,
          "text": "وَأَنَّهُۥ هُوَ أَضْحَكَ وَأَبْكَىٰ",
          "numberInSurah": 43,
          "juz": 27,
          "manzil": 7,
          "page": 527,
          "ruku": 462,
          "hizbQuarter": 211,
          "sajda": false
        },
        {
          "number": 4828,
          "text": "وَأَنَّهُۥ هُوَ أَمَاتَ وَأَحْيَا",
          "numberInSurah": 44,
          "juz": 27,
          "manzil": 7,
          "page": 527,
          "ruku": 462,
          "hizbQuarter": 211,
          "sajda": false
        },
        {
          "number": 4829,
          "text": "وَأَنَّهُۥ خَلَقَ ٱلزَّوْجَيْنِ ٱلذَّكَرَ وَٱلْأُنثَىٰ",
          "numberInSurah": 45,
          "juz": 27,
          "manzil": 7,
          "page": 528,
          "ruku": 462,
          "hizbQuarter": 211,
          "sajda": false
        },
        {
