  "data": [
    {
      "number": 69,
      "name": "سورة الحاقة",
      "englishName": "Al-Haaqqa",
      "englishNameTranslation": "The Reality",
      "revelationType": "Meccan",
      "numberOfAyahs": 52,
      "ayahs": [
        {
          "number": 5324,
          "text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ ٱلْحَآقَّةُ",
          "numberInSurah": 1,
          "juz": 29,
          "manzil": 7,
          "page": 566,
          "ruku": 500,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5325,
          "text": "مَا ٱلْحَآقَّةُ",
          "numberInSurah": 2,
          "juz": 29,
          "manzil": 7,
          "page": 566,
          "ruku": 500,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5326,
          "text": "وَمَآ أَدْرَىٰكَ مَا ٱلْحَآقَّةُ",
          "numberInSurah": 3,
          "juz": 29,
          "manzil": 7,
          "page": 566,
          "ruku": 500,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5327,
          "text": "كَذَّبَتْ ثَمُودُ وَعَادٌۢ بِٱلْقَارِعَةِ",
          "numberInSurah": 4,
          "juz": 29,
          "manzil": 7,
          "page": 566,
          "ruku": 500,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5328,
          "text": "فَأَمَّا ثَمُودُ فَأُهْلِكُوا۟ بِٱلطَّاغِيَةِ",
          "numberInSurah": 5,
          "juz": 29,
          "manzil": 7,
          "page": 566,
          "ruku": 500,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5329,
          "text": "وَأَمَّا عَادٌۭ فَأُهْلِكُوا۟ بِرِيحٍۢ صَرْصَرٍ عَاتِيَةٍۢ",
          "numberInSurah": 6,
          "juz": 29,
          "manzil": 7,
          "page": 566,
          "ruku": 500,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5330,
          "text": "سَخَّرَهَا عَلَيْهِمْ سَبْعَ لَيَالٍۢ وَثَمَٰنِيَةَ أَيَّامٍ حُسُومًۭا فَتَرَى ٱلْقَوْمَ فِيهَا صَرْعَىٰ كَأَنَّهُمْ أَعْجَازُ نَخْلٍ خَاوِيَةٍۢ",
          "numberInSurah": 7,
          "juz": 29,
          "manzil": 7,
          "page": 566,
          "ruku": 500,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5331,
          "text": "فَهَلْ تَرَىٰ لَهُم مِّنۢ بَاقِيَةٍۢ",
          "numberInSurah": 8,
          "juz": 29,
          "manzil": 7,
          "page": 566,
          "ruku": 500,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5332,
          "text": "وَجَآءَ فِرْعَوْنُ وَمَن قَبْلَهُۥ وَٱلْمُؤْتَفِكَٰتُ بِٱلْخَاطِئَةِ",
          "numberInSurah": 9,
          "juz": 29,
          "manzil": 7,
          "page": 567,
          "ruku": 500,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5333,
          "text": "فَعَصَوْا۟ رَسُولَ رَبِّهِمْ فَأَخَذَهُمْ أَخْذَةًۭ رَّابِيَةً",
          "numberInSurah": 10,
          "juz": 29,
          "manzil": 7,
          "page": 567,
          "ruku": 500,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5334,
          "text": "إِنَّا لَمَّا طَغَا ٱلْمَآءُ حَمَلْنَٰكُمْ فِى ٱلْجَارِيَةِ",
          "numberInSurah": 11,
          "juz": 29,
          "manzil": 7,
          "page": 567,
          "ruku": 500,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5335,
          "text": "لِنَجْعَلَهَا لَكُمْ تَذْكِرَةًۭ وَتَعِيَهَآ أُذُنٌۭ وَٰعِيَةٌۭ",
          "numberInSurah": 12,
          "juz": 29,
          "manzil": 7,
          "page": 567,
          "ruku": 500,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5336,
          "text": "فَإِذَا نُفِخَ فِى ٱلصُّورِ نَفْخَةٌۭ وَٰحِدَةٌۭ",
          "numberInSurah": 13,
          "juz": 29,
          "manzil": 7,
          "page": 567,
          "ruku": 500,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5337,
          "text": "وَحُمِلَتِ ٱلْأَرْضُ وَٱلْجِبَالُ فَدُكَّتَا دَكَّةًۭ وَٰحِدَةًۭ",
          "numberInSurah": 14,
          "juz": 29,
          "manzil": 7,
          "page": 567,
          "ruku": 500,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5338,
          "text": "فَيَوْمَئِذٍۢ وَقَعَتِ ٱلْوَاقِعَةُ",
          "numberInSurah": 15,
          "juz": 29,
          "manzil": 7,
          "page": 567,
          "ruku": 500,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5339,
          "text": "وَٱنشَقَّتِ ٱلسَّمَآءُ فَهِىَ يَوْمَئِذٍۢ وَاهِيَةٌۭ",
          "numberInSurah": 16,
          "juz": 29,
          "manzil": 7,
          "page": 567,
          "ruku": 500,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5340,
          "text": "وَٱلْمَلَكُ عَلَىٰٓ أَرْجَآئِهَا ۚ وَيَحْمِلُ عَرْشَ رَبِّكَ فَوْقَهُمْ يَوْمَئِذٍۢ ثَمَٰنِيَةٌۭ",
          "numberInSurah": 17,
          "juz": 29,
          "manzil": 7,
          "page": 567,
          "ruku": 500,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5341,
          "text": "يَوْمَئِذٍۢ تُعْرَضُونَ لَا تَخْفَىٰ مِنكُمْ خَافِيَةٌۭ",
          "numberInSurah": 18,
          "juz": 29,
          "manzil": 7,
          "page": 567,
          "ruku": 500,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5342,
          "text": "فَأَمَّا مَنْ أُوتِىَ كِتَٰبَهُۥ بِيَمِينِهِۦ فَيَقُولُ هَآؤُمُ ٱقْرَءُوا۟ كِتَٰبِيَهْ",
          "numberInSurah": 19,
          "juz": 29,
          "manzil": 7,
          "page": 567,
          "ruku": 500,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5343,
          "text": "إِنِّى ظَنَنتُ أَنِّى مُلَٰقٍ حِسَابِيَهْ",
          "numberInSurah": 20,
          "juz": 29,
          "manzil": 7,
          "page": 567,
          "ruku": 500,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5344,
          "text": "فَهُوَ فِى عِيشَةٍۢ رَّاضِيَةٍۢ",
          "numberInSurah": 21,
          "juz": 29,
          "manzil": 7,
          "page": 567,
          "ruku": 500,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5345,
          "text": "فِى جَنَّةٍ عَالِيَةٍۢ",
          "numberInSurah": 22,
          "juz": 29,
          "manzil": 7,
          "page": 567,
          "ruku": 500,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5346,
          "text": "قُطُوفُهَا دَانِيَةٌۭ",
          "numberInSurah": 23,
          "juz": 29,
          "manzil": 7,
          "page": 567,
          "ruku": 500,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5347,
          "text": "كُلُوا۟ وَٱشْرَبُوا۟ هَنِيٓـًٔۢا بِمَآ أَسْلَفْتُمْ فِى ٱلْأَيَّامِ ٱلْخَالِيَةِ",
          "numberInSurah": 24,
          "juz": 29,
          "manzil": 7,
          "page": 567,
          "ruku": 500,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5348,
          "text": "وَأَمَّا مَنْ أُوتِىَ كِتَٰبَهُۥ بِشِمَالِهِۦ فَيَقُولُ يَٰلَيْتَنِى لَمْ أُوتَ كِتَٰبِيَهْ",
          "numberInSurah": 25,
          "juz": 29,
          "manzil": 7,
          "page": 567,
          "ruku": 500,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5349,
          "text": "وَلَمْ أَدْرِ مَا حِسَابِيَهْ",
          "numberInSurah": 26,
          "juz": 29,
          "manzil": 7,
          "page": 567,
          "ruku": 500,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5350,
          "text": "يَٰلَيْتَهَا كَانَتِ ٱلْقَاضِيَةَ",
          "numberInSurah": 27,
          "juz": 29,
          "manzil": 7,
          "page": 567,
          "ruku": 500,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5351,
          "text": "مَآ أَغْنَىٰ عَنِّى مَالِيَهْ ۜ",
          "numberInSurah": 28,
          "juz": 29,
          "manzil": 7,
          "page": 567,
          "ruku": 500,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5352,
          "text": "هَلَكَ عَنِّى سُلْطَٰنِيَهْ",
          "numberInSurah": 29,
          "juz": 29,
          "manzil": 7,
          "page": 567,
          "ruku": 500,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5353,
          "text": "خُذُوهُ فَغُلُّوهُ",
          "numberInSurah": 30,
          "juz": 29,
          "manzil": 7,
          "page": 567,
          "ruku": 500,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5354,
          "text": "ثُمَّ ٱلْجَحِيمَ صَلُّوهُ",
          "numberInSurah": 31,
          "juz": 29,
          "manzil": 7,
          "page": 567,
          "ruku": 500,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5355,
          "text": "ثُمَّ فِى سِلْسِلَةٍۢ ذَرْعُهَا سَبْعُونَ ذِرَاعًۭا فَٱسْلُكُوهُ",
          "numberInSurah": 32,
          "juz": 29,
          "manzil": 7,
          "page": 567,
          "ruku": 500,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5356,
          "text": "إِنَّهُۥ كَانَ لَا يُؤْمِنُ بِٱللَّهِ ٱلْعَظِيمِ",
          "numberInSurah": 33,
          "juz": 29,
          "manzil": 7,
          "page": 567,
          "ruku": 500,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5357,
          "text": "وَلَا يَحُضُّ عَلَىٰ طَعَامِ ٱلْمِسْكِينِ",
          "numberInSurah": 34,
          "juz": 29,
          "manzil": 7,
          "page": 567,
          "ruku": 500,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5358,
          "text": "فَلَيْسَ لَهُ ٱلْيَوْمَ هَٰهُنَا حَمِيمٌۭ",
          "numberInSurah": 35,
          "juz": 29,
          "manzil": 7,
          "page": 568,
          "ruku": 500,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5359,
          "text": "وَلَا طَعَامٌ إِلَّا مِنْ غِسْلِينٍۢ",
          "numberInSurah": 36,
          "juz": 29,
          "manzil": 7,
          "page": 568,
          "ruku": 500,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5360,
          "text": "لَّا يَأْكُلُهُۥٓ إِلَّا ٱلْخَٰطِـُٔونَ",
          "numberInSurah": 37,
          "juz": 29,
          "manzil": 7,
          "page": 568,
          "ruku": 500,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5361,
          "text": "فَلَآ أُقْسِمُ بِمَا تُبْصِرُونَ",
          "numberInSurah": 38,
          "juz": 29,
          "manzil": 7,
          "page": 568,
          "ruku": 501,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5362,
          "text": "وَمَا لَا تُبْصِرُونَ",
          "numberInSurah": 39,
          "juz": 29,
          "manzil": 7,
          "page": 568,
          "ruku": 501,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5363,
          "text": "إِنَّهُۥ لَقَوْلُ رَسُولٍۢ كَرِيمٍۢ",
          "numberInSurah": 40,
          "juz": 29,
          "manzil": 7,
          "page": 568,
          "ruku": 501,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5364,
          "text": "وَمَا هُوَ بِقَوْلِ شَاعِرٍۢ ۚ قَلِيلًۭا مَّا تُؤْمِنُونَ",
          "numberInSurah": 41,
          "juz": 29,
          "manzil": 7,
          "page": 568,
          "ruku": 501,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5365,
          "text": "وَلَا بِقَوْلِ كَاهِنٍۢ ۚ قَلِيلًۭا مَّا تَذَكَّرُونَ",
          "numberInSurah": 42,
          "juz": 29,
          "manzil": 7,
          "page": 568,
          "ruku": 501,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5366,
          "text": "تَنزِيلٌۭ مِّن رَّبِّ ٱلْعَٰلَمِينَ",
          "numberInSurah": 43,
          "juz": 29,
          "manzil": 7,
          "page": 568,
          "ruku": 501,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5367,
          "text": "وَلَوْ تَقَوَّلَ عَلَيْنَا بَعْضَ ٱلْأَقَاوِيلِ",
          "numberInSurah": 44,
          "juz": 29,
          "manzil": 7,
          "page": 568,
          "ruku": 501,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5368,
          "text": "لَأَخَذْنَا مِنْهُ بِٱلْيَمِينِ",
          "numberInSurah": 45,
          "juz": 29,
          "manzil": 7,
          "page": 568,
          "ruku": 501,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5369,
          "text": "ثُمَّ لَقَطَعْنَا مِنْهُ ٱلْوَتِينَ",
          "numberInSurah": 46,
          "juz": 29,
          "manzil": 7,
          "page": 568,
          "ruku": 501,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5370,
          "text": "فَمَا مِنكُم مِّنْ أَحَدٍ عَنْهُ حَٰجِزِينَ",
          "numberInSurah": 47,
          "juz": 29,
          "manzil": 7,
          "page": 568,
          "ruku": 501,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5371,
          "text": "وَإِنَّهُۥ لَتَذْكِرَةٌۭ لِّلْمُتَّقِينَ",
          "numberInSurah": 48,
          "juz": 29,
          "manzil": 7,
          "page": 568,
          "ruku": 501,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5372,
          "text": "وَإِنَّا لَنَعْلَمُ أَنَّ مِنكُم مُّكَذِّبِينَ",
          "numberInSurah": 49,
          "juz": 29,
          "manzil": 7,
          "page": 568,
          "ruku": 501,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5373,
          "text": "وَإِنَّهُۥ لَحَسْرَةٌ عَلَى ٱلْكَٰفِرِينَ",
          "numberInSurah": 50,
          "juz": 29,
          "manzil": 7,
          "page": 568,
          "ruku": 501,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5374,
          "text": "وَإِنَّهُۥ لَحَقُّ ٱلْيَقِينِ",
          "numberInSurah": 51,
          "juz": 29,
          "manzil": 7,
          "page": 568,
          "ruku": 501,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5375,
          "text": "فَسَبِّحْ بِٱسْمِ رَبِّكَ ٱلْعَظِيمِ",
          "numberInSurah": 52,
          "juz": 29,
