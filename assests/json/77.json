          "manzil": 7,
          "page": 580,
          "ruku": 516,
          "hizbQuarter": 232,
          "sajda": false
        },
        {
          "number": 5630,
          "text": "فَإِذَا ٱلنُّجُومُ طُمِسَتْ",
