  "data": [
    {
      "number": 80,
      "name": "سورة عبس",
      "englishName": "Abasa",
      "englishNameTranslation": "He frowned",
      "revelationType": "Meccan",
      "numberOfAyahs": 42,
      "ayahs": [
        {
          "number": 5759,
          "text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ عَبَسَ وَتَوَلَّىٰٓ",
          "numberInSurah": 1,
          "juz": 30,
          "manzil": 7,
          "page": 585,
          "ruku": 522,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5760,
          "text": "أَن جَآءَهُ ٱلْأَعْمَىٰ",
          "numberInSurah": 2,
          "juz": 30,
          "manzil": 7,
          "page": 585,
          "ruku": 522,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5761,
          "text": "وَمَا يُدْرِيكَ لَعَلَّهُۥ يَزَّكَّىٰٓ",
          "numberInSurah": 3,
          "juz": 30,
          "manzil": 7,
          "page": 585,
          "ruku": 522,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5762,
          "text": "أَوْ يَذَّكَّرُ فَتَنفَعَهُ ٱلذِّكْرَىٰٓ",
          "numberInSurah": 4,
          "juz": 30,
          "manzil": 7,
          "page": 585,
          "ruku": 522,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5763,
          "text": "أَمَّا مَنِ ٱسْتَغْنَىٰ",
          "numberInSurah": 5,
          "juz": 30,
          "manzil": 7,
          "page": 585,
          "ruku": 522,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5764,
          "text": "فَأَنتَ لَهُۥ تَصَدَّىٰ",
          "numberInSurah": 6,
          "juz": 30,
          "manzil": 7,
          "page": 585,
          "ruku": 522,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5765,
          "text": "وَمَا عَلَيْكَ أَلَّا يَزَّكَّىٰ",
          "numberInSurah": 7,
          "juz": 30,
          "manzil": 7,
          "page": 585,
          "ruku": 522,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5766,
          "text": "وَأَمَّا مَن جَآءَكَ يَسْعَىٰ",
          "numberInSurah": 8,
          "juz": 30,
          "manzil": 7,
          "page": 585,
          "ruku": 522,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5767,
          "text": "وَهُوَ يَخْشَىٰ",
          "numberInSurah": 9,
          "juz": 30,
          "manzil": 7,
          "page": 585,
          "ruku": 522,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5768,
          "text": "فَأَنتَ عَنْهُ تَلَهَّىٰ",
          "numberInSurah": 10,
          "juz": 30,
          "manzil": 7,
          "page": 585,
          "ruku": 522,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5769,
          "text": "كَلَّآ إِنَّهَا تَذْكِرَةٌۭ",
          "numberInSurah": 11,
          "juz": 30,
          "manzil": 7,
          "page": 585,
          "ruku": 522,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5770,
          "text": "فَمَن شَآءَ ذَكَرَهُۥ",
          "numberInSurah": 12,
          "juz": 30,
          "manzil": 7,
          "page": 585,
          "ruku": 522,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5771,
          "text": "فِى صُحُفٍۢ مُّكَرَّمَةٍۢ",
          "numberInSurah": 13,
          "juz": 30,
          "manzil": 7,
          "page": 585,
          "ruku": 522,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5772,
          "text": "مَّرْفُوعَةٍۢ مُّطَهَّرَةٍۭ",
          "numberInSurah": 14,
          "juz": 30,
          "manzil": 7,
          "page": 585,
          "ruku": 522,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5773,
          "text": "بِأَيْدِى سَفَرَةٍۢ",
          "numberInSurah": 15,
          "juz": 30,
          "manzil": 7,
          "page": 585,
          "ruku": 522,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5774,
          "text": "كِرَامٍۭ بَرَرَةٍۢ",
          "numberInSurah": 16,
          "juz": 30,
          "manzil": 7,
          "page": 585,
          "ruku": 522,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5775,
          "text": "قُتِلَ ٱلْإِنسَٰنُ مَآ أَكْفَرَهُۥ",
          "numberInSurah": 17,
          "juz": 30,
          "manzil": 7,
          "page": 585,
          "ruku": 522,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5776,
          "text": "مِنْ أَىِّ شَىْءٍ خَلَقَهُۥ",
          "numberInSurah": 18,
          "juz": 30,
          "manzil": 7,
          "page": 585,
          "ruku": 522,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5777,
          "text": "مِن نُّطْفَةٍ خَلَقَهُۥ فَقَدَّرَهُۥ",
          "numberInSurah": 19,
          "juz": 30,
          "manzil": 7,
          "page": 585,
          "ruku": 522,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5778,
          "text": "ثُمَّ ٱلسَّبِيلَ يَسَّرَهُۥ",
          "numberInSurah": 20,
          "juz": 30,
          "manzil": 7,
          "page": 585,
          "ruku": 522,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5779,
          "text": "ثُمَّ أَمَاتَهُۥ فَأَقْبَرَهُۥ",
          "numberInSurah": 21,
          "juz": 30,
          "manzil": 7,
          "page": 585,
          "ruku": 522,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5780,
          "text": "ثُمَّ إِذَا شَآءَ أَنشَرَهُۥ",
          "numberInSurah": 22,
          "juz": 30,
          "manzil": 7,
          "page": 585,
          "ruku": 522,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5781,
          "text": "كَلَّا لَمَّا يَقْضِ مَآ أَمَرَهُۥ",
          "numberInSurah": 23,
          "juz": 30,
          "manzil": 7,
          "page": 585,
          "ruku": 522,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5782,
          "text": "فَلْيَنظُرِ ٱلْإِنسَٰنُ إِلَىٰ طَعَامِهِۦٓ",
          "numberInSurah": 24,
          "juz": 30,
          "manzil": 7,
          "page": 585,
          "ruku": 522,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5783,
          "text": "أَنَّا صَبَبْنَا ٱلْمَآءَ صَبًّۭا",
          "numberInSurah": 25,
          "juz": 30,
          "manzil": 7,
          "page": 585,
          "ruku": 522,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5784,
          "text": "ثُمَّ شَقَقْنَا ٱلْأَرْضَ شَقًّۭا",
          "numberInSurah": 26,
          "juz": 30,
          "manzil": 7,
          "page": 585,
          "ruku": 522,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5785,
          "text": "فَأَنۢبَتْنَا فِيهَا حَبًّۭا",
          "numberInSurah": 27,
          "juz": 30,
          "manzil": 7,
          "page": 585,
          "ruku": 522,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5786,
          "text": "وَعِنَبًۭا وَقَضْبًۭا",
          "numberInSurah": 28,
          "juz": 30,
          "manzil": 7,
          "page": 585,
          "ruku": 522,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5787,
          "text": "وَزَيْتُونًۭا وَنَخْلًۭا",
          "numberInSurah": 29,
          "juz": 30,
          "manzil": 7,
          "page": 585,
          "ruku": 522,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5788,
          "text": "وَحَدَآئِقَ غُلْبًۭا",
          "numberInSurah": 30,
          "juz": 30,
          "manzil": 7,
          "page": 585,
          "ruku": 522,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5789,
          "text": "وَفَٰكِهَةًۭ وَأَبًّۭا",
          "numberInSurah": 31,
          "juz": 30,
          "manzil": 7,
          "page": 585,
          "ruku": 522,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5790,
          "text": "مَّتَٰعًۭا لَّكُمْ وَلِأَنْعَٰمِكُمْ",
          "numberInSurah": 32,
          "juz": 30,
          "manzil": 7,
          "page": 585,
          "ruku": 522,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5791,
          "text": "فَإِذَا جَآءَتِ ٱلصَّآخَّةُ",
          "numberInSurah": 33,
          "juz": 30,
          "manzil": 7,
          "page": 585,
          "ruku": 522,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5792,
          "text": "يَوْمَ يَفِرُّ ٱلْمَرْءُ مِنْ أَخِيهِ",
          "numberInSurah": 34,
          "juz": 30,
          "manzil": 7,
          "page": 585,
          "ruku": 522,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5793,
          "text": "وَأُمِّهِۦ وَأَبِيهِ",
          "numberInSurah": 35,
          "juz": 30,
          "manzil": 7,
          "page": 585,
          "ruku": 522,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5794,
          "text": "وَصَٰحِبَتِهِۦ وَبَنِيهِ",
          "numberInSurah": 36,
          "juz": 30,
          "manzil": 7,
          "page": 585,
          "ruku": 522,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5795,
          "text": "لِكُلِّ ٱمْرِئٍۢ مِّنْهُمْ يَوْمَئِذٍۢ شَأْنٌۭ يُغْنِيهِ",
          "numberInSurah": 37,
          "juz": 30,
          "manzil": 7,
          "page": 585,
          "ruku": 522,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5796,
          "text": "وُجُوهٌۭ يَوْمَئِذٍۢ مُّسْفِرَةٌۭ",
          "numberInSurah": 38,
          "juz": 30,
          "manzil": 7,
          "page": 585,
          "ruku": 522,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5797,
          "text": "ضَاحِكَةٌۭ مُّسْتَبْشِرَةٌۭ",
          "numberInSurah": 39,
          "juz": 30,
          "manzil": 7,
          "page": 585,
          "ruku": 522,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5798,
          "text": "وَوُجُوهٌۭ يَوْمَئِذٍ عَلَيْهَا غَبَرَةٌۭ",
          "numberInSurah": 40,
          "juz": 30,
          "manzil": 7,
          "page": 585,
          "ruku": 522,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5799,
          "text": "تَرْهَقُهَا قَتَرَةٌ",
          "numberInSurah": 41,
          "juz": 30,
          "manzil": 7,
          "page": 585,
          "ruku": 522,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5800,
          "text": "أُو۟لَٰٓئِكَ هُمُ ٱلْكَفَرَةُ ٱلْفَجَرَةُ",
          "numberInSurah": 42,
          "juz": 30,
          "manzil": 7,
          "page": 585,
          "ruku": 522,
          "hizbQuarter": 234,
          "sajda": false
        }
      ],
