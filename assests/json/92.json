      "name": "سورة الليل",
      "englishName": "Al-Lail",
      "englishNameTranslation": "The Night",
      "revelationType": "Meccan",
      "numberOfAyahs": 21,
      "ayahs": [
        {
          "number": 6059,
          "text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ وَٱلَّيْلِ إِذَا يَغْشَىٰ",
          "numberInSurah": 1,
          "juz": 30,
          "manzil": 7,
          "page": 595,
          "ruku": 534,
          "hizbQuarter": 238,
          "sajda": false
        },
        {
          "number": 6060,
          "text": "وَٱلنَّهَارِ إِذَا تَجَلَّىٰ",
          "numberInSurah": 2,
          "juz": 30,
          "manzil": 7,
          "page": 595,
          "ruku": 534,
          "hizbQuarter": 238,
          "sajda": false
        },
        {
          "number": 6061,
          "text": "وَمَا خَلَقَ ٱلذَّكَرَ وَٱلْأُنثَىٰٓ",
          "numberInSurah": 3,
          "juz": 30,
          "manzil": 7,
          "page": 595,
          "ruku": 534,
          "hizbQuarter": 238,
          "sajda": false
        },
        {
          "number": 6062,
          "text": "إِنَّ سَعْيَكُمْ لَشَتَّىٰ",
          "numberInSurah": 4,
          "juz": 30,
          "manzil": 7,
          "page": 595,
          "ruku": 534,
          "hizbQuarter": 238,
          "sajda": false
        },
        {
          "number": 6063,
          "text": "فَأَمَّا مَنْ أَعْطَىٰ وَٱتَّقَىٰ",
          "numberInSurah": 5,
          "juz": 30,
          "manzil": 7,
          "page": 595,
          "ruku": 534,
          "hizbQuarter": 238,
          "sajda": false
        },
        {
          "number": 6064,
          "text": "وَصَدَّقَ بِٱلْحُسْنَىٰ",
          "numberInSurah": 6,
          "juz": 30,
          "manzil": 7,
          "page": 595,
          "ruku": 534,
          "hizbQuarter": 238,
          "sajda": false
        },
        {
          "number": 6065,
          "text": "فَسَنُيَسِّرُهُۥ لِلْيُسْرَىٰ",
          "numberInSurah": 7,
          "juz": 30,
          "manzil": 7,
          "page": 595,
          "ruku": 534,
          "hizbQuarter": 238,
          "sajda": false
        },
        {
          "number": 6066,
          "text": "وَأَمَّا مَنۢ بَخِلَ وَٱسْتَغْنَىٰ",
          "numberInSurah": 8,
          "juz": 30,
          "manzil": 7,
          "page": 595,
          "ruku": 534,
          "hizbQuarter": 238,
          "sajda": false
        },
        {
          "number": 6067,
          "text": "وَكَذَّبَ بِٱلْحُسْنَىٰ",
          "numberInSurah": 9,
          "juz": 30,
          "manzil": 7,
          "page": 595,
          "ruku": 534,
          "hizbQuarter": 238,
          "sajda": false
        },
        {
          "number": 6068,
          "text": "فَسَنُيَسِّرُهُۥ لِلْعُسْرَىٰ",
          "numberInSurah": 10,
          "juz": 30,
          "manzil": 7,
          "page": 595,
          "ruku": 534,
          "hizbQuarter": 238,
          "sajda": false
        },
        {
          "number": 6069,
          "text": "وَمَا يُغْنِى عَنْهُ مَالُهُۥٓ إِذَا تَرَدَّىٰٓ",
          "numberInSurah": 11,
          "juz": 30,
          "manzil": 7,
          "page": 595,
          "ruku": 534,
          "hizbQuarter": 238,
          "sajda": false
        },
        {
          "number": 6070,
          "text": "إِنَّ عَلَيْنَا لَلْهُدَىٰ",
          "numberInSurah": 12,
          "juz": 30,
          "manzil": 7,
          "page": 595,
          "ruku": 534,
          "hizbQuarter": 238,
          "sajda": false
        },
        {
          "number": 6071,
          "text": "وَإِنَّ لَنَا لَلْءَاخِرَةَ وَٱلْأُولَىٰ",
          "numberInSurah": 13,
          "juz": 30,
          "manzil": 7,
          "page": 595,
          "ruku": 534,
          "hizbQuarter": 238,
          "sajda": false
        },
        {
          "number": 6072,
          "text": "فَأَنذَرْتُكُمْ نَارًۭا تَلَظَّىٰ",
          "numberInSurah": 14,
          "juz": 30,
          "manzil": 7,
          "page": 595,
          "ruku": 534,
          "hizbQuarter": 238,
          "sajda": false
        },
        {
          "number": 6073,
          "text": "لَا يَصْلَىٰهَآ إِلَّا ٱلْأَشْقَى",
          "numberInSurah": 15,
          "juz": 30,
          "manzil": 7,
          "page": 596,
          "ruku": 534,
          "hizbQuarter": 238,
          "sajda": false
        },
        {
          "number": 6074,
          "text": "ٱلَّذِى كَذَّبَ وَتَوَلَّىٰ",
          "numberInSurah": 16,
          "juz": 30,
          "manzil": 7,
          "page": 596,
          "ruku": 534,
          "hizbQuarter": 238,
          "sajda": false
        },
        {
          "number": 6075,
          "text": "وَسَيُجَنَّبُهَا ٱلْأَتْقَى",
          "numberInSurah": 17,
          "juz": 30,
          "manzil": 7,
          "page": 596,
          "ruku": 534,
          "hizbQuarter": 238,
          "sajda": false
        },
        {
          "number": 6076,
          "text": "ٱلَّذِى يُؤْتِى مَالَهُۥ يَتَزَكَّىٰ",
          "numberInSurah": 18,
          "juz": 30,
          "manzil": 7,
          "page": 596,
          "ruku": 534,
          "hizbQuarter": 238,
          "sajda": false
        },
        {
          "number": 6077,
          "text": "وَمَا لِأَحَدٍ عِندَهُۥ مِن نِّعْمَةٍۢ تُجْزَىٰٓ",
          "numberInSurah": 19,
          "juz": 30,
          "manzil": 7,
          "page": 596,
          "ruku": 534,
          "hizbQuarter": 238,
          "sajda": false
        },
        {
          "number": 6078,
          "text": "إِلَّا ٱبْتِغَآءَ وَجْهِ رَبِّهِ ٱلْأَعْلَىٰ",
          "numberInSurah": 20,
          "juz": 30,
          "manzil": 7,
          "page": 596,
          "ruku": 534,
          "hizbQuarter": 238,
          "sajda": false
        },
        {
