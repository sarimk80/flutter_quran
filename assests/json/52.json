  "data": [
    {
      "number": 52,
      "name": "سورة الطور",
      "englishName": "At-Tur",
      "englishNameTranslation": "The Mount",
      "revelationType": "Meccan",
      "numberOfAyahs": 49,
      "ayahs": [
        {
          "number": 4736,
          "text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ وَٱلطُّورِ",
          "numberInSurah": 1,
          "juz": 27,
          "manzil": 7,
          "page": 523,
          "ruku": 458,
          "hizbQuarter": 209,
          "sajda": false
        },
        {
          "number": 4737,
          "text": "وَكِتَٰبٍۢ مَّسْطُورٍۢ",
          "numberInSurah": 2,
          "juz": 27,
          "manzil": 7,
          "page": 523,
          "ruku": 458,
          "hizbQuarter": 209,
          "sajda": false
        },
        {
          "number": 4738,
          "text": "فِى رَقٍّۢ مَّنشُورٍۢ",
          "numberInSurah": 3,
          "juz": 27,
          "manzil": 7,
          "page": 523,
          "ruku": 458,
          "hizbQuarter": 209,
          "sajda": false
        },
        {
          "number": 4739,
          "text": "وَٱلْبَيْتِ ٱلْمَعْمُورِ",
          "numberInSurah": 4,
          "juz": 27,
          "manzil": 7,
          "page": 523,
          "ruku": 458,
          "hizbQuarter": 209,
          "sajda": false
        },
        {
          "number": 4740,
          "text": "وَٱلسَّقْفِ ٱلْمَرْفُوعِ",
          "numberInSurah": 5,
          "juz": 27,
          "manzil": 7,
          "page": 523,
          "ruku": 458,
          "hizbQuarter": 209,
          "sajda": false
        },
        {
          "number": 4741,
          "text": "وَٱلْبَحْرِ ٱلْمَسْجُورِ",
          "numberInSurah": 6,
          "juz": 27,
          "manzil": 7,
          "page": 523,
          "ruku": 458,
          "hizbQuarter": 209,
          "sajda": false
        },
        {
          "number": 4742,
          "text": "إِنَّ عَذَابَ رَبِّكَ لَوَٰقِعٌۭ",
          "numberInSurah": 7,
          "juz": 27,
          "manzil": 7,
          "page": 523,
          "ruku": 458,
          "hizbQuarter": 209,
          "sajda": false
        },
        {
          "number": 4743,
          "text": "مَّا لَهُۥ مِن دَافِعٍۢ",
          "numberInSurah": 8,
          "juz": 27,
          "manzil": 7,
          "page": 523,
          "ruku": 458,
          "hizbQuarter": 209,
          "sajda": false
        },
        {
          "number": 4744,
          "text": "يَوْمَ تَمُورُ ٱلسَّمَآءُ مَوْرًۭا",
          "numberInSurah": 9,
          "juz": 27,
          "manzil": 7,
          "page": 523,
          "ruku": 458,
          "hizbQuarter": 209,
          "sajda": false
        },
        {
          "number": 4745,
          "text": "وَتَسِيرُ ٱلْجِبَالُ سَيْرًۭا",
          "numberInSurah": 10,
          "juz": 27,
          "manzil": 7,
          "page": 523,
          "ruku": 458,
          "hizbQuarter": 209,
          "sajda": false
        },
        {
          "number": 4746,
          "text": "فَوَيْلٌۭ يَوْمَئِذٍۢ لِّلْمُكَذِّبِينَ",
          "numberInSurah": 11,
          "juz": 27,
          "manzil": 7,
          "page": 523,
          "ruku": 458,
          "hizbQuarter": 209,
          "sajda": false
        },
        {
          "number": 4747,
          "text": "ٱلَّذِينَ هُمْ فِى خَوْضٍۢ يَلْعَبُونَ",
          "numberInSurah": 12,
          "juz": 27,
          "manzil": 7,
          "page": 523,
          "ruku": 458,
          "hizbQuarter": 209,
          "sajda": false
        },
        {
          "number": 4748,
          "text": "يَوْمَ يُدَعُّونَ إِلَىٰ نَارِ جَهَنَّمَ دَعًّا",
          "numberInSurah": 13,
          "juz": 27,
          "manzil": 7,
          "page": 523,
          "ruku": 458,
          "hizbQuarter": 209,
          "sajda": false
        },
        {
          "number": 4749,
          "text": "هَٰذِهِ ٱلنَّارُ ٱلَّتِى كُنتُم بِهَا تُكَذِّبُونَ",
          "numberInSurah": 14,
          "juz": 27,
          "manzil": 7,
          "page": 523,
          "ruku": 458,
          "hizbQuarter": 209,
          "sajda": false
        },
        {
          "number": 4750,
          "text": "أَفَسِحْرٌ هَٰذَآ أَمْ أَنتُمْ لَا تُبْصِرُونَ",
          "numberInSurah": 15,
          "juz": 27,
          "manzil": 7,
          "page": 524,
          "ruku": 458,
          "hizbQuarter": 209,
          "sajda": false
        },
        {
          "number": 4751,
          "text": "ٱصْلَوْهَا فَٱصْبِرُوٓا۟ أَوْ لَا تَصْبِرُوا۟ سَوَآءٌ عَلَيْكُمْ ۖ إِنَّمَا تُجْزَوْنَ مَا كُنتُمْ تَعْمَلُونَ",
          "numberInSurah": 16,
          "juz": 27,
          "manzil": 7,
          "page": 524,
          "ruku": 458,
          "hizbQuarter": 209,
          "sajda": false
        },
        {
          "number": 4752,
          "text": "إِنَّ ٱلْمُتَّقِينَ فِى جَنَّٰتٍۢ وَنَعِيمٍۢ",
          "numberInSurah": 17,
          "juz": 27,
          "manzil": 7,
          "page": 524,
          "ruku": 458,
          "hizbQuarter": 209,
          "sajda": false
        },
        {
          "number": 4753,
          "text": "فَٰكِهِينَ بِمَآ ءَاتَىٰهُمْ رَبُّهُمْ وَوَقَىٰهُمْ رَبُّهُمْ عَذَابَ ٱلْجَحِيمِ",
          "numberInSurah": 18,
          "juz": 27,
          "manzil": 7,
          "page": 524,
          "ruku": 458,
          "hizbQuarter": 209,
          "sajda": false
        },
        {
          "number": 4754,
          "text": "كُلُوا۟ وَٱشْرَبُوا۟ هَنِيٓـًٔۢا بِمَا كُنتُمْ تَعْمَلُونَ",
          "numberInSurah": 19,
          "juz": 27,
          "manzil": 7,
          "page": 524,
          "ruku": 458,
          "hizbQuarter": 209,
          "sajda": false
        },
        {
          "number": 4755,
          "text": "مُتَّكِـِٔينَ عَلَىٰ سُرُرٍۢ مَّصْفُوفَةٍۢ ۖ وَزَوَّجْنَٰهُم بِحُورٍ عِينٍۢ",
          "numberInSurah": 20,
          "juz": 27,
          "manzil": 7,
          "page": 524,
          "ruku": 458,
          "hizbQuarter": 209,
          "sajda": false
        },
        {
          "number": 4756,
          "text": "وَٱلَّذِينَ ءَامَنُوا۟ وَٱتَّبَعَتْهُمْ ذُرِّيَّتُهُم بِإِيمَٰنٍ أَلْحَقْنَا بِهِمْ ذُرِّيَّتَهُمْ وَمَآ أَلَتْنَٰهُم مِّنْ عَمَلِهِم مِّن شَىْءٍۢ ۚ كُلُّ ٱمْرِئٍۭ بِمَا كَسَبَ رَهِينٌۭ",
          "numberInSurah": 21,
          "juz": 27,
          "manzil": 7,
          "page": 524,
          "ruku": 458,
          "hizbQuarter": 209,
          "sajda": false
        },
        {
          "number": 4757,
          "text": "وَأَمْدَدْنَٰهُم بِفَٰكِهَةٍۢ وَلَحْمٍۢ مِّمَّا يَشْتَهُونَ",
          "numberInSurah": 22,
          "juz": 27,
          "manzil": 7,
          "page": 524,
          "ruku": 458,
          "hizbQuarter": 209,
          "sajda": false
        },
        {
          "number": 4758,
          "text": "يَتَنَٰزَعُونَ فِيهَا كَأْسًۭا لَّا لَغْوٌۭ فِيهَا وَلَا تَأْثِيمٌۭ",
          "numberInSurah": 23,
          "juz": 27,
          "manzil": 7,
          "page": 524,
          "ruku": 458,
          "hizbQuarter": 209,
          "sajda": false
        },
        {
          "number": 4759,
          "text": "۞ وَيَطُوفُ عَلَيْهِمْ غِلْمَانٌۭ لَّهُمْ كَأَنَّهُمْ لُؤْلُؤٌۭ مَّكْنُونٌۭ",
          "numberInSurah": 24,
          "juz": 27,
          "manzil": 7,
          "page": 524,
          "ruku": 458,
          "hizbQuarter": 210,
          "sajda": false
        },
        {
          "number": 4760,
          "text": "وَأَقْبَلَ بَعْضُهُمْ عَلَىٰ بَعْضٍۢ يَتَسَآءَلُونَ",
          "numberInSurah": 25,
          "juz": 27,
          "manzil": 7,
          "page": 524,
          "ruku": 458,
          "hizbQuarter": 210,
          "sajda": false
        },
        {
          "number": 4761,
          "text": "قَالُوٓا۟ إِنَّا كُنَّا قَبْلُ فِىٓ أَهْلِنَا مُشْفِقِينَ",
          "numberInSurah": 26,
          "juz": 27,
          "manzil": 7,
          "page": 524,
          "ruku": 458,
          "hizbQuarter": 210,
          "sajda": false
        },
        {
          "number": 4762,
          "text": "فَمَنَّ ٱللَّهُ عَلَيْنَا وَوَقَىٰنَا عَذَابَ ٱلسَّمُومِ",
          "numberInSurah": 27,
          "juz": 27,
          "manzil": 7,
          "page": 524,
          "ruku": 458,
          "hizbQuarter": 210,
          "sajda": false
        },
        {
          "number": 4763,
          "text": "إِنَّا كُنَّا مِن قَبْلُ نَدْعُوهُ ۖ إِنَّهُۥ هُوَ ٱلْبَرُّ ٱلرَّحِيمُ",
          "numberInSurah": 28,
          "juz": 27,
          "manzil": 7,
          "page": 524,
          "ruku": 458,
          "hizbQuarter": 210,
          "sajda": false
        },
        {
          "number": 4764,
          "text": "فَذَكِّرْ فَمَآ أَنتَ بِنِعْمَتِ رَبِّكَ بِكَاهِنٍۢ وَلَا مَجْنُونٍ",
          "numberInSurah": 29,
          "juz": 27,
          "manzil": 7,
          "page": 524,
          "ruku": 459,
          "hizbQuarter": 210,
          "sajda": false
        },
        {
          "number": 4765,
          "text": "أَمْ يَقُولُونَ شَاعِرٌۭ نَّتَرَبَّصُ بِهِۦ رَيْبَ ٱلْمَنُونِ",
          "numberInSurah": 30,
          "juz": 27,
          "manzil": 7,
          "page": 524,
          "ruku": 459,
          "hizbQuarter": 210,
          "sajda": false
        },
        {
          "number": 4766,
          "text": "قُلْ تَرَبَّصُوا۟ فَإِنِّى مَعَكُم مِّنَ ٱلْمُتَرَبِّصِينَ",
          "numberInSurah": 31,
          "juz": 27,
          "manzil": 7,
          "page": 524,
          "ruku": 459,
          "hizbQuarter": 210,
          "sajda": false
        },
        {
          "number": 4767,
          "text": "أَمْ تَأْمُرُهُمْ أَحْلَٰمُهُم بِهَٰذَآ ۚ أَمْ هُمْ قَوْمٌۭ طَاغُونَ",
          "numberInSurah": 32,
          "juz": 27,
          "manzil": 7,
          "page": 525,
          "ruku": 459,
          "hizbQuarter": 210,
          "sajda": false
        },
        {
          "number": 4768,
          "text": "أَمْ يَقُولُونَ تَقَوَّلَهُۥ ۚ بَل لَّا يُؤْمِنُونَ",
          "numberInSurah": 33,
          "juz": 27,
          "manzil": 7,
          "page": 525,
          "ruku": 459,
          "hizbQuarter": 210,
          "sajda": false
        },
        {
          "number": 4769,
          "text": "فَلْيَأْتُوا۟ بِحَدِيثٍۢ مِّثْلِهِۦٓ إِن كَانُوا۟ صَٰدِقِينَ",
          "numberInSurah": 34,
          "juz": 27,
          "manzil": 7,
          "page": 525,
          "ruku": 459,
          "hizbQuarter": 210,
          "sajda": false
        },
        {
          "number": 4770,
          "text": "أَمْ خُلِقُوا۟ مِنْ غَيْرِ شَىْءٍ أَمْ هُمُ ٱلْخَٰلِقُونَ",
          "numberInSurah": 35,
          "juz": 27,
          "manzil": 7,
          "page": 525,
          "ruku": 459,
          "hizbQuarter": 210,
          "sajda": false
        },
        {
          "number": 4771,
          "text": "أَمْ خَلَقُوا۟ ٱلسَّمَٰوَٰتِ وَٱلْأَرْضَ ۚ بَل لَّا يُوقِنُونَ",
          "numberInSurah": 36,
          "juz": 27,
          "manzil": 7,
          "page": 525,
          "ruku": 459,
          "hizbQuarter": 210,
          "sajda": false
        },
        {
          "number": 4772,
          "text": "أَمْ عِندَهُمْ خَزَآئِنُ رَبِّكَ أَمْ هُمُ ٱلْمُصَۣيْطِرُونَ",
          "numberInSurah": 37,
          "juz": 27,
          "manzil": 7,
          "page": 525,
          "ruku": 459,
          "hizbQuarter": 210,
          "sajda": false
        },
        {
          "number": 4773,
          "text": "أَمْ لَهُمْ سُلَّمٌۭ يَسْتَمِعُونَ فِيهِ ۖ فَلْيَأْتِ مُسْتَمِعُهُم بِسُلْطَٰنٍۢ مُّبِينٍ",
          "numberInSurah": 38,
          "juz": 27,
          "manzil": 7,
          "page": 525,
          "ruku": 459,
          "hizbQuarter": 210,
          "sajda": false
        },
        {
          "number": 4774,
          "text": "أَمْ لَهُ ٱلْبَنَٰتُ وَلَكُمُ ٱلْبَنُونَ",
          "numberInSurah": 39,
          "juz": 27,
          "manzil": 7,
          "page": 525,
          "ruku": 459,
          "hizbQuarter": 210,
          "sajda": false
        },
        {
          "number": 4775,
          "text": "أَمْ تَسْـَٔلُهُمْ أَجْرًۭا فَهُم مِّن مَّغْرَمٍۢ مُّثْقَلُونَ",
          "numberInSurah": 40,
          "juz": 27,
          "manzil": 7,
          "page": 525,
          "ruku": 459,
          "hizbQuarter": 210,
          "sajda": false
        },
        {
          "number": 4776,
          "text": "أَمْ عِندَهُمُ ٱلْغَيْبُ فَهُمْ يَكْتُبُونَ",
          "numberInSurah": 41,
          "juz": 27,
          "manzil": 7,
          "page": 525,
          "ruku": 459,
          "hizbQuarter": 210,
          "sajda": false
        },
        {
          "number": 4777,
          "text": "أَمْ يُرِيدُونَ كَيْدًۭا ۖ فَٱلَّذِينَ كَفَرُوا۟ هُمُ ٱلْمَكِيدُونَ",
          "numberInSurah": 42,
          "juz": 27,
          "manzil": 7,
          "page": 525,
          "ruku": 459,
          "hizbQuarter": 210,
          "sajda": false
        },
        {
          "number": 4778,
          "text": "أَمْ لَهُمْ إِلَٰهٌ غَيْرُ ٱللَّهِ ۚ سُبْحَٰنَ ٱللَّهِ عَمَّا يُشْرِكُونَ",
          "numberInSurah": 43,
          "juz": 27,
          "manzil": 7,
          "page": 525,
          "ruku": 459,
          "hizbQuarter": 210,
          "sajda": false
        },
        {
          "number": 4779,
          "text": "وَإِن يَرَوْا۟ كِسْفًۭا مِّنَ ٱلسَّمَآءِ سَاقِطًۭا يَقُولُوا۟ سَحَابٌۭ مَّرْكُومٌۭ",
          "numberInSurah": 44,
          "juz": 27,
          "manzil": 7,
          "page": 525,
          "ruku": 459,
          "hizbQuarter": 210,
          "sajda": false
        },
        {
          "number": 4780,
          "text": "فَذَرْهُمْ حَتَّىٰ يُلَٰقُوا۟ يَوْمَهُمُ ٱلَّذِى فِيهِ يُصْعَقُونَ",
          "numberInSurah": 45,
          "juz": 27,
          "manzil": 7,
          "page": 525,
          "ruku": 459,
          "hizbQuarter": 210,
          "sajda": false
        },
        {
          "number": 4781,
          "text": "يَوْمَ لَا يُغْنِى عَنْهُمْ كَيْدُهُمْ شَيْـًۭٔا وَلَا هُمْ يُنصَرُونَ",
          "numberInSurah": 46,
          "juz": 27,
          "manzil": 7,
          "page": 525,
          "ruku": 459,
          "hizbQuarter": 210,
          "sajda": false
        },
        {
          "number": 4782,
          "text": "وَإِنَّ لِلَّذِينَ ظَلَمُوا۟ عَذَابًۭا دُونَ ذَٰلِكَ وَلَٰكِنَّ أَكْثَرَهُمْ لَا يَعْلَمُونَ",
          "numberInSurah": 47,
          "juz": 27,
          "manzil": 7,
          "page": 525,
          "ruku": 459,
          "hizbQuarter": 210,
          "sajda": false
        },
        {
          "number": 4783,
          "text": "وَٱصْبِرْ لِحُكْمِ رَبِّكَ فَإِنَّكَ بِأَعْيُنِنَا ۖ وَسَبِّحْ بِحَمْدِ رَبِّكَ حِينَ تَقُومُ",
          "numberInSurah": 48,
          "juz": 27,
          "manzil": 7,
          "page": 525,
          "ruku": 459,
          "hizbQuarter": 210,
          "sajda": false
        },
        {
          "number": 4784,
          "text": "وَمِنَ ٱلَّيْلِ فَسَبِّحْهُ وَإِدْبَٰرَ ٱلنُّجُومِ",
          "numberInSurah": 49,
          "juz": 27,
          "manzil": 7,
          "page": 525,
          "ruku": 459,
          "hizbQuarter": 210,
          "sajda": false
        }
      ],
