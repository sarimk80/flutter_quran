  "data": [
    {
      "number": 83,
      "name": "سورة المطففين",
      "englishName": "Al-Mutaffifin",
      "englishNameTranslation": "Defrauding",
      "revelationType": "Meccan",
      "numberOfAyahs": 36,
      "ayahs": [
        {
          "number": 5849,
          "text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ وَيْلٌۭ لِّلْمُطَفِّفِينَ",
          "numberInSurah": 1,
          "juz": 30,
          "manzil": 7,
          "page": 587,
          "ruku": 525,
          "hizbQuarter": 235,
          "sajda": false
        },
        {
          "number": 5850,
          "text": "ٱلَّذِينَ إِذَا ٱكْتَالُوا۟ عَلَى ٱلنَّاسِ يَسْتَوْفُونَ",
          "numberInSurah": 2,
          "juz": 30,
          "manzil": 7,
          "page": 587,
          "ruku": 525,
          "hizbQuarter": 235,
          "sajda": false
        },
        {
          "number": 5851,
          "text": "وَإِذَا كَالُوهُمْ أَو وَّزَنُوهُمْ يُخْسِرُونَ",
          "numberInSurah": 3,
          "juz": 30,
          "manzil": 7,
          "page": 587,
          "ruku": 525,
          "hizbQuarter": 235,
          "sajda": false
        },
        {
          "number": 5852,
          "text": "أَلَا يَظُنُّ أُو۟لَٰٓئِكَ أَنَّهُم مَّبْعُوثُونَ",
          "numberInSurah": 4,
          "juz": 30,
          "manzil": 7,
          "page": 587,
          "ruku": 525,
          "hizbQuarter": 235,
          "sajda": false
        },
        {
          "number": 5853,
          "text": "لِيَوْمٍ عَظِيمٍۢ",
          "numberInSurah": 5,
          "juz": 30,
          "manzil": 7,
          "page": 587,
          "ruku": 525,
          "hizbQuarter": 235,
          "sajda": false
        },
        {
          "number": 5854,
          "text": "يَوْمَ يَقُومُ ٱلنَّاسُ لِرَبِّ ٱلْعَٰلَمِينَ",
          "numberInSurah": 6,
          "juz": 30,
          "manzil": 7,
          "page": 587,
          "ruku": 525,
          "hizbQuarter": 235,
          "sajda": false
        },
        {
          "number": 5855,
          "text": "كَلَّآ إِنَّ كِتَٰبَ ٱلْفُجَّارِ لَفِى سِجِّينٍۢ",
          "numberInSurah": 7,
          "juz": 30,
          "manzil": 7,
          "page": 588,
          "ruku": 525,
          "hizbQuarter": 235,
          "sajda": false
        },
        {
          "number": 5856,
          "text": "وَمَآ أَدْرَىٰكَ مَا سِجِّينٌۭ",
          "numberInSurah": 8,
          "juz": 30,
          "manzil": 7,
          "page": 588,
          "ruku": 525,
          "hizbQuarter": 235,
          "sajda": false
        },
        {
          "number": 5857,
          "text": "كِتَٰبٌۭ مَّرْقُومٌۭ",
          "numberInSurah": 9,
          "juz": 30,
          "manzil": 7,
          "page": 588,
          "ruku": 525,
          "hizbQuarter": 235,
          "sajda": false
        },
        {
          "number": 5858,
          "text": "وَيْلٌۭ يَوْمَئِذٍۢ لِّلْمُكَذِّبِينَ",
          "numberInSurah": 10,
          "juz": 30,
          "manzil": 7,
          "page": 588,
          "ruku": 525,
          "hizbQuarter": 235,
          "sajda": false
        },
        {
          "number": 5859,
          "text": "ٱلَّذِينَ يُكَذِّبُونَ بِيَوْمِ ٱلدِّينِ",
          "numberInSurah": 11,
          "juz": 30,
          "manzil": 7,
          "page": 588,
          "ruku": 525,
          "hizbQuarter": 235,
          "sajda": false
        },
        {
          "number": 5860,
          "text": "وَمَا يُكَذِّبُ بِهِۦٓ إِلَّا كُلُّ مُعْتَدٍ أَثِيمٍ",
          "numberInSurah": 12,
          "juz": 30,
          "manzil": 7,
          "page": 588,
          "ruku": 525,
          "hizbQuarter": 235,
          "sajda": false
        },
        {
          "number": 5861,
          "text": "إِذَا تُتْلَىٰ عَلَيْهِ ءَايَٰتُنَا قَالَ أَسَٰطِيرُ ٱلْأَوَّلِينَ",
          "numberInSurah": 13,
          "juz": 30,
          "manzil": 7,
          "page": 588,
          "ruku": 525,
          "hizbQuarter": 235,
          "sajda": false
        },
        {
          "number": 5862,
          "text": "كَلَّا ۖ بَلْ ۜ رَانَ عَلَىٰ قُلُوبِهِم مَّا كَانُوا۟ يَكْسِبُونَ",
          "numberInSurah": 14,
          "juz": 30,
          "manzil": 7,
          "page": 588,
          "ruku": 525,
          "hizbQuarter": 235,
          "sajda": false
        },
        {
          "number": 5863,
          "text": "كَلَّآ إِنَّهُمْ عَن رَّبِّهِمْ يَوْمَئِذٍۢ لَّمَحْجُوبُونَ",
          "numberInSurah": 15,
          "juz": 30,
          "manzil": 7,
          "page": 588,
          "ruku": 525,
          "hizbQuarter": 235,
          "sajda": false
        },
        {
          "number": 5864,
          "text": "ثُمَّ إِنَّهُمْ لَصَالُوا۟ ٱلْجَحِيمِ",
          "numberInSurah": 16,
          "juz": 30,
          "manzil": 7,
          "page": 588,
          "ruku": 525,
          "hizbQuarter": 235,
          "sajda": false
        },
        {
          "number": 5865,
          "text": "ثُمَّ يُقَالُ هَٰذَا ٱلَّذِى كُنتُم بِهِۦ تُكَذِّبُونَ",
          "numberInSurah": 17,
          "juz": 30,
          "manzil": 7,
          "page": 588,
          "ruku": 525,
          "hizbQuarter": 235,
          "sajda": false
        },
        {
          "number": 5866,
          "text": "كَلَّآ إِنَّ كِتَٰبَ ٱلْأَبْرَارِ لَفِى عِلِّيِّينَ",
          "numberInSurah": 18,
          "juz": 30,
          "manzil": 7,
          "page": 588,
          "ruku": 525,
          "hizbQuarter": 235,
          "sajda": false
        },
        {
          "number": 5867,
          "text": "وَمَآ أَدْرَىٰكَ مَا عِلِّيُّونَ",
          "numberInSurah": 19,
          "juz": 30,
          "manzil": 7,
          "page": 588,
          "ruku": 525,
          "hizbQuarter": 235,
          "sajda": false
        },
        {
          "number": 5868,
          "text": "كِتَٰبٌۭ مَّرْقُومٌۭ",
          "numberInSurah": 20,
          "juz": 30,
          "manzil": 7,
          "page": 588,
          "ruku": 525,
          "hizbQuarter": 235,
          "sajda": false
        },
        {
          "number": 5869,
          "text": "يَشْهَدُهُ ٱلْمُقَرَّبُونَ",
          "numberInSurah": 21,
          "juz": 30,
          "manzil": 7,
          "page": 588,
          "ruku": 525,
          "hizbQuarter": 235,
          "sajda": false
        },
        {
          "number": 5870,
          "text": "إِنَّ ٱلْأَبْرَارَ لَفِى نَعِيمٍ",
          "numberInSurah": 22,
          "juz": 30,
          "manzil": 7,
          "page": 588,
          "ruku": 525,
          "hizbQuarter": 235,
          "sajda": false
        },
        {
          "number": 5871,
          "text": "عَلَى ٱلْأَرَآئِكِ يَنظُرُونَ",
          "numberInSurah": 23,
          "juz": 30,
          "manzil": 7,
          "page": 588,
          "ruku": 525,
          "hizbQuarter": 235,
          "sajda": false
        },
        {
          "number": 5872,
          "text": "تَعْرِفُ فِى وُجُوهِهِمْ نَضْرَةَ ٱلنَّعِيمِ",
          "numberInSurah": 24,
          "juz": 30,
          "manzil": 7,
          "page": 588,
          "ruku": 525,
          "hizbQuarter": 235,
          "sajda": false
        },
        {
          "number": 5873,
          "text": "يُسْقَوْنَ مِن رَّحِيقٍۢ مَّخْتُومٍ",
          "numberInSurah": 25,
          "juz": 30,
          "manzil": 7,
          "page": 588,
          "ruku": 525,
          "hizbQuarter": 235,
          "sajda": false
        },
        {
          "number": 5874,
          "text": "خِتَٰمُهُۥ مِسْكٌۭ ۚ وَفِى ذَٰلِكَ فَلْيَتَنَافَسِ ٱلْمُتَنَٰفِسُونَ",
          "numberInSurah": 26,
          "juz": 30,
          "manzil": 7,
          "page": 588,
          "ruku": 525,
          "hizbQuarter": 235,
          "sajda": false
        },
        {
          "number": 5875,
          "text": "وَمِزَاجُهُۥ مِن تَسْنِيمٍ",
          "numberInSurah": 27,
          "juz": 30,
          "manzil": 7,
          "page": 588,
          "ruku": 525,
          "hizbQuarter": 235,
          "sajda": false
        },
        {
          "number": 5876,
          "text": "عَيْنًۭا يَشْرَبُ بِهَا ٱلْمُقَرَّبُونَ",
          "numberInSurah": 28,
          "juz": 30,
          "manzil": 7,
          "page": 588,
          "ruku": 525,
          "hizbQuarter": 235,
          "sajda": false
        },
        {
          "number": 5877,
          "text": "إِنَّ ٱلَّذِينَ أَجْرَمُوا۟ كَانُوا۟ مِنَ ٱلَّذِينَ ءَامَنُوا۟ يَضْحَكُونَ",
          "numberInSurah": 29,
          "juz": 30,
          "manzil": 7,
          "page": 588,
          "ruku": 525,
          "hizbQuarter": 235,
          "sajda": false
        },
        {
          "number": 5878,
          "text": "وَإِذَا مَرُّوا۟ بِهِمْ يَتَغَامَزُونَ",
          "numberInSurah": 30,
          "juz": 30,
          "manzil": 7,
          "page": 588,
          "ruku": 525,
          "hizbQuarter": 235,
          "sajda": false
        },
        {
          "number": 5879,
          "text": "وَإِذَا ٱنقَلَبُوٓا۟ إِلَىٰٓ أَهْلِهِمُ ٱنقَلَبُوا۟ فَكِهِينَ",
          "numberInSurah": 31,
          "juz": 30,
          "manzil": 7,
          "page": 588,
          "ruku": 525,
          "hizbQuarter": 235,
          "sajda": false
        },
        {
          "number": 5880,
          "text": "وَإِذَا رَأَوْهُمْ قَالُوٓا۟ إِنَّ هَٰٓؤُلَآءِ لَضَآلُّونَ",
          "numberInSurah": 32,
          "juz": 30,
          "manzil": 7,
          "page": 588,
          "ruku": 525,
          "hizbQuarter": 235,
          "sajda": false
        },
        {
          "number": 5881,
          "text": "وَمَآ أُرْسِلُوا۟ عَلَيْهِمْ حَٰفِظِينَ",
          "numberInSurah": 33,
          "juz": 30,
          "manzil": 7,
          "page": 588,
          "ruku": 525,
          "hizbQuarter": 235,
          "sajda": false
        },
        {
          "number": 5882,
          "text": "فَٱلْيَوْمَ ٱلَّذِينَ ءَامَنُوا۟ مِنَ ٱلْكُفَّارِ يَضْحَكُونَ",
          "numberInSurah": 34,
          "juz": 30,
          "manzil": 7,
          "page": 588,
          "ruku": 525,
          "hizbQuarter": 235,
          "sajda": false
        },
        {
          "number": 5883,
          "text": "عَلَى ٱلْأَرَآئِكِ يَنظُرُونَ",
          "numberInSurah": 35,
          "juz": 30,
          "manzil": 7,
          "page": 589,
          "ruku": 525,
          "hizbQuarter": 235,
          "sajda": false
        },
        {
          "number": 5884,
          "text": "هَلْ ثُوِّبَ ٱلْكُفَّارُ مَا كَانُوا۟ يَفْعَلُونَ",
          "numberInSurah": 36,
          "juz": 30,
          "manzil": 7,
          "page": 589,
          "ruku": 525,
          "hizbQuarter": 235,
          "sajda": false
        }
      ],
