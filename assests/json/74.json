  "data": [
    {
      "number": 74,
      "name": "سورة المدثر",
      "englishName": "Al-Muddaththir",
      "englishNameTranslation": "The Cloaked One",
      "revelationType": "Meccan",
      "numberOfAyahs": 56,
      "ayahs": [
        {
          "number": 5496,
          "text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ يَٰٓأَيُّهَا ٱلْمُدَّثِّرُ",
          "numberInSurah": 1,
          "juz": 29,
          "manzil": 7,
          "page": 575,
          "ruku": 510,
          "hizbQuarter": 230,
          "sajda": false
        },
        {
          "number": 5497,
          "text": "قُمْ فَأَنذِرْ",
          "numberInSurah": 2,
          "juz": 29,
          "manzil": 7,
          "page": 575,
          "ruku": 510,
          "hizbQuarter": 230,
          "sajda": false
        },
        {
          "number": 5498,
          "text": "وَرَبَّكَ فَكَبِّرْ",
          "numberInSurah": 3,
          "juz": 29,
          "manzil": 7,
          "page": 575,
          "ruku": 510,
          "hizbQuarter": 230,
          "sajda": false
        },
        {
          "number": 5499,
          "text": "وَثِيَابَكَ فَطَهِّرْ",
          "numberInSurah": 4,
          "juz": 29,
          "manzil": 7,
          "page": 575,
          "ruku": 510,
          "hizbQuarter": 230,
          "sajda": false
        },
        {
          "number": 5500,
          "text": "وَٱلرُّجْزَ فَٱهْجُرْ",
          "numberInSurah": 5,
          "juz": 29,
          "manzil": 7,
          "page": 575,
          "ruku": 510,
          "hizbQuarter": 230,
          "sajda": false
        },
        {
          "number": 5501,
          "text": "وَلَا تَمْنُن تَسْتَكْثِرُ",
          "numberInSurah": 6,
          "juz": 29,
          "manzil": 7,
          "page": 575,
          "ruku": 510,
          "hizbQuarter": 230,
          "sajda": false
        },
        {
          "number": 5502,
          "text": "وَلِرَبِّكَ فَٱصْبِرْ",
          "numberInSurah": 7,
          "juz": 29,
          "manzil": 7,
          "page": 575,
          "ruku": 510,
          "hizbQuarter": 230,
          "sajda": false
        },
        {
          "number": 5503,
          "text": "فَإِذَا نُقِرَ فِى ٱلنَّاقُورِ",
          "numberInSurah": 8,
          "juz": 29,
          "manzil": 7,
          "page": 575,
          "ruku": 510,
          "hizbQuarter": 230,
          "sajda": false
        },
        {
          "number": 5504,
          "text": "فَذَٰلِكَ يَوْمَئِذٍۢ يَوْمٌ عَسِيرٌ",
          "numberInSurah": 9,
          "juz": 29,
          "manzil": 7,
          "page": 575,
          "ruku": 510,
          "hizbQuarter": 230,
          "sajda": false
        },
        {
          "number": 5505,
          "text": "عَلَى ٱلْكَٰفِرِينَ غَيْرُ يَسِيرٍۢ",
          "numberInSurah": 10,
          "juz": 29,
          "manzil": 7,
          "page": 575,
          "ruku": 510,
          "hizbQuarter": 230,
          "sajda": false
        },
        {
          "number": 5506,
          "text": "ذَرْنِى وَمَنْ خَلَقْتُ وَحِيدًۭا",
          "numberInSurah": 11,
          "juz": 29,
          "manzil": 7,
          "page": 575,
          "ruku": 510,
          "hizbQuarter": 230,
          "sajda": false
        },
        {
          "number": 5507,
          "text": "وَجَعَلْتُ لَهُۥ مَالًۭا مَّمْدُودًۭا",
          "numberInSurah": 12,
          "juz": 29,
          "manzil": 7,
          "page": 575,
          "ruku": 510,
          "hizbQuarter": 230,
          "sajda": false
        },
        {
          "number": 5508,
          "text": "وَبَنِينَ شُهُودًۭا",
          "numberInSurah": 13,
          "juz": 29,
          "manzil": 7,
          "page": 575,
          "ruku": 510,
          "hizbQuarter": 230,
          "sajda": false
        },
        {
          "number": 5509,
          "text": "وَمَهَّدتُّ لَهُۥ تَمْهِيدًۭا",
          "numberInSurah": 14,
          "juz": 29,
          "manzil": 7,
          "page": 575,
          "ruku": 510,
          "hizbQuarter": 230,
          "sajda": false
        },
        {
          "number": 5510,
          "text": "ثُمَّ يَطْمَعُ أَنْ أَزِيدَ",
          "numberInSurah": 15,
          "juz": 29,
          "manzil": 7,
          "page": 575,
          "ruku": 510,
          "hizbQuarter": 230,
          "sajda": false
        },
        {
          "number": 5511,
          "text": "كَلَّآ ۖ إِنَّهُۥ كَانَ لِءَايَٰتِنَا عَنِيدًۭا",
          "numberInSurah": 16,
          "juz": 29,
          "manzil": 7,
          "page": 575,
          "ruku": 510,
          "hizbQuarter": 230,
          "sajda": false
        },
        {
          "number": 5512,
          "text": "سَأُرْهِقُهُۥ صَعُودًا",
          "numberInSurah": 17,
          "juz": 29,
          "manzil": 7,
          "page": 575,
          "ruku": 510,
          "hizbQuarter": 230,
          "sajda": false
        },
        {
          "number": 5513,
          "text": "إِنَّهُۥ فَكَّرَ وَقَدَّرَ",
          "numberInSurah": 18,
          "juz": 29,
          "manzil": 7,
          "page": 576,
          "ruku": 510,
          "hizbQuarter": 230,
          "sajda": false
        },
        {
          "number": 5514,
          "text": "فَقُتِلَ كَيْفَ قَدَّرَ",
          "numberInSurah": 19,
          "juz": 29,
          "manzil": 7,
          "page": 576,
          "ruku": 510,
          "hizbQuarter": 230,
          "sajda": false
        },
        {
          "number": 5515,
          "text": "ثُمَّ قُتِلَ كَيْفَ قَدَّرَ",
          "numberInSurah": 20,
          "juz": 29,
          "manzil": 7,
          "page": 576,
          "ruku": 510,
          "hizbQuarter": 230,
          "sajda": false
        },
        {
          "number": 5516,
          "text": "ثُمَّ نَظَرَ",
          "numberInSurah": 21,
          "juz": 29,
          "manzil": 7,
          "page": 576,
          "ruku": 510,
          "hizbQuarter": 230,
          "sajda": false
        },
        {
          "number": 5517,
          "text": "ثُمَّ عَبَسَ وَبَسَرَ",
          "numberInSurah": 22,
          "juz": 29,
          "manzil": 7,
          "page": 576,
          "ruku": 510,
          "hizbQuarter": 230,
          "sajda": false
        },
        {
          "number": 5518,
          "text": "ثُمَّ أَدْبَرَ وَٱسْتَكْبَرَ",
          "numberInSurah": 23,
          "juz": 29,
          "manzil": 7,
          "page": 576,
          "ruku": 510,
          "hizbQuarter": 230,
          "sajda": false
        },
        {
          "number": 5519,
          "text": "فَقَالَ إِنْ هَٰذَآ إِلَّا سِحْرٌۭ يُؤْثَرُ",
          "numberInSurah": 24,
          "juz": 29,
          "manzil": 7,
          "page": 576,
          "ruku": 510,
          "hizbQuarter": 230,
          "sajda": false
        },
        {
          "number": 5520,
          "text": "إِنْ هَٰذَآ إِلَّا قَوْلُ ٱلْبَشَرِ",
          "numberInSurah": 25,
          "juz": 29,
          "manzil": 7,
          "page": 576,
          "ruku": 510,
          "hizbQuarter": 230,
          "sajda": false
        },
        {
          "number": 5521,
          "text": "سَأُصْلِيهِ سَقَرَ",
          "numberInSurah": 26,
          "juz": 29,
          "manzil": 7,
          "page": 576,
          "ruku": 510,
          "hizbQuarter": 230,
          "sajda": false
        },
        {
          "number": 5522,
          "text": "وَمَآ أَدْرَىٰكَ مَا سَقَرُ",
          "numberInSurah": 27,
          "juz": 29,
          "manzil": 7,
          "page": 576,
          "ruku": 510,
          "hizbQuarter": 230,
          "sajda": false
        },
        {
          "number": 5523,
          "text": "لَا تُبْقِى وَلَا تَذَرُ",
          "numberInSurah": 28,
          "juz": 29,
          "manzil": 7,
          "page": 576,
          "ruku": 510,
          "hizbQuarter": 230,
          "sajda": false
        },
        {
          "number": 5524,
          "text": "لَوَّاحَةٌۭ لِّلْبَشَرِ",
          "numberInSurah": 29,
          "juz": 29,
          "manzil": 7,
          "page": 576,
          "ruku": 510,
          "hizbQuarter": 230,
          "sajda": false
        },
        {
          "number": 5525,
          "text": "عَلَيْهَا تِسْعَةَ عَشَرَ",
          "numberInSurah": 30,
          "juz": 29,
          "manzil": 7,
          "page": 576,
          "ruku": 510,
          "hizbQuarter": 230,
          "sajda": false
        },
        {
          "number": 5526,
          "text": "وَمَا جَعَلْنَآ أَصْحَٰبَ ٱلنَّارِ إِلَّا مَلَٰٓئِكَةًۭ ۙ وَمَا جَعَلْنَا عِدَّتَهُمْ إِلَّا فِتْنَةًۭ لِّلَّذِينَ كَفَرُوا۟ لِيَسْتَيْقِنَ ٱلَّذِينَ أُوتُوا۟ ٱلْكِتَٰبَ وَيَزْدَادَ ٱلَّذِينَ ءَامَنُوٓا۟ إِيمَٰنًۭا ۙ وَلَا يَرْتَابَ ٱلَّذِينَ أُوتُوا۟ ٱلْكِتَٰبَ وَٱلْمُؤْمِنُونَ ۙ وَلِيَقُولَ ٱلَّذِينَ فِى قُلُوبِهِم مَّرَضٌۭ وَٱلْكَٰفِرُونَ مَاذَآ أَرَادَ ٱللَّهُ بِهَٰذَا مَثَلًۭا ۚ كَذَٰلِكَ يُضِلُّ ٱللَّهُ مَن يَشَآءُ وَيَهْدِى مَن يَشَآءُ ۚ وَمَا يَعْلَمُ جُنُودَ رَبِّكَ إِلَّا هُوَ ۚ وَمَا هِىَ إِلَّا ذِكْرَىٰ لِلْبَشَرِ",
          "numberInSurah": 31,
          "juz": 29,
          "manzil": 7,
          "page": 576,
          "ruku": 510,
          "hizbQuarter": 230,
          "sajda": false
        },
        {
          "number": 5527,
          "text": "كَلَّا وَٱلْقَمَرِ",
          "numberInSurah": 32,
          "juz": 29,
          "manzil": 7,
          "page": 576,
          "ruku": 511,
          "hizbQuarter": 230,
          "sajda": false
        },
        {
          "number": 5528,
          "text": "وَٱلَّيْلِ إِذْ أَدْبَرَ",
          "numberInSurah": 33,
          "juz": 29,
          "manzil": 7,
          "page": 576,
          "ruku": 511,
          "hizbQuarter": 230,
          "sajda": false
        },
        {
          "number": 5529,
          "text": "وَٱلصُّبْحِ إِذَآ أَسْفَرَ",
          "numberInSurah": 34,
          "juz": 29,
          "manzil": 7,
          "page": 576,
          "ruku": 511,
          "hizbQuarter": 230,
          "sajda": false
        },
        {
          "number": 5530,
          "text": "إِنَّهَا لَإِحْدَى ٱلْكُبَرِ",
          "numberInSurah": 35,
          "juz": 29,
          "manzil": 7,
          "page": 576,
          "ruku": 511,
          "hizbQuarter": 230,
          "sajda": false
        },
        {
          "number": 5531,
          "text": "نَذِيرًۭا لِّلْبَشَرِ",
          "numberInSurah": 36,
          "juz": 29,
          "manzil": 7,
          "page": 576,
          "ruku": 511,
          "hizbQuarter": 230,
          "sajda": false
        },
        {
          "number": 5532,
          "text": "لِمَن شَآءَ مِنكُمْ أَن يَتَقَدَّمَ أَوْ يَتَأَخَّرَ",
          "numberInSurah": 37,
          "juz": 29,
          "manzil": 7,
          "page": 576,
          "ruku": 511,
          "hizbQuarter": 230,
          "sajda": false
        },
        {
          "number": 5533,
          "text": "كُلُّ نَفْسٍۭ بِمَا كَسَبَتْ رَهِينَةٌ",
          "numberInSurah": 38,
          "juz": 29,
          "manzil": 7,
          "page": 576,
          "ruku": 511,
          "hizbQuarter": 230,
          "sajda": false
        },
        {
          "number": 5534,
          "text": "إِلَّآ أَصْحَٰبَ ٱلْيَمِينِ",
          "numberInSurah": 39,
          "juz": 29,
          "manzil": 7,
          "page": 576,
          "ruku": 511,
          "hizbQuarter": 230,
          "sajda": false
        },
        {
          "number": 5535,
          "text": "فِى جَنَّٰتٍۢ يَتَسَآءَلُونَ",
          "numberInSurah": 40,
          "juz": 29,
          "manzil": 7,
          "page": 576,
          "ruku": 511,
          "hizbQuarter": 230,
          "sajda": false
        },
        {
          "number": 5536,
          "text": "عَنِ ٱلْمُجْرِمِينَ",
          "numberInSurah": 41,
          "juz": 29,
          "manzil": 7,
          "page": 576,
          "ruku": 511,
          "hizbQuarter": 230,
          "sajda": false
        },
        {
          "number": 5537,
          "text": "مَا سَلَكَكُمْ فِى سَقَرَ",
          "numberInSurah": 42,
          "juz": 29,
          "manzil": 7,
          "page": 576,
          "ruku": 511,
          "hizbQuarter": 230,
          "sajda": false
        },
        {
          "number": 5538,
          "text": "قَالُوا۟ لَمْ نَكُ مِنَ ٱلْمُصَلِّينَ",
          "numberInSurah": 43,
          "juz": 29,
          "manzil": 7,
          "page": 576,
          "ruku": 511,
          "hizbQuarter": 230,
          "sajda": false
        },
        {
          "number": 5539,
          "text": "وَلَمْ نَكُ نُطْعِمُ ٱلْمِسْكِينَ",
          "numberInSurah": 44,
          "juz": 29,
          "manzil": 7,
          "page": 576,
          "ruku": 511,
          "hizbQuarter": 230,
          "sajda": false
        },
        {
          "number": 5540,
          "text": "وَكُنَّا نَخُوضُ مَعَ ٱلْخَآئِضِينَ",
          "numberInSurah": 45,
          "juz": 29,
          "manzil": 7,
          "page": 576,
          "ruku": 511,
          "hizbQuarter": 230,
          "sajda": false
        },
        {
          "number": 5541,
          "text": "وَكُنَّا نُكَذِّبُ بِيَوْمِ ٱلدِّينِ",
          "numberInSurah": 46,
          "juz": 29,
          "manzil": 7,
          "page": 576,
          "ruku": 511,
          "hizbQuarter": 230,
          "sajda": false
        },
        {
          "number": 5542,
          "text": "حَتَّىٰٓ أَتَىٰنَا ٱلْيَقِينُ",
          "numberInSurah": 47,
          "juz": 29,
          "manzil": 7,
          "page": 576,
          "ruku": 511,
          "hizbQuarter": 230,
          "sajda": false
        },
        {
          "number": 5543,
          "text": "فَمَا تَنفَعُهُمْ شَفَٰعَةُ ٱلشَّٰفِعِينَ",
          "numberInSurah": 48,
          "juz": 29,
          "manzil": 7,
          "page": 577,
          "ruku": 511,
          "hizbQuarter": 230,
          "sajda": false
        },
        {
          "number": 5544,
          "text": "فَمَا لَهُمْ عَنِ ٱلتَّذْكِرَةِ مُعْرِضِينَ",
          "numberInSurah": 49,
          "juz": 29,
          "manzil": 7,
          "page": 577,
          "ruku": 511,
          "hizbQuarter": 230,
          "sajda": false
        },
        {
          "number": 5545,
          "text": "كَأَنَّهُمْ حُمُرٌۭ مُّسْتَنفِرَةٌۭ",
          "numberInSurah": 50,
          "juz": 29,
          "manzil": 7,
          "page": 577,
          "ruku": 511,
          "hizbQuarter": 230,
          "sajda": false
        },
        {
          "number": 5546,
          "text": "فَرَّتْ مِن قَسْوَرَةٍۭ",
          "numberInSurah": 51,
          "juz": 29,
          "manzil": 7,
          "page": 577,
          "ruku": 511,
          "hizbQuarter": 230,
          "sajda": false
        },
        {
          "number": 5547,
          "text": "بَلْ يُرِيدُ كُلُّ ٱمْرِئٍۢ مِّنْهُمْ أَن يُؤْتَىٰ صُحُفًۭا مُّنَشَّرَةًۭ",
          "numberInSurah": 52,
          "juz": 29,
          "manzil": 7,
          "page": 577,
          "ruku": 511,
          "hizbQuarter": 230,
          "sajda": false
        },
        {
          "number": 5548,
          "text": "كَلَّا ۖ بَل لَّا يَخَافُونَ ٱلْءَاخِرَةَ",
          "numberInSurah": 53,
          "juz": 29,
          "manzil": 7,
          "page": 577,
          "ruku": 511,
          "hizbQuarter": 230,
          "sajda": false
        },
        {
          "number": 5549,
          "text": "كَلَّآ إِنَّهُۥ تَذْكِرَةٌۭ",
          "numberInSurah": 54,
          "juz": 29,
          "manzil": 7,
          "page": 577,
          "ruku": 511,
          "hizbQuarter": 230,
          "sajda": false
        },
        {
          "number": 5550,
          "text": "فَمَن شَآءَ ذَكَرَهُۥ",
          "numberInSurah": 55,
          "juz": 29,
          "manzil": 7,
          "page": 577,
          "ruku": 511,
          "hizbQuarter": 230,
          "sajda": false
        },
        {
          "number": 5551,
          "text": "وَمَا يَذْكُرُونَ إِلَّآ أَن يَشَآءَ ٱللَّهُ ۚ هُوَ أَهْلُ ٱلتَّقْوَىٰ وَأَهْلُ ٱلْمَغْفِرَةِ",
          "numberInSurah": 56,
          "juz": 29,
          "manzil": 7,
          "page": 577,
          "ruku": 511,
          "hizbQuarter": 230,
          "sajda": false
        }
