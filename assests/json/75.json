  "data": [
    {
      "number": 75,
      "name": "سورة القيامة",
      "englishName": "Al-Qiyaama",
      "englishNameTranslation": "The Resurrection",
      "revelationType": "Meccan",
      "numberOfAyahs": 40,
      "ayahs": [
        {
          "number": 5552,
          "text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ لَآ أُقْسِمُ بِيَوْمِ ٱلْقِيَٰمَةِ",
          "numberInSurah": 1,
          "juz": 29,
          "manzil": 7,
          "page": 577,
          "ruku": 512,
          "hizbQuarter": 231,
          "sajda": false
        },
        {
          "number": 5553,
          "text": "وَلَآ أُقْسِمُ بِٱلنَّفْسِ ٱللَّوَّامَةِ",
          "numberInSurah": 2,
          "juz": 29,
          "manzil": 7,
          "page": 577,
          "ruku": 512,
          "hizbQuarter": 231,
          "sajda": false
        },
        {
          "number": 5554,
          "text": "أَيَحْسَبُ ٱلْإِنسَٰنُ أَلَّن نَّجْمَعَ عِظَامَهُۥ",
          "numberInSurah": 3,
          "juz": 29,
          "manzil": 7,
          "page": 577,
          "ruku": 512,
          "hizbQuarter": 231,
          "sajda": false
        },
        {
          "number": 5555,
          "text": "بَلَىٰ قَٰدِرِينَ عَلَىٰٓ أَن نُّسَوِّىَ بَنَانَهُۥ",
          "numberInSurah": 4,
          "juz": 29,
          "manzil": 7,
          "page": 577,
          "ruku": 512,
          "hizbQuarter": 231,
          "sajda": false
        },
        {
          "number": 5556,
          "text": "بَلْ يُرِيدُ ٱلْإِنسَٰنُ لِيَفْجُرَ أَمَامَهُۥ",
          "numberInSurah": 5,
          "juz": 29,
          "manzil": 7,
          "page": 577,
          "ruku": 512,
          "hizbQuarter": 231,
          "sajda": false
        },
        {
          "number": 5557,
          "text": "يَسْـَٔلُ أَيَّانَ يَوْمُ ٱلْقِيَٰمَةِ",
          "numberInSurah": 6,
          "juz": 29,
          "manzil": 7,
          "page": 577,
          "ruku": 512,
          "hizbQuarter": 231,
          "sajda": false
        },
        {
          "number": 5558,
          "text": "فَإِذَا بَرِقَ ٱلْبَصَرُ",
          "numberInSurah": 7,
          "juz": 29,
          "manzil": 7,
          "page": 577,
          "ruku": 512,
          "hizbQuarter": 231,
          "sajda": false
        },
        {
          "number": 5559,
          "text": "وَخَسَفَ ٱلْقَمَرُ",
          "numberInSurah": 8,
          "juz": 29,
          "manzil": 7,
          "page": 577,
          "ruku": 512,
          "hizbQuarter": 231,
          "sajda": false
        },
        {
          "number": 5560,
          "text": "وَجُمِعَ ٱلشَّمْسُ وَٱلْقَمَرُ",
          "numberInSurah": 9,
          "juz": 29,
          "manzil": 7,
          "page": 577,
          "ruku": 512,
          "hizbQuarter": 231,
          "sajda": false
        },
        {
          "number": 5561,
          "text": "يَقُولُ ٱلْإِنسَٰنُ يَوْمَئِذٍ أَيْنَ ٱلْمَفَرُّ",
          "numberInSurah": 10,
          "juz": 29,
          "manzil": 7,
          "page": 577,
          "ruku": 512,
          "hizbQuarter": 231,
          "sajda": false
        },
        {
          "number": 5562,
          "text": "كَلَّا لَا وَزَرَ",
          "numberInSurah": 11,
          "juz": 29,
          "manzil": 7,
          "page": 577,
          "ruku": 512,
          "hizbQuarter": 231,
          "sajda": false
        },
        {
          "number": 5563,
          "text": "إِلَىٰ رَبِّكَ يَوْمَئِذٍ ٱلْمُسْتَقَرُّ",
          "numberInSurah": 12,
          "juz": 29,
          "manzil": 7,
          "page": 577,
          "ruku": 512,
          "hizbQuarter": 231,
          "sajda": false
        },
        {
          "number": 5564,
          "text": "يُنَبَّؤُا۟ ٱلْإِنسَٰنُ يَوْمَئِذٍۭ بِمَا قَدَّمَ وَأَخَّرَ",
          "numberInSurah": 13,
          "juz": 29,
          "manzil": 7,
          "page": 577,
          "ruku": 512,
          "hizbQuarter": 231,
          "sajda": false
        },
        {
          "number": 5565,
          "text": "بَلِ ٱلْإِنسَٰنُ عَلَىٰ نَفْسِهِۦ بَصِيرَةٌۭ",
          "numberInSurah": 14,
          "juz": 29,
          "manzil": 7,
          "page": 577,
          "ruku": 512,
          "hizbQuarter": 231,
          "sajda": false
        },
        {
          "number": 5566,
          "text": "وَلَوْ أَلْقَىٰ مَعَاذِيرَهُۥ",
          "numberInSurah": 15,
          "juz": 29,
          "manzil": 7,
          "page": 577,
          "ruku": 512,
          "hizbQuarter": 231,
          "sajda": false
        },
        {
          "number": 5567,
          "text": "لَا تُحَرِّكْ بِهِۦ لِسَانَكَ لِتَعْجَلَ بِهِۦٓ",
          "numberInSurah": 16,
          "juz": 29,
          "manzil": 7,
          "page": 577,
          "ruku": 512,
          "hizbQuarter": 231,
          "sajda": false
        },
        {
          "number": 5568,
          "text": "إِنَّ عَلَيْنَا جَمْعَهُۥ وَقُرْءَانَهُۥ",
          "numberInSurah": 17,
          "juz": 29,
          "manzil": 7,
          "page": 577,
          "ruku": 512,
          "hizbQuarter": 231,
          "sajda": false
        },
        {
          "number": 5569,
          "text": "فَإِذَا قَرَأْنَٰهُ فَٱتَّبِعْ قُرْءَانَهُۥ",
          "numberInSurah": 18,
          "juz": 29,
          "manzil": 7,
          "page": 577,
          "ruku": 512,
          "hizbQuarter": 231,
          "sajda": false
        },
        {
          "number": 5570,
          "text": "ثُمَّ إِنَّ عَلَيْنَا بَيَانَهُۥ",
          "numberInSurah": 19,
          "juz": 29,
          "manzil": 7,
          "page": 577,
          "ruku": 512,
          "hizbQuarter": 231,
          "sajda": false
        },
        {
          "number": 5571,
          "text": "كَلَّا بَلْ تُحِبُّونَ ٱلْعَاجِلَةَ",
          "numberInSurah": 20,
          "juz": 29,
          "manzil": 7,
          "page": 578,
          "ruku": 512,
          "hizbQuarter": 231,
          "sajda": false
        },
        {
          "number": 5572,
          "text": "وَتَذَرُونَ ٱلْءَاخِرَةَ",
          "numberInSurah": 21,
          "juz": 29,
          "manzil": 7,
          "page": 578,
          "ruku": 512,
          "hizbQuarter": 231,
          "sajda": false
        },
        {
          "number": 5573,
          "text": "وُجُوهٌۭ يَوْمَئِذٍۢ نَّاضِرَةٌ",
          "numberInSurah": 22,
          "juz": 29,
          "manzil": 7,
          "page": 578,
          "ruku": 512,
          "hizbQuarter": 231,
          "sajda": false
        },
        {
          "number": 5574,
          "text": "إِلَىٰ رَبِّهَا نَاظِرَةٌۭ",
          "numberInSurah": 23,
          "juz": 29,
          "manzil": 7,
          "page": 578,
          "ruku": 512,
          "hizbQuarter": 231,
          "sajda": false
        },
        {
          "number": 5575,
          "text": "وَوُجُوهٌۭ يَوْمَئِذٍۭ بَاسِرَةٌۭ",
          "numberInSurah": 24,
          "juz": 29,
          "manzil": 7,
          "page": 578,
          "ruku": 512,
          "hizbQuarter": 231,
          "sajda": false
        },
        {
          "number": 5576,
          "text": "تَظُنُّ أَن يُفْعَلَ بِهَا فَاقِرَةٌۭ",
          "numberInSurah": 25,
          "juz": 29,
          "manzil": 7,
          "page": 578,
          "ruku": 512,
          "hizbQuarter": 231,
          "sajda": false
        },
        {
          "number": 5577,
          "text": "كَلَّآ إِذَا بَلَغَتِ ٱلتَّرَاقِىَ",
          "numberInSurah": 26,
          "juz": 29,
          "manzil": 7,
          "page": 578,
          "ruku": 512,
          "hizbQuarter": 231,
          "sajda": false
        },
        {
          "number": 5578,
          "text": "وَقِيلَ مَنْ ۜ رَاقٍۢ",
          "numberInSurah": 27,
          "juz": 29,
          "manzil": 7,
          "page": 578,
          "ruku": 512,
          "hizbQuarter": 231,
          "sajda": false
        },
        {
          "number": 5579,
          "text": "وَظَنَّ أَنَّهُ ٱلْفِرَاقُ",
          "numberInSurah": 28,
          "juz": 29,
          "manzil": 7,
          "page": 578,
          "ruku": 512,
          "hizbQuarter": 231,
          "sajda": false
        },
        {
          "number": 5580,
          "text": "وَٱلْتَفَّتِ ٱلسَّاقُ بِٱلسَّاقِ",
          "numberInSurah": 29,
          "juz": 29,
          "manzil": 7,
          "page": 578,
          "ruku": 512,
          "hizbQuarter": 231,
          "sajda": false
        },
        {
          "number": 5581,
          "text": "إِلَىٰ رَبِّكَ يَوْمَئِذٍ ٱلْمَسَاقُ",
          "numberInSurah": 30,
          "juz": 29,
          "manzil": 7,
          "page": 578,
          "ruku": 512,
          "hizbQuarter": 231,
          "sajda": false
        },
        {
          "number": 5582,
          "text": "فَلَا صَدَّقَ وَلَا صَلَّىٰ",
          "numberInSurah": 31,
          "juz": 29,
          "manzil": 7,
          "page": 578,
          "ruku": 513,
          "hizbQuarter": 231,
          "sajda": false
        },
        {
          "number": 5583,
          "text": "وَلَٰكِن كَذَّبَ وَتَوَلَّىٰ",
          "numberInSurah": 32,
          "juz": 29,
          "manzil": 7,
          "page": 578,
          "ruku": 513,
          "hizbQuarter": 231,
          "sajda": false
        },
        {
          "number": 5584,
          "text": "ثُمَّ ذَهَبَ إِلَىٰٓ أَهْلِهِۦ يَتَمَطَّىٰٓ",
          "numberInSurah": 33,
          "juz": 29,
          "manzil": 7,
          "page": 578,
          "ruku": 513,
          "hizbQuarter": 231,
          "sajda": false
        },
        {
          "number": 5585,
          "text": "أَوْلَىٰ لَكَ فَأَوْلَىٰ",
          "numberInSurah": 34,
          "juz": 29,
          "manzil": 7,
          "page": 578,
          "ruku": 513,
          "hizbQuarter": 231,
          "sajda": false
        },
        {
          "number": 5586,
          "text": "ثُمَّ أَوْلَىٰ لَكَ فَأَوْلَىٰٓ",
          "numberInSurah": 35,
          "juz": 29,
          "manzil": 7,
          "page": 578,
          "ruku": 513,
          "hizbQuarter": 231,
          "sajda": false
        },
        {
          "number": 5587,
          "text": "أَيَحْسَبُ ٱلْإِنسَٰنُ أَن يُتْرَكَ سُدًى",
          "numberInSurah": 36,
          "juz": 29,
          "manzil": 7,
          "page": 578,
          "ruku": 513,
          "hizbQuarter": 231,
          "sajda": false
        },
        {
          "number": 5588,
          "text": "أَلَمْ يَكُ نُطْفَةًۭ مِّن مَّنِىٍّۢ يُمْنَىٰ",
          "numberInSurah": 37,
          "juz": 29,
          "manzil": 7,
          "page": 578,
          "ruku": 513,
          "hizbQuarter": 231,
          "sajda": false
        },
        {
          "number": 5589,
          "text": "ثُمَّ كَانَ عَلَقَةًۭ فَخَلَقَ فَسَوَّىٰ",
          "numberInSurah": 38,
          "juz": 29,
          "manzil": 7,
          "page": 578,
          "ruku": 513,
          "hizbQuarter": 231,
          "sajda": false
        },
        {
          "number": 5590,
          "text": "فَجَعَلَ مِنْهُ ٱلزَّوْجَيْنِ ٱلذَّكَرَ وَٱلْأُنثَىٰٓ",
          "numberInSurah": 39,
          "juz": 29,
          "manzil": 7,
          "page": 578,
          "ruku": 513,
          "hizbQuarter": 231,
          "sajda": false
        },
        {
          "number": 5591,
          "text": "أَلَيْسَ ذَٰلِكَ بِقَٰدِرٍ عَلَىٰٓ أَن يُحْۦِىَ ٱلْمَوْتَىٰ",
          "numberInSurah": 40,
          "juz": 29,
          "manzil": 7,
          "page": 578,
          "ruku": 513,
          "hizbQuarter": 231,
          "sajda": false
        }
