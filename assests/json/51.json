  "data": [
    {
      "number": 51,
      "name": "سورة الذاريات",
      "englishName": "Adh-Dhaariyat",
      "englishNameTranslation": "The Winnowing Winds",
      "revelationType": "Meccan",
      "numberOfAyahs": 60,
      "ayahs": [
        {
          "number": 4676,
          "text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ وَٱلذَّٰرِيَٰتِ ذَرْوًۭا",
          "numberInSurah": 1,
          "juz": 26,
          "manzil": 7,
          "page": 520,
          "ruku": 455,
          "hizbQuarter": 208,
          "sajda": false
        },
        {
          "number": 4677,
          "text": "فَٱلْحَٰمِلَٰتِ وِقْرًۭا",
          "numberInSurah": 2,
          "juz": 26,
          "manzil": 7,
          "page": 520,
          "ruku": 455,
          "hizbQuarter": 208,
          "sajda": false
        },
        {
          "number": 4678,
          "text": "فَٱلْجَٰرِيَٰتِ يُسْرًۭا",
          "numberInSurah": 3,
          "juz": 26,
          "manzil": 7,
          "page": 520,
          "ruku": 455,
          "hizbQuarter": 208,
          "sajda": false
        },
        {
          "number": 4679,
          "text": "فَٱلْمُقَسِّمَٰتِ أَمْرًا",
          "numberInSurah": 4,
          "juz": 26,
          "manzil": 7,
          "page": 520,
          "ruku": 455,
          "hizbQuarter": 208,
          "sajda": false
        },
        {
          "number": 4680,
          "text": "إِنَّمَا تُوعَدُونَ لَصَادِقٌۭ",
          "numberInSurah": 5,
          "juz": 26,
          "manzil": 7,
          "page": 520,
          "ruku": 455,
          "hizbQuarter": 208,
          "sajda": false
        },
        {
          "number": 4681,
          "text": "وَإِنَّ ٱلدِّينَ لَوَٰقِعٌۭ",
          "numberInSurah": 6,
          "juz": 26,
          "manzil": 7,
          "page": 520,
          "ruku": 455,
          "hizbQuarter": 208,
          "sajda": false
        },
        {
          "number": 4682,
          "text": "وَٱلسَّمَآءِ ذَاتِ ٱلْحُبُكِ",
          "numberInSurah": 7,
          "juz": 26,
          "manzil": 7,
          "page": 521,
          "ruku": 455,
          "hizbQuarter": 208,
          "sajda": false
        },
        {
          "number": 4683,
          "text": "إِنَّكُمْ لَفِى قَوْلٍۢ مُّخْتَلِفٍۢ",
          "numberInSurah": 8,
          "juz": 26,
          "manzil": 7,
          "page": 521,
          "ruku": 455,
          "hizbQuarter": 208,
          "sajda": false
        },
        {
          "number": 4684,
          "text": "يُؤْفَكُ عَنْهُ مَنْ أُفِكَ",
          "numberInSurah": 9,
          "juz": 26,
          "manzil": 7,
          "page": 521,
          "ruku": 455,
          "hizbQuarter": 208,
          "sajda": false
        },
        {
          "number": 4685,
          "text": "قُتِلَ ٱلْخَرَّٰصُونَ",
          "numberInSurah": 10,
          "juz": 26,
          "manzil": 7,
          "page": 521,
          "ruku": 455,
          "hizbQuarter": 208,
          "sajda": false
        },
        {
          "number": 4686,
          "text": "ٱلَّذِينَ هُمْ فِى غَمْرَةٍۢ سَاهُونَ",
          "numberInSurah": 11,
          "juz": 26,
          "manzil": 7,
          "page": 521,
          "ruku": 455,
          "hizbQuarter": 208,
          "sajda": false
        },
        {
          "number": 4687,
          "text": "يَسْـَٔلُونَ أَيَّانَ يَوْمُ ٱلدِّينِ",
          "numberInSurah": 12,
          "juz": 26,
          "manzil": 7,
          "page": 521,
          "ruku": 455,
          "hizbQuarter": 208,
          "sajda": false
        },
        {
          "number": 4688,
          "text": "يَوْمَ هُمْ عَلَى ٱلنَّارِ يُفْتَنُونَ",
          "numberInSurah": 13,
          "juz": 26,
          "manzil": 7,
          "page": 521,
          "ruku": 455,
          "hizbQuarter": 208,
          "sajda": false
        },
        {
          "number": 4689,
          "text": "ذُوقُوا۟ فِتْنَتَكُمْ هَٰذَا ٱلَّذِى كُنتُم بِهِۦ تَسْتَعْجِلُونَ",
          "numberInSurah": 14,
          "juz": 26,
          "manzil": 7,
          "page": 521,
          "ruku": 455,
          "hizbQuarter": 208,
          "sajda": false
        },
        {
          "number": 4690,
          "text": "إِنَّ ٱلْمُتَّقِينَ فِى جَنَّٰتٍۢ وَعُيُونٍ",
          "numberInSurah": 15,
          "juz": 26,
          "manzil": 7,
          "page": 521,
          "ruku": 455,
          "hizbQuarter": 208,
          "sajda": false
        },
        {
          "number": 4691,
          "text": "ءَاخِذِينَ مَآ ءَاتَىٰهُمْ رَبُّهُمْ ۚ إِنَّهُمْ كَانُوا۟ قَبْلَ ذَٰلِكَ مُحْسِنِينَ",
          "numberInSurah": 16,
          "juz": 26,
          "manzil": 7,
          "page": 521,
          "ruku": 455,
          "hizbQuarter": 208,
          "sajda": false
        },
        {
          "number": 4692,
          "text": "كَانُوا۟ قَلِيلًۭا مِّنَ ٱلَّيْلِ مَا يَهْجَعُونَ",
          "numberInSurah": 17,
          "juz": 26,
          "manzil": 7,
          "page": 521,
          "ruku": 455,
          "hizbQuarter": 208,
          "sajda": false
        },
        {
          "number": 4693,
          "text": "وَبِٱلْأَسْحَارِ هُمْ يَسْتَغْفِرُونَ",
          "numberInSurah": 18,
          "juz": 26,
          "manzil": 7,
          "page": 521,
          "ruku": 455,
          "hizbQuarter": 208,
          "sajda": false
        },
        {
          "number": 4694,
          "text": "وَفِىٓ أَمْوَٰلِهِمْ حَقٌّۭ لِّلسَّآئِلِ وَٱلْمَحْرُومِ",
          "numberInSurah": 19,
          "juz": 26,
          "manzil": 7,
          "page": 521,
          "ruku": 455,
          "hizbQuarter": 208,
          "sajda": false
        },
        {
          "number": 4695,
          "text": "وَفِى ٱلْأَرْضِ ءَايَٰتٌۭ لِّلْمُوقِنِينَ",
          "numberInSurah": 20,
          "juz": 26,
          "manzil": 7,
          "page": 521,
          "ruku": 455,
          "hizbQuarter": 208,
          "sajda": false
        },
        {
          "number": 4696,
          "text": "وَفِىٓ أَنفُسِكُمْ ۚ أَفَلَا تُبْصِرُونَ",
          "numberInSurah": 21,
          "juz": 26,
          "manzil": 7,
          "page": 521,
          "ruku": 455,
          "hizbQuarter": 208,
          "sajda": false
        },
        {
          "number": 4697,
          "text": "وَفِى ٱلسَّمَآءِ رِزْقُكُمْ وَمَا تُوعَدُونَ",
          "numberInSurah": 22,
          "juz": 26,
          "manzil": 7,
          "page": 521,
          "ruku": 455,
          "hizbQuarter": 208,
          "sajda": false
        },
        {
          "number": 4698,
          "text": "فَوَرَبِّ ٱلسَّمَآءِ وَٱلْأَرْضِ إِنَّهُۥ لَحَقٌّۭ مِّثْلَ مَآ أَنَّكُمْ تَنطِقُونَ",
          "numberInSurah": 23,
          "juz": 26,
          "manzil": 7,
          "page": 521,
          "ruku": 455,
          "hizbQuarter": 208,
          "sajda": false
        },
        {
          "number": 4699,
          "text": "هَلْ أَتَىٰكَ حَدِيثُ ضَيْفِ إِبْرَٰهِيمَ ٱلْمُكْرَمِينَ",
          "numberInSurah": 24,
          "juz": 26,
          "manzil": 7,
          "page": 521,
          "ruku": 456,
          "hizbQuarter": 208,
          "sajda": false
        },
        {
          "number": 4700,
          "text": "إِذْ دَخَلُوا۟ عَلَيْهِ فَقَالُوا۟ سَلَٰمًۭا ۖ قَالَ سَلَٰمٌۭ قَوْمٌۭ مُّنكَرُونَ",
          "numberInSurah": 25,
          "juz": 26,
          "manzil": 7,
          "page": 521,
          "ruku": 456,
          "hizbQuarter": 208,
          "sajda": false
        },
        {
          "number": 4701,
          "text": "فَرَاغَ إِلَىٰٓ أَهْلِهِۦ فَجَآءَ بِعِجْلٍۢ سَمِينٍۢ",
          "numberInSurah": 26,
          "juz": 26,
          "manzil": 7,
          "page": 521,
          "ruku": 456,
          "hizbQuarter": 208,
          "sajda": false
        },
        {
          "number": 4702,
          "text": "فَقَرَّبَهُۥٓ إِلَيْهِمْ قَالَ أَلَا تَأْكُلُونَ",
          "numberInSurah": 27,
          "juz": 26,
          "manzil": 7,
          "page": 521,
          "ruku": 456,
          "hizbQuarter": 208,
          "sajda": false
        },
        {
          "number": 4703,
          "text": "فَأَوْجَسَ مِنْهُمْ خِيفَةًۭ ۖ قَالُوا۟ لَا تَخَفْ ۖ وَبَشَّرُوهُ بِغُلَٰمٍ عَلِيمٍۢ",
          "numberInSurah": 28,
          "juz": 26,
          "manzil": 7,
          "page": 521,
          "ruku": 456,
          "hizbQuarter": 208,
          "sajda": false
        },
        {
          "number": 4704,
          "text": "فَأَقْبَلَتِ ٱمْرَأَتُهُۥ فِى صَرَّةٍۢ فَصَكَّتْ وَجْهَهَا وَقَالَتْ عَجُوزٌ عَقِيمٌۭ",
          "numberInSurah": 29,
          "juz": 26,
          "manzil": 7,
          "page": 521,
          "ruku": 456,
          "hizbQuarter": 208,
          "sajda": false
        },
        {
          "number": 4705,
          "text": "قَالُوا۟ كَذَٰلِكِ قَالَ رَبُّكِ ۖ إِنَّهُۥ هُوَ ٱلْحَكِيمُ ٱلْعَلِيمُ",
          "numberInSurah": 30,
          "juz": 26,
          "manzil": 7,
          "page": 521,
          "ruku": 456,
          "hizbQuarter": 208,
          "sajda": false
        },
        {
          "number": 4706,
          "text": "۞ قَالَ فَمَا خَطْبُكُمْ أَيُّهَا ٱلْمُرْسَلُونَ",
          "numberInSurah": 31,
          "juz": 27,
          "manzil": 7,
          "page": 522,
          "ruku": 456,
          "hizbQuarter": 209,
          "sajda": false
        },
        {
          "number": 4707,
          "text": "قَالُوٓا۟ إِنَّآ أُرْسِلْنَآ إِلَىٰ قَوْمٍۢ مُّجْرِمِينَ",
          "numberInSurah": 32,
          "juz": 27,
          "manzil": 7,
          "page": 522,
          "ruku": 456,
          "hizbQuarter": 209,
          "sajda": false
        },
        {
          "number": 4708,
          "text": "لِنُرْسِلَ عَلَيْهِمْ حِجَارَةًۭ مِّن طِينٍۢ",
          "numberInSurah": 33,
          "juz": 27,
          "manzil": 7,
          "page": 522,
          "ruku": 456,
          "hizbQuarter": 209,
          "sajda": false
        },
        {
          "number": 4709,
          "text": "مُّسَوَّمَةً عِندَ رَبِّكَ لِلْمُسْرِفِينَ",
          "numberInSurah": 34,
          "juz": 27,
          "manzil": 7,
          "page": 522,
          "ruku": 456,
          "hizbQuarter": 209,
          "sajda": false
        },
        {
          "number": 4710,
          "text": "فَأَخْرَجْنَا مَن كَانَ فِيهَا مِنَ ٱلْمُؤْمِنِينَ",
          "numberInSurah": 35,
          "juz": 27,
          "manzil": 7,
          "page": 522,
          "ruku": 456,
          "hizbQuarter": 209,
          "sajda": false
        },
        {
          "number": 4711,
          "text": "فَمَا وَجَدْنَا فِيهَا غَيْرَ بَيْتٍۢ مِّنَ ٱلْمُسْلِمِينَ",
          "numberInSurah": 36,
          "juz": 27,
          "manzil": 7,
          "page": 522,
          "ruku": 456,
          "hizbQuarter": 209,
          "sajda": false
        },
        {
          "number": 4712,
          "text": "وَتَرَكْنَا فِيهَآ ءَايَةًۭ لِّلَّذِينَ يَخَافُونَ ٱلْعَذَابَ ٱلْأَلِيمَ",
          "numberInSurah": 37,
          "juz": 27,
          "manzil": 7,
          "page": 522,
          "ruku": 456,
          "hizbQuarter": 209,
          "sajda": false
        },
        {
          "number": 4713,
          "text": "وَفِى مُوسَىٰٓ إِذْ أَرْسَلْنَٰهُ إِلَىٰ فِرْعَوْنَ بِسُلْطَٰنٍۢ مُّبِينٍۢ",
          "numberInSurah": 38,
          "juz": 27,
          "manzil": 7,
          "page": 522,
          "ruku": 456,
          "hizbQuarter": 209,
          "sajda": false
        },
        {
          "number": 4714,
          "text": "فَتَوَلَّىٰ بِرُكْنِهِۦ وَقَالَ سَٰحِرٌ أَوْ مَجْنُونٌۭ",
          "numberInSurah": 39,
          "juz": 27,
          "manzil": 7,
          "page": 522,
          "ruku": 456,
          "hizbQuarter": 209,
          "sajda": false
        },
        {
          "number": 4715,
          "text": "فَأَخَذْنَٰهُ وَجُنُودَهُۥ فَنَبَذْنَٰهُمْ فِى ٱلْيَمِّ وَهُوَ مُلِيمٌۭ",
          "numberInSurah": 40,
          "juz": 27,
          "manzil": 7,
          "page": 522,
          "ruku": 456,
          "hizbQuarter": 209,
          "sajda": false
        },
        {
          "number": 4716,
          "text": "وَفِى عَادٍ إِذْ أَرْسَلْنَا عَلَيْهِمُ ٱلرِّيحَ ٱلْعَقِيمَ",
          "numberInSurah": 41,
          "juz": 27,
          "manzil": 7,
          "page": 522,
          "ruku": 456,
          "hizbQuarter": 209,
          "sajda": false
        },
        {
          "number": 4717,
          "text": "مَا تَذَرُ مِن شَىْءٍ أَتَتْ عَلَيْهِ إِلَّا جَعَلَتْهُ كَٱلرَّمِيمِ",
          "numberInSurah": 42,
          "juz": 27,
          "manzil": 7,
          "page": 522,
          "ruku": 456,
          "hizbQuarter": 209,
          "sajda": false
        },
        {
          "number": 4718,
          "text": "وَفِى ثَمُودَ إِذْ قِيلَ لَهُمْ تَمَتَّعُوا۟ حَتَّىٰ حِينٍۢ",
          "numberInSurah": 43,
          "juz": 27,
          "manzil": 7,
          "page": 522,
          "ruku": 456,
          "hizbQuarter": 209,
          "sajda": false
        },
        {
          "number": 4719,
          "text": "فَعَتَوْا۟ عَنْ أَمْرِ رَبِّهِمْ فَأَخَذَتْهُمُ ٱلصَّٰعِقَةُ وَهُمْ يَنظُرُونَ",
          "numberInSurah": 44,
          "juz": 27,
          "manzil": 7,
          "page": 522,
          "ruku": 456,
          "hizbQuarter": 209,
          "sajda": false
        },
        {
          "number": 4720,
          "text": "فَمَا ٱسْتَطَٰعُوا۟ مِن قِيَامٍۢ وَمَا كَانُوا۟ مُنتَصِرِينَ",
          "numberInSurah": 45,
          "juz": 27,
          "manzil": 7,
          "page": 522,
          "ruku": 456,
          "hizbQuarter": 209,
          "sajda": false
        },
        {
          "number": 4721,
          "text": "وَقَوْمَ نُوحٍۢ مِّن قَبْلُ ۖ إِنَّهُمْ كَانُوا۟ قَوْمًۭا فَٰسِقِينَ",
          "numberInSurah": 46,
          "juz": 27,
          "manzil": 7,
          "page": 522,
          "ruku": 456,
          "hizbQuarter": 209,
          "sajda": false
        },
        {
          "number": 4722,
          "text": "وَٱلسَّمَآءَ بَنَيْنَٰهَا بِأَيْي۟دٍۢ وَإِنَّا لَمُوسِعُونَ",
          "numberInSurah": 47,
          "juz": 27,
          "manzil": 7,
          "page": 522,
          "ruku": 457,
          "hizbQuarter": 209,
          "sajda": false
        },
        {
          "number": 4723,
          "text": "وَٱلْأَرْضَ فَرَشْنَٰهَا فَنِعْمَ ٱلْمَٰهِدُونَ",
          "numberInSurah": 48,
          "juz": 27,
          "manzil": 7,
          "page": 522,
          "ruku": 457,
          "hizbQuarter": 209,
          "sajda": false
        },
        {
          "number": 4724,
          "text": "وَمِن كُلِّ شَىْءٍ خَلَقْنَا زَوْجَيْنِ لَعَلَّكُمْ تَذَكَّرُونَ",
          "numberInSurah": 49,
          "juz": 27,
          "manzil": 7,
          "page": 522,
          "ruku": 457,
          "hizbQuarter": 209,
          "sajda": false
        },
        {
          "number": 4725,
          "text": "فَفِرُّوٓا۟ إِلَى ٱللَّهِ ۖ إِنِّى لَكُم مِّنْهُ نَذِيرٌۭ مُّبِينٌۭ",
          "numberInSurah": 50,
          "juz": 27,
          "manzil": 7,
          "page": 522,
          "ruku": 457,
          "hizbQuarter": 209,
          "sajda": false
        },
        {
          "number": 4726,
          "text": "وَلَا تَجْعَلُوا۟ مَعَ ٱللَّهِ إِلَٰهًا ءَاخَرَ ۖ إِنِّى لَكُم مِّنْهُ نَذِيرٌۭ مُّبِينٌۭ",
          "numberInSurah": 51,
          "juz": 27,
          "manzil": 7,
          "page": 522,
          "ruku": 457,
          "hizbQuarter": 209,
          "sajda": false
        },
        {
          "number": 4727,
