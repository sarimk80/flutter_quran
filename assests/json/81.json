    {
      "number": 81,
      "name": "سورة التكوير",
      "englishName": "At-Takwir",
      "englishNameTranslation": "The Overthrowing",
      "revelationType": "Meccan",
      "numberOfAyahs": 29,
      "ayahs": [
        {
          "number": 5801,
          "text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ إِذَا ٱلشَّمْسُ كُوِّرَتْ",
          "numberInSurah": 1,
          "juz": 30,
          "manzil": 7,
          "page": 586,
          "ruku": 523,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5802,
          "text": "وَإِذَا ٱلنُّجُومُ ٱنكَدَرَتْ",
          "numberInSurah": 2,
          "juz": 30,
          "manzil": 7,
          "page": 586,
          "ruku": 523,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5803,
          "text": "وَإِذَا ٱلْجِبَالُ سُيِّرَتْ",
          "numberInSurah": 3,
          "juz": 30,
          "manzil": 7,
          "page": 586,
          "ruku": 523,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5804,
          "text": "وَإِذَا ٱلْعِشَارُ عُطِّلَتْ",
          "numberInSurah": 4,
          "juz": 30,
          "manzil": 7,
          "page": 586,
          "ruku": 523,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5805,
          "text": "وَإِذَا ٱلْوُحُوشُ حُشِرَتْ",
          "numberInSurah": 5,
          "juz": 30,
          "manzil": 7,
          "page": 586,
          "ruku": 523,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5806,
          "text": "وَإِذَا ٱلْبِحَارُ سُجِّرَتْ",
          "numberInSurah": 6,
          "juz": 30,
          "manzil": 7,
          "page": 586,
          "ruku": 523,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5807,
          "text": "وَإِذَا ٱلنُّفُوسُ زُوِّجَتْ",
          "numberInSurah": 7,
          "juz": 30,
          "manzil": 7,
          "page": 586,
          "ruku": 523,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5808,
          "text": "وَإِذَا ٱلْمَوْءُۥدَةُ سُئِلَتْ",
          "numberInSurah": 8,
          "juz": 30,
          "manzil": 7,
          "page": 586,
          "ruku": 523,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5809,
          "text": "بِأَىِّ ذَنۢبٍۢ قُتِلَتْ",
          "numberInSurah": 9,
          "juz": 30,
          "manzil": 7,
          "page": 586,
          "ruku": 523,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5810,
          "text": "وَإِذَا ٱلصُّحُفُ نُشِرَتْ",
          "numberInSurah": 10,
          "juz": 30,
          "manzil": 7,
          "page": 586,
          "ruku": 523,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5811,
          "text": "وَإِذَا ٱلسَّمَآءُ كُشِطَتْ",
          "numberInSurah": 11,
          "juz": 30,
          "manzil": 7,
          "page": 586,
          "ruku": 523,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5812,
          "text": "وَإِذَا ٱلْجَحِيمُ سُعِّرَتْ",
          "numberInSurah": 12,
          "juz": 30,
          "manzil": 7,
          "page": 586,
          "ruku": 523,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5813,
          "text": "وَإِذَا ٱلْجَنَّةُ أُزْلِفَتْ",
          "numberInSurah": 13,
          "juz": 30,
          "manzil": 7,
          "page": 586,
          "ruku": 523,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5814,
          "text": "عَلِمَتْ نَفْسٌۭ مَّآ أَحْضَرَتْ",
          "numberInSurah": 14,
          "juz": 30,
          "manzil": 7,
          "page": 586,
          "ruku": 523,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5815,
          "text": "فَلَآ أُقْسِمُ بِٱلْخُنَّسِ",
          "numberInSurah": 15,
          "juz": 30,
          "manzil": 7,
          "page": 586,
          "ruku": 523,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5816,
          "text": "ٱلْجَوَارِ ٱلْكُنَّسِ",
          "numberInSurah": 16,
          "juz": 30,
          "manzil": 7,
          "page": 586,
          "ruku": 523,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5817,
          "text": "وَٱلَّيْلِ إِذَا عَسْعَسَ",
          "numberInSurah": 17,
          "juz": 30,
          "manzil": 7,
          "page": 586,
          "ruku": 523,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5818,
          "text": "وَٱلصُّبْحِ إِذَا تَنَفَّسَ",
          "numberInSurah": 18,
          "juz": 30,
          "manzil": 7,
          "page": 586,
          "ruku": 523,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5819,
          "text": "إِنَّهُۥ لَقَوْلُ رَسُولٍۢ كَرِيمٍۢ",
          "numberInSurah": 19,
          "juz": 30,
          "manzil": 7,
          "page": 586,
          "ruku": 523,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5820,
          "text": "ذِى قُوَّةٍ عِندَ ذِى ٱلْعَرْشِ مَكِينٍۢ",
          "numberInSurah": 20,
          "juz": 30,
          "manzil": 7,
          "page": 586,
          "ruku": 523,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5821,
          "text": "مُّطَاعٍۢ ثَمَّ أَمِينٍۢ",
          "numberInSurah": 21,
          "juz": 30,
          "manzil": 7,
          "page": 586,
          "ruku": 523,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5822,
          "text": "وَمَا صَاحِبُكُم بِمَجْنُونٍۢ",
          "numberInSurah": 22,
          "juz": 30,
          "manzil": 7,
          "page": 586,
          "ruku": 523,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5823,
          "text": "وَلَقَدْ رَءَاهُ بِٱلْأُفُقِ ٱلْمُبِينِ",
          "numberInSurah": 23,
          "juz": 30,
          "manzil": 7,
          "page": 586,
          "ruku": 523,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5824,
          "text": "وَمَا هُوَ عَلَى ٱلْغَيْبِ بِضَنِينٍۢ",
          "numberInSurah": 24,
          "juz": 30,
          "manzil": 7,
          "page": 586,
          "ruku": 523,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5825,
          "text": "وَمَا هُوَ بِقَوْلِ شَيْطَٰنٍۢ رَّجِيمٍۢ",
          "numberInSurah": 25,
          "juz": 30,
          "manzil": 7,
          "page": 586,
          "ruku": 523,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5826,
          "text": "فَأَيْنَ تَذْهَبُونَ",
          "numberInSurah": 26,
          "juz": 30,
          "manzil": 7,
          "page": 586,
          "ruku": 523,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5827,
          "text": "إِنْ هُوَ إِلَّا ذِكْرٌۭ لِّلْعَٰلَمِينَ",
          "numberInSurah": 27,
          "juz": 30,
          "manzil": 7,
          "page": 586,
          "ruku": 523,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5828,
          "text": "لِمَن شَآءَ مِنكُمْ أَن يَسْتَقِيمَ",
          "numberInSurah": 28,
          "juz": 30,
          "manzil": 7,
          "page": 586,
          "ruku": 523,
          "hizbQuarter": 234,
          "sajda": false
        },
        {
          "number": 5829,
          "text": "وَمَا تَشَآءُونَ إِلَّآ أَن يَشَآءَ ٱللَّهُ رَبُّ ٱلْعَٰلَمِينَ",
          "numberInSurah": 29,
          "juz": 30,
          "manzil": 7,
          "page": 586,
          "ruku": 523,
          "hizbQuarter": 234,
          "sajda": false
        }
