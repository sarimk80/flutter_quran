  "data": [
    {
      "number": 79,
      "name": "سورة النازعات",
      "englishName": "An-Naazi'aat",
      "englishNameTranslation": "Those who drag forth",
      "revelationType": "Meccan",
      "numberOfAyahs": 46,
      "ayahs": [
        {
          "number": 5713,
          "text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ وَٱلنَّٰزِعَٰتِ غَرْقًۭا",
          "numberInSurah": 1,
          "juz": 30,
          "manzil": 7,
          "page": 583,
          "ruku": 520,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5714,
          "text": "وَٱلنَّٰشِطَٰتِ نَشْطًۭا",
          "numberInSurah": 2,
          "juz": 30,
          "manzil": 7,
          "page": 583,
          "ruku": 520,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5715,
          "text": "وَٱلسَّٰبِحَٰتِ سَبْحًۭا",
          "numberInSurah": 3,
          "juz": 30,
          "manzil": 7,
          "page": 583,
          "ruku": 520,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5716,
          "text": "فَٱلسَّٰبِقَٰتِ سَبْقًۭا",
          "numberInSurah": 4,
          "juz": 30,
          "manzil": 7,
          "page": 583,
          "ruku": 520,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5717,
          "text": "فَٱلْمُدَبِّرَٰتِ أَمْرًۭا",
          "numberInSurah": 5,
          "juz": 30,
          "manzil": 7,
          "page": 583,
          "ruku": 520,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5718,
          "text": "يَوْمَ تَرْجُفُ ٱلرَّاجِفَةُ",
          "numberInSurah": 6,
          "juz": 30,
          "manzil": 7,
          "page": 583,
          "ruku": 520,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5719,
          "text": "تَتْبَعُهَا ٱلرَّادِفَةُ",
          "numberInSurah": 7,
          "juz": 30,
          "manzil": 7,
          "page": 583,
          "ruku": 520,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5720,
          "text": "قُلُوبٌۭ يَوْمَئِذٍۢ وَاجِفَةٌ",
          "numberInSurah": 8,
          "juz": 30,
          "manzil": 7,
          "page": 583,
          "ruku": 520,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5721,
          "text": "أَبْصَٰرُهَا خَٰشِعَةٌۭ",
          "numberInSurah": 9,
          "juz": 30,
          "manzil": 7,
          "page": 583,
          "ruku": 520,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5722,
          "text": "يَقُولُونَ أَءِنَّا لَمَرْدُودُونَ فِى ٱلْحَافِرَةِ",
          "numberInSurah": 10,
          "juz": 30,
          "manzil": 7,
          "page": 583,
          "ruku": 520,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5723,
          "text": "أَءِذَا كُنَّا عِظَٰمًۭا نَّخِرَةًۭ",
          "numberInSurah": 11,
          "juz": 30,
          "manzil": 7,
          "page": 583,
          "ruku": 520,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5724,
          "text": "قَالُوا۟ تِلْكَ إِذًۭا كَرَّةٌ خَاسِرَةٌۭ",
          "numberInSurah": 12,
          "juz": 30,
          "manzil": 7,
          "page": 583,
          "ruku": 520,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5725,
          "text": "فَإِنَّمَا هِىَ زَجْرَةٌۭ وَٰحِدَةٌۭ",
          "numberInSurah": 13,
          "juz": 30,
          "manzil": 7,
          "page": 583,
          "ruku": 520,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5726,
          "text": "فَإِذَا هُم بِٱلسَّاهِرَةِ",
          "numberInSurah": 14,
          "juz": 30,
          "manzil": 7,
          "page": 583,
          "ruku": 520,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5727,
          "text": "هَلْ أَتَىٰكَ حَدِيثُ مُوسَىٰٓ",
          "numberInSurah": 15,
          "juz": 30,
          "manzil": 7,
          "page": 583,
          "ruku": 520,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5728,
          "text": "إِذْ نَادَىٰهُ رَبُّهُۥ بِٱلْوَادِ ٱلْمُقَدَّسِ طُوًى",
          "numberInSurah": 16,
          "juz": 30,
          "manzil": 7,
          "page": 584,
          "ruku": 520,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5729,
          "text": "ٱذْهَبْ إِلَىٰ فِرْعَوْنَ إِنَّهُۥ طَغَىٰ",
          "numberInSurah": 17,
          "juz": 30,
          "manzil": 7,
          "page": 584,
          "ruku": 520,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5730,
          "text": "فَقُلْ هَل لَّكَ إِلَىٰٓ أَن تَزَكَّىٰ",
          "numberInSurah": 18,
          "juz": 30,
          "manzil": 7,
          "page": 584,
          "ruku": 520,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5731,
          "text": "وَأَهْدِيَكَ إِلَىٰ رَبِّكَ فَتَخْشَىٰ",
          "numberInSurah": 19,
          "juz": 30,
          "manzil": 7,
          "page": 584,
          "ruku": 520,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5732,
          "text": "فَأَرَىٰهُ ٱلْءَايَةَ ٱلْكُبْرَىٰ",
          "numberInSurah": 20,
          "juz": 30,
          "manzil": 7,
          "page": 584,
          "ruku": 520,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5733,
          "text": "فَكَذَّبَ وَعَصَىٰ",
          "numberInSurah": 21,
          "juz": 30,
          "manzil": 7,
          "page": 584,
          "ruku": 520,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5734,
          "text": "ثُمَّ أَدْبَرَ يَسْعَىٰ",
          "numberInSurah": 22,
          "juz": 30,
          "manzil": 7,
          "page": 584,
          "ruku": 520,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5735,
          "text": "فَحَشَرَ فَنَادَىٰ",
          "numberInSurah": 23,
          "juz": 30,
          "manzil": 7,
          "page": 584,
          "ruku": 520,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5736,
          "text": "فَقَالَ أَنَا۠ رَبُّكُمُ ٱلْأَعْلَىٰ",
          "numberInSurah": 24,
          "juz": 30,
          "manzil": 7,
          "page": 584,
          "ruku": 520,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5737,
          "text": "فَأَخَذَهُ ٱللَّهُ نَكَالَ ٱلْءَاخِرَةِ وَٱلْأُولَىٰٓ",
          "numberInSurah": 25,
          "juz": 30,
          "manzil": 7,
          "page": 584,
          "ruku": 520,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5738,
          "text": "إِنَّ فِى ذَٰلِكَ لَعِبْرَةًۭ لِّمَن يَخْشَىٰٓ",
          "numberInSurah": 26,
          "juz": 30,
          "manzil": 7,
          "page": 584,
          "ruku": 520,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5739,
          "text": "ءَأَنتُمْ أَشَدُّ خَلْقًا أَمِ ٱلسَّمَآءُ ۚ بَنَىٰهَا",
          "numberInSurah": 27,
          "juz": 30,
          "manzil": 7,
          "page": 584,
          "ruku": 521,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5740,
          "text": "رَفَعَ سَمْكَهَا فَسَوَّىٰهَا",
          "numberInSurah": 28,
          "juz": 30,
          "manzil": 7,
          "page": 584,
          "ruku": 521,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5741,
          "text": "وَأَغْطَشَ لَيْلَهَا وَأَخْرَجَ ضُحَىٰهَا",
          "numberInSurah": 29,
          "juz": 30,
          "manzil": 7,
          "page": 584,
          "ruku": 521,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5742,
          "text": "وَٱلْأَرْضَ بَعْدَ ذَٰلِكَ دَحَىٰهَآ",
          "numberInSurah": 30,
          "juz": 30,
          "manzil": 7,
          "page": 584,
          "ruku": 521,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5743,
          "text": "أَخْرَجَ مِنْهَا مَآءَهَا وَمَرْعَىٰهَا",
          "numberInSurah": 31,
          "juz": 30,
          "manzil": 7,
          "page": 584,
          "ruku": 521,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5744,
          "text": "وَٱلْجِبَالَ أَرْسَىٰهَا",
          "numberInSurah": 32,
          "juz": 30,
          "manzil": 7,
          "page": 584,
          "ruku": 521,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5745,
          "text": "مَتَٰعًۭا لَّكُمْ وَلِأَنْعَٰمِكُمْ",
          "numberInSurah": 33,
          "juz": 30,
          "manzil": 7,
          "page": 584,
          "ruku": 521,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5746,
          "text": "فَإِذَا جَآءَتِ ٱلطَّآمَّةُ ٱلْكُبْرَىٰ",
          "numberInSurah": 34,
          "juz": 30,
          "manzil": 7,
          "page": 584,
          "ruku": 521,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5747,
          "text": "يَوْمَ يَتَذَكَّرُ ٱلْإِنسَٰنُ مَا سَعَىٰ",
          "numberInSurah": 35,
          "juz": 30,
          "manzil": 7,
          "page": 584,
          "ruku": 521,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5748,
          "text": "وَبُرِّزَتِ ٱلْجَحِيمُ لِمَن يَرَىٰ",
          "numberInSurah": 36,
          "juz": 30,
          "manzil": 7,
          "page": 584,
          "ruku": 521,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5749,
          "text": "فَأَمَّا مَن طَغَىٰ",
          "numberInSurah": 37,
          "juz": 30,
          "manzil": 7,
          "page": 584,
          "ruku": 521,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5750,
          "text": "وَءَاثَرَ ٱلْحَيَوٰةَ ٱلدُّنْيَا",
          "numberInSurah": 38,
          "juz": 30,
          "manzil": 7,
          "page": 584,
          "ruku": 521,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5751,
          "text": "فَإِنَّ ٱلْجَحِيمَ هِىَ ٱلْمَأْوَىٰ",
          "numberInSurah": 39,
          "juz": 30,
          "manzil": 7,
          "page": 584,
          "ruku": 521,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5752,
          "text": "وَأَمَّا مَنْ خَافَ مَقَامَ رَبِّهِۦ وَنَهَى ٱلنَّفْسَ عَنِ ٱلْهَوَىٰ",
          "numberInSurah": 40,
          "juz": 30,
          "manzil": 7,
          "page": 584,
          "ruku": 521,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5753,
          "text": "فَإِنَّ ٱلْجَنَّةَ هِىَ ٱلْمَأْوَىٰ",
          "numberInSurah": 41,
          "juz": 30,
          "manzil": 7,
          "page": 584,
          "ruku": 521,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5754,
          "text": "يَسْـَٔلُونَكَ عَنِ ٱلسَّاعَةِ أَيَّانَ مُرْسَىٰهَا",
          "numberInSurah": 42,
          "juz": 30,
          "manzil": 7,
          "page": 584,
          "ruku": 521,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5755,
          "text": "فِيمَ أَنتَ مِن ذِكْرَىٰهَآ",
          "numberInSurah": 43,
          "juz": 30,
          "manzil": 7,
          "page": 584,
          "ruku": 521,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5756,
          "text": "إِلَىٰ رَبِّكَ مُنتَهَىٰهَآ",
          "numberInSurah": 44,
          "juz": 30,
          "manzil": 7,
          "page": 584,
          "ruku": 521,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5757,
          "text": "إِنَّمَآ أَنتَ مُنذِرُ مَن يَخْشَىٰهَا",
          "numberInSurah": 45,
          "juz": 30,
          "manzil": 7,
          "page": 584,
          "ruku": 521,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5758,
          "text": "كَأَنَّهُمْ يَوْمَ يَرَوْنَهَا لَمْ يَلْبَثُوٓا۟ إِلَّا عَشِيَّةً أَوْ ضُحَىٰهَا",
          "numberInSurah": 46,
          "juz": 30,
          "manzil": 7,
          "page": 584,
          "ruku": 521,
          "hizbQuarter": 233,
          "sajda": false
        }
      ],
