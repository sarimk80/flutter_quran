      "name": "سورة الإنفطار",
      "englishName": "Al-Infitaar",
      "englishNameTranslation": "The Cleaving",
      "revelationType": "Meccan",
      "numberOfAyahs": 19,
      "ayahs": [
        {
          "number": 5830,
          "text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ إِذَا ٱلسَّمَآءُ ٱنفَطَرَتْ",
          "numberInSurah": 1,
          "juz": 30,
          "manzil": 7,
          "page": 587,
          "ruku": 524,
          "hizbQuarter": 235,
          "sajda": false
        },
        {
          "number": 5831,
          "text": "وَإِذَا ٱلْكَوَاكِبُ ٱنتَثَرَتْ",
          "numberInSurah": 2,
          "juz": 30,
          "manzil": 7,
          "page": 587,
          "ruku": 524,
          "hizbQuarter": 235,
          "sajda": false
        },
        {
          "number": 5832,
          "text": "وَإِذَا ٱلْبِحَارُ فُجِّرَتْ",
          "numberInSurah": 3,
          "juz": 30,
          "manzil": 7,
          "page": 587,
          "ruku": 524,
          "hizbQuarter": 235,
          "sajda": false
        },
        {
          "number": 5833,
          "text": "وَإِذَا ٱلْقُبُورُ بُعْثِرَتْ",
          "numberInSurah": 4,
          "juz": 30,
          "manzil": 7,
          "page": 587,
          "ruku": 524,
          "hizbQuarter": 235,
          "sajda": false
        },
        {
          "number": 5834,
          "text": "عَلِمَتْ نَفْسٌۭ مَّا قَدَّمَتْ وَأَخَّرَتْ",
          "numberInSurah": 5,
          "juz": 30,
          "manzil": 7,
          "page": 587,
          "ruku": 524,
          "hizbQuarter": 235,
          "sajda": false
        },
        {
          "number": 5835,
          "text": "يَٰٓأَيُّهَا ٱلْإِنسَٰنُ مَا غَرَّكَ بِرَبِّكَ ٱلْكَرِيمِ",
          "numberInSurah": 6,
          "juz": 30,
          "manzil": 7,
          "page": 587,
          "ruku": 524,
          "hizbQuarter": 235,
          "sajda": false
        },
        {
          "number": 5836,
          "text": "ٱلَّذِى خَلَقَكَ فَسَوَّىٰكَ فَعَدَلَكَ",
          "numberInSurah": 7,
          "juz": 30,
          "manzil": 7,
          "page": 587,
          "ruku": 524,
          "hizbQuarter": 235,
          "sajda": false
        },
        {
          "number": 5837,
          "text": "فِىٓ أَىِّ صُورَةٍۢ مَّا شَآءَ رَكَّبَكَ",
          "numberInSurah": 8,
          "juz": 30,
          "manzil": 7,
          "page": 587,
          "ruku": 524,
          "hizbQuarter": 235,
          "sajda": false
        },
        {
          "number": 5838,
          "text": "كَلَّا بَلْ تُكَذِّبُونَ بِٱلدِّينِ",
          "numberInSurah": 9,
          "juz": 30,
          "manzil": 7,
          "page": 587,
          "ruku": 524,
          "hizbQuarter": 235,
          "sajda": false
        },
        {
          "number": 5839,
          "text": "وَإِنَّ عَلَيْكُمْ لَحَٰفِظِينَ",
          "numberInSurah": 10,
          "juz": 30,
          "manzil": 7,
          "page": 587,
          "ruku": 524,
          "hizbQuarter": 235,
          "sajda": false
        },
        {
          "number": 5840,
          "text": "كِرَامًۭا كَٰتِبِينَ",
          "numberInSurah": 11,
          "juz": 30,
          "manzil": 7,
          "page": 587,
          "ruku": 524,
          "hizbQuarter": 235,
          "sajda": false
        },
        {
          "number": 5841,
          "text": "يَعْلَمُونَ مَا تَفْعَلُونَ",
          "numberInSurah": 12,
          "juz": 30,
          "manzil": 7,
          "page": 587,
          "ruku": 524,
          "hizbQuarter": 235,
          "sajda": false
        },
        {
          "number": 5842,
          "text": "إِنَّ ٱلْأَبْرَارَ لَفِى نَعِيمٍۢ",
          "numberInSurah": 13,
          "juz": 30,
          "manzil": 7,
          "page": 587,
          "ruku": 524,
          "hizbQuarter": 235,
          "sajda": false
        },
        {
          "number": 5843,
          "text": "وَإِنَّ ٱلْفُجَّارَ لَفِى جَحِيمٍۢ",
          "numberInSurah": 14,
          "juz": 30,
          "manzil": 7,
          "page": 587,
          "ruku": 524,
          "hizbQuarter": 235,
          "sajda": false
        },
        {
          "number": 5844,
          "text": "يَصْلَوْنَهَا يَوْمَ ٱلدِّينِ",
          "numberInSurah": 15,
          "juz": 30,
          "manzil": 7,
          "page": 587,
          "ruku": 524,
          "hizbQuarter": 235,
          "sajda": false
        },
        {
          "number": 5845,
          "text": "وَمَا هُمْ عَنْهَا بِغَآئِبِينَ",
          "numberInSurah": 16,
          "juz": 30,
          "manzil": 7,
          "page": 587,
          "ruku": 524,
          "hizbQuarter": 235,
          "sajda": false
        },
        {
          "number": 5846,
          "text": "وَمَآ أَدْرَىٰكَ مَا يَوْمُ ٱلدِّينِ",
          "numberInSurah": 17,
          "juz": 30,
          "manzil": 7,
          "page": 587,
          "ruku": 524,
          "hizbQuarter": 235,
          "sajda": false
        },
        {
          "number": 5847,
          "text": "ثُمَّ مَآ أَدْرَىٰكَ مَا يَوْمُ ٱلدِّينِ",
