          "ruku": 532,
          "hizbQuarter": 238,
          "sajda": false
        },
        {
          "number": 6030,
          "text": "أَيَحْسَبُ أَن لَّمْ يَرَهُۥٓ أَحَدٌ",
          "numberInSurah": 7,
          "juz": 30,
          "manzil": 7,
          "page": 594,
          "ruku": 532,
          "hizbQuarter": 238,
          "sajda": false
        },
        {
