  "data": [
    {
      "number": 15,
      "name": "سورة الحجر",
      "englishName": "Al-Hijr",
      "englishNameTranslation": "The Rock",
      "revelationType": "Meccan",
      "numberOfAyahs": 99,
      "ayahs": [
        {
          "number": 1803,
          "text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ الٓر ۚ تِلْكَ ءَايَٰتُ ٱلْكِتَٰبِ وَقُرْءَانٍۢ مُّبِينٍۢ",
          "numberInSurah": 1,
          "juz": 14,
          "manzil": 3,
          "page": 262,
          "ruku": 218,
          "hizbQuarter": 105,
          "sajda": false
        },
        {
          "number": 1804,
          "text": "رُّبَمَا يَوَدُّ ٱلَّذِينَ كَفَرُوا۟ لَوْ كَانُوا۟ مُسْلِمِينَ",
          "numberInSurah": 2,
          "juz": 14,
          "manzil": 3,
          "page": 262,
          "ruku": 218,
          "hizbQuarter": 105,
          "sajda": false
        },
        {
          "number": 1805,
          "text": "ذَرْهُمْ يَأْكُلُوا۟ وَيَتَمَتَّعُوا۟ وَيُلْهِهِمُ ٱلْأَمَلُ ۖ فَسَوْفَ يَعْلَمُونَ",
          "numberInSurah": 3,
          "juz": 14,
          "manzil": 3,
          "page": 262,
          "ruku": 218,
          "hizbQuarter": 105,
          "sajda": false
        },
        {
          "number": 1806,
          "text": "وَمَآ أَهْلَكْنَا مِن قَرْيَةٍ إِلَّا وَلَهَا كِتَابٌۭ مَّعْلُومٌۭ",
          "numberInSurah": 4,
          "juz": 14,
          "manzil": 3,
          "page": 262,
          "ruku": 218,
          "hizbQuarter": 105,
          "sajda": false
        },
        {
          "number": 1807,
          "text": "مَّا تَسْبِقُ مِنْ أُمَّةٍ أَجَلَهَا وَمَا يَسْتَـْٔخِرُونَ",
          "numberInSurah": 5,
          "juz": 14,
          "manzil": 3,
          "page": 262,
          "ruku": 218,
          "hizbQuarter": 105,
          "sajda": false
        },
        {
          "number": 1808,
          "text": "وَقَالُوا۟ يَٰٓأَيُّهَا ٱلَّذِى نُزِّلَ عَلَيْهِ ٱلذِّكْرُ إِنَّكَ لَمَجْنُونٌۭ",
          "numberInSurah": 6,
          "juz": 14,
          "manzil": 3,
          "page": 262,
          "ruku": 218,
          "hizbQuarter": 105,
          "sajda": false
        },
        {
          "number": 1809,
          "text": "لَّوْ مَا تَأْتِينَا بِٱلْمَلَٰٓئِكَةِ إِن كُنتَ مِنَ ٱلصَّٰدِقِينَ",
          "numberInSurah": 7,
          "juz": 14,
          "manzil": 3,
          "page": 262,
          "ruku": 218,
          "hizbQuarter": 105,
          "sajda": false
        },
        {
          "number": 1810,
          "text": "مَا نُنَزِّلُ ٱلْمَلَٰٓئِكَةَ إِلَّا بِٱلْحَقِّ وَمَا كَانُوٓا۟ إِذًۭا مُّنظَرِينَ",
          "numberInSurah": 8,
          "juz": 14,
          "manzil": 3,
          "page": 262,
          "ruku": 218,
          "hizbQuarter": 105,
          "sajda": false
        },
        {
          "number": 1811,
          "text": "إِنَّا نَحْنُ نَزَّلْنَا ٱلذِّكْرَ وَإِنَّا لَهُۥ لَحَٰفِظُونَ",
          "numberInSurah": 9,
          "juz": 14,
          "manzil": 3,
          "page": 262,
          "ruku": 218,
          "hizbQuarter": 105,
          "sajda": false
        },
        {
          "number": 1812,
          "text": "وَلَقَدْ أَرْسَلْنَا مِن قَبْلِكَ فِى شِيَعِ ٱلْأَوَّلِينَ",
          "numberInSurah": 10,
          "juz": 14,
          "manzil": 3,
          "page": 262,
          "ruku": 218,
          "hizbQuarter": 105,
          "sajda": false
        },
        {
          "number": 1813,
          "text": "وَمَا يَأْتِيهِم مِّن رَّسُولٍ إِلَّا كَانُوا۟ بِهِۦ يَسْتَهْزِءُونَ",
          "numberInSurah": 11,
          "juz": 14,
          "manzil": 3,
          "page": 262,
          "ruku": 218,
          "hizbQuarter": 105,
          "sajda": false
        },
        {
          "number": 1814,
          "text": "كَذَٰلِكَ نَسْلُكُهُۥ فِى قُلُوبِ ٱلْمُجْرِمِينَ",
          "numberInSurah": 12,
          "juz": 14,
          "manzil": 3,
          "page": 262,
          "ruku": 218,
          "hizbQuarter": 105,
          "sajda": false
        },
        {
          "number": 1815,
          "text": "لَا يُؤْمِنُونَ بِهِۦ ۖ وَقَدْ خَلَتْ سُنَّةُ ٱلْأَوَّلِينَ",
          "numberInSurah": 13,
          "juz": 14,
          "manzil": 3,
          "page": 262,
          "ruku": 218,
          "hizbQuarter": 105,
          "sajda": false
        },
        {
          "number": 1816,
          "text": "وَلَوْ فَتَحْنَا عَلَيْهِم بَابًۭا مِّنَ ٱلسَّمَآءِ فَظَلُّوا۟ فِيهِ يَعْرُجُونَ",
          "numberInSurah": 14,
          "juz": 14,
          "manzil": 3,
          "page": 262,
          "ruku": 218,
          "hizbQuarter": 105,
          "sajda": false
        },
        {
          "number": 1817,
          "text": "لَقَالُوٓا۟ إِنَّمَا سُكِّرَتْ أَبْصَٰرُنَا بَلْ نَحْنُ قَوْمٌۭ مَّسْحُورُونَ",
          "numberInSurah": 15,
          "juz": 14,
          "manzil": 3,
          "page": 262,
          "ruku": 218,
          "hizbQuarter": 105,
          "sajda": false
        },
        {
          "number": 1818,
          "text": "وَلَقَدْ جَعَلْنَا فِى ٱلسَّمَآءِ بُرُوجًۭا وَزَيَّنَّٰهَا لِلنَّٰظِرِينَ",
          "numberInSurah": 16,
          "juz": 14,
          "manzil": 3,
          "page": 263,
          "ruku": 219,
          "hizbQuarter": 105,
          "sajda": false
        },
        {
          "number": 1819,
          "text": "وَحَفِظْنَٰهَا مِن كُلِّ شَيْطَٰنٍۢ رَّجِيمٍ",
          "numberInSurah": 17,
          "juz": 14,
          "manzil": 3,
          "page": 263,
          "ruku": 219,
          "hizbQuarter": 105,
          "sajda": false
        },
        {
          "number": 1820,
          "text": "إِلَّا مَنِ ٱسْتَرَقَ ٱلسَّمْعَ فَأَتْبَعَهُۥ شِهَابٌۭ مُّبِينٌۭ",
          "numberInSurah": 18,
          "juz": 14,
          "manzil": 3,
          "page": 263,
          "ruku": 219,
          "hizbQuarter": 105,
          "sajda": false
        },
        {
          "number": 1821,
          "text": "وَٱلْأَرْضَ مَدَدْنَٰهَا وَأَلْقَيْنَا فِيهَا رَوَٰسِىَ وَأَنۢبَتْنَا فِيهَا مِن كُلِّ شَىْءٍۢ مَّوْزُونٍۢ",
          "numberInSurah": 19,
          "juz": 14,
          "manzil": 3,
          "page": 263,
          "ruku": 219,
          "hizbQuarter": 105,
          "sajda": false
        },
        {
          "number": 1822,
          "text": "وَجَعَلْنَا لَكُمْ فِيهَا مَعَٰيِشَ وَمَن لَّسْتُمْ لَهُۥ بِرَٰزِقِينَ",
          "numberInSurah": 20,
          "juz": 14,
          "manzil": 3,
          "page": 263,
          "ruku": 219,
          "hizbQuarter": 105,
          "sajda": false
        },
        {
          "number": 1823,
          "text": "وَإِن مِّن شَىْءٍ إِلَّا عِندَنَا خَزَآئِنُهُۥ وَمَا نُنَزِّلُهُۥٓ إِلَّا بِقَدَرٍۢ مَّعْلُومٍۢ",
          "numberInSurah": 21,
          "juz": 14,
          "manzil": 3,
          "page": 263,
          "ruku": 219,
          "hizbQuarter": 105,
          "sajda": false
        },
        {
          "number": 1824,
          "text": "وَأَرْسَلْنَا ٱلرِّيَٰحَ لَوَٰقِحَ فَأَنزَلْنَا مِنَ ٱلسَّمَآءِ مَآءًۭ فَأَسْقَيْنَٰكُمُوهُ وَمَآ أَنتُمْ لَهُۥ بِخَٰزِنِينَ",
          "numberInSurah": 22,
          "juz": 14,
          "manzil": 3,
          "page": 263,
          "ruku": 219,
          "hizbQuarter": 105,
          "sajda": false
        },
        {
          "number": 1825,
          "text": "وَإِنَّا لَنَحْنُ نُحْىِۦ وَنُمِيتُ وَنَحْنُ ٱلْوَٰرِثُونَ",
          "numberInSurah": 23,
          "juz": 14,
          "manzil": 3,
          "page": 263,
          "ruku": 219,
          "hizbQuarter": 105,
          "sajda": false
        },
        {
          "number": 1826,
          "text": "وَلَقَدْ عَلِمْنَا ٱلْمُسْتَقْدِمِينَ مِنكُمْ وَلَقَدْ عَلِمْنَا ٱلْمُسْتَـْٔخِرِينَ",
          "numberInSurah": 24,
          "juz": 14,
          "manzil": 3,
          "page": 263,
          "ruku": 219,
          "hizbQuarter": 105,
          "sajda": false
        },
        {
          "number": 1827,
          "text": "وَإِنَّ رَبَّكَ هُوَ يَحْشُرُهُمْ ۚ إِنَّهُۥ حَكِيمٌ عَلِيمٌۭ",
          "numberInSurah": 25,
          "juz": 14,
          "manzil": 3,
          "page": 263,
          "ruku": 219,
          "hizbQuarter": 105,
          "sajda": false
        },
        {
          "number": 1828,
          "text": "وَلَقَدْ خَلَقْنَا ٱلْإِنسَٰنَ مِن صَلْصَٰلٍۢ مِّنْ حَمَإٍۢ مَّسْنُونٍۢ",
          "numberInSurah": 26,
          "juz": 14,
          "manzil": 3,
          "page": 263,
          "ruku": 220,
          "hizbQuarter": 105,
          "sajda": false
        },
        {
          "number": 1829,
          "text": "وَٱلْجَآنَّ خَلَقْنَٰهُ مِن قَبْلُ مِن نَّارِ ٱلسَّمُومِ",
          "numberInSurah": 27,
          "juz": 14,
          "manzil": 3,
          "page": 263,
          "ruku": 220,
          "hizbQuarter": 105,
          "sajda": false
        },
        {
          "number": 1830,
          "text": "وَإِذْ قَالَ رَبُّكَ لِلْمَلَٰٓئِكَةِ إِنِّى خَٰلِقٌۢ بَشَرًۭا مِّن صَلْصَٰلٍۢ مِّنْ حَمَإٍۢ مَّسْنُونٍۢ",
          "numberInSurah": 28,
          "juz": 14,
          "manzil": 3,
          "page": 263,
          "ruku": 220,
          "hizbQuarter": 105,
          "sajda": false
        },
        {
          "number": 1831,
          "text": "فَإِذَا سَوَّيْتُهُۥ وَنَفَخْتُ فِيهِ مِن رُّوحِى فَقَعُوا۟ لَهُۥ سَٰجِدِينَ",
          "numberInSurah": 29,
          "juz": 14,
          "manzil": 3,
          "page": 263,
          "ruku": 220,
          "hizbQuarter": 105,
          "sajda": false
        },
        {
          "number": 1832,
          "text": "فَسَجَدَ ٱلْمَلَٰٓئِكَةُ كُلُّهُمْ أَجْمَعُونَ",
          "numberInSurah": 30,
          "juz": 14,
          "manzil": 3,
          "page": 263,
          "ruku": 220,
          "hizbQuarter": 105,
          "sajda": false
        },
        {
          "number": 1833,
          "text": "إِلَّآ إِبْلِيسَ أَبَىٰٓ أَن يَكُونَ مَعَ ٱلسَّٰجِدِينَ",
          "numberInSurah": 31,
          "juz": 14,
          "manzil": 3,
          "page": 263,
          "ruku": 220,
          "hizbQuarter": 105,
          "sajda": false
        },
        {
          "number": 1834,
          "text": "قَالَ يَٰٓإِبْلِيسُ مَا لَكَ أَلَّا تَكُونَ مَعَ ٱلسَّٰجِدِينَ",
          "numberInSurah": 32,
          "juz": 14,
          "manzil": 3,
          "page": 264,
          "ruku": 220,
          "hizbQuarter": 105,
          "sajda": false
        },
        {
          "number": 1835,
          "text": "قَالَ لَمْ أَكُن لِّأَسْجُدَ لِبَشَرٍ خَلَقْتَهُۥ مِن صَلْصَٰلٍۢ مِّنْ حَمَإٍۢ مَّسْنُونٍۢ",
          "numberInSurah": 33,
          "juz": 14,
          "manzil": 3,
          "page": 264,
          "ruku": 220,
          "hizbQuarter": 105,
          "sajda": false
        },
        {
          "number": 1836,
          "text": "قَالَ فَٱخْرُجْ مِنْهَا فَإِنَّكَ رَجِيمٌۭ",
          "numberInSurah": 34,
          "juz": 14,
          "manzil": 3,
          "page": 264,
          "ruku": 220,
          "hizbQuarter": 105,
          "sajda": false
        },
        {
          "number": 1837,
          "text": "وَإِنَّ عَلَيْكَ ٱللَّعْنَةَ إِلَىٰ يَوْمِ ٱلدِّينِ",
          "numberInSurah": 35,
          "juz": 14,
          "manzil": 3,
          "page": 264,
          "ruku": 220,
          "hizbQuarter": 105,
          "sajda": false
        },
        {
          "number": 1838,
          "text": "قَالَ رَبِّ فَأَنظِرْنِىٓ إِلَىٰ يَوْمِ يُبْعَثُونَ",
          "numberInSurah": 36,
          "juz": 14,
          "manzil": 3,
          "page": 264,
          "ruku": 220,
          "hizbQuarter": 105,
          "sajda": false
        },
        {
          "number": 1839,
          "text": "قَالَ فَإِنَّكَ مِنَ ٱلْمُنظَرِينَ",
          "numberInSurah": 37,
          "juz": 14,
          "manzil": 3,
          "page": 264,
          "ruku": 220,
          "hizbQuarter": 105,
          "sajda": false
        },
        {
          "number": 1840,
          "text": "إِلَىٰ يَوْمِ ٱلْوَقْتِ ٱلْمَعْلُومِ",
          "numberInSurah": 38,
          "juz": 14,
          "manzil": 3,
          "page": 264,
          "ruku": 220,
          "hizbQuarter": 105,
          "sajda": false
        },
        {
          "number": 1841,
          "text": "قَالَ رَبِّ بِمَآ أَغْوَيْتَنِى لَأُزَيِّنَنَّ لَهُمْ فِى ٱلْأَرْضِ وَلَأُغْوِيَنَّهُمْ أَجْمَعِينَ",
          "numberInSurah": 39,
          "juz": 14,
          "manzil": 3,
          "page": 264,
          "ruku": 220,
          "hizbQuarter": 105,
          "sajda": false
        },
        {
          "number": 1842,
          "text": "إِلَّا عِبَادَكَ مِنْهُمُ ٱلْمُخْلَصِينَ",
          "numberInSurah": 40,
          "juz": 14,
          "manzil": 3,
          "page": 264,
          "ruku": 220,
          "hizbQuarter": 105,
          "sajda": false
        },
        {
          "number": 1843,
          "text": "قَالَ هَٰذَا صِرَٰطٌ عَلَىَّ مُسْتَقِيمٌ",
          "numberInSurah": 41,
          "juz": 14,
          "manzil": 3,
          "page": 264,
          "ruku": 220,
          "hizbQuarter": 105,
          "sajda": false
        },
        {
          "number": 1844,
          "text": "إِنَّ عِبَادِى لَيْسَ لَكَ عَلَيْهِمْ سُلْطَٰنٌ إِلَّا مَنِ ٱتَّبَعَكَ مِنَ ٱلْغَاوِينَ",
          "numberInSurah": 42,
          "juz": 14,
          "manzil": 3,
          "page": 264,
          "ruku": 220,
          "hizbQuarter": 105,
          "sajda": false
        },
        {
          "number": 1845,
          "text": "وَإِنَّ جَهَنَّمَ لَمَوْعِدُهُمْ أَجْمَعِينَ",
          "numberInSurah": 43,
          "juz": 14,
          "manzil": 3,
          "page": 264,
          "ruku": 220,
          "hizbQuarter": 105,
          "sajda": false
        },
        {
          "number": 1846,
          "text": "لَهَا سَبْعَةُ أَبْوَٰبٍۢ لِّكُلِّ بَابٍۢ مِّنْهُمْ جُزْءٌۭ مَّقْسُومٌ",
          "numberInSurah": 44,
          "juz": 14,
          "manzil": 3,
          "page": 264,
          "ruku": 220,
          "hizbQuarter": 105,
          "sajda": false
        },
        {
          "number": 1847,
          "text": "إِنَّ ٱلْمُتَّقِينَ فِى جَنَّٰتٍۢ وَعُيُونٍ",
          "numberInSurah": 45,
          "juz": 14,
          "manzil": 3,
          "page": 264,
          "ruku": 221,
          "hizbQuarter": 105,
          "sajda": false
        },
        {
          "number": 1848,
          "text": "ٱدْخُلُوهَا بِسَلَٰمٍ ءَامِنِينَ",
          "numberInSurah": 46,
          "juz": 14,
          "manzil": 3,
          "page": 264,
          "ruku": 221,
          "hizbQuarter": 105,
          "sajda": false
        },
        {
          "number": 1849,
          "text": "وَنَزَعْنَا مَا فِى صُدُورِهِم مِّنْ غِلٍّ إِخْوَٰنًا عَلَىٰ سُرُرٍۢ مُّتَقَٰبِلِينَ",
          "numberInSurah": 47,
          "juz": 14,
          "manzil": 3,
          "page": 264,
          "ruku": 221,
          "hizbQuarter": 105,
          "sajda": false
        },
        {
          "number": 1850,
          "text": "لَا يَمَسُّهُمْ فِيهَا نَصَبٌۭ وَمَا هُم مِّنْهَا بِمُخْرَجِينَ",
          "numberInSurah": 48,
          "juz": 14,
          "manzil": 3,
          "page": 264,
          "ruku": 221,
          "hizbQuarter": 105,
          "sajda": false
        },
        {
          "number": 1851,
          "text": "۞ نَبِّئْ عِبَادِىٓ أَنِّىٓ أَنَا ٱلْغَفُورُ ٱلرَّحِيمُ",
          "numberInSurah": 49,
          "juz": 14,
          "manzil": 3,
          "page": 264,
          "ruku": 221,
          "hizbQuarter": 105,
          "sajda": false
        },
        {
          "number": 1852,
          "text": "وَأَنَّ عَذَابِى هُوَ ٱلْعَذَابُ ٱلْأَلِيمُ",
          "numberInSurah": 50,
          "juz": 14,
          "manzil": 3,
          "page": 264,
          "ruku": 221,
          "hizbQuarter": 106,
          "sajda": false
        },
        {
          "number": 1853,
          "text": "وَنَبِّئْهُمْ عَن ضَيْفِ إِبْرَٰهِيمَ",
          "numberInSurah": 51,
          "juz": 14,
          "manzil": 3,
          "page": 264,
          "ruku": 221,
          "hizbQuarter": 106,
          "sajda": false
        },
        {
          "number": 1854,
          "text": "إِذْ دَخَلُوا۟ عَلَيْهِ فَقَالُوا۟ سَلَٰمًۭا قَالَ إِنَّا مِنكُمْ وَجِلُونَ",
          "numberInSurah": 52,
          "juz": 14,
          "manzil": 3,
          "page": 265,
          "ruku": 221,
          "hizbQuarter": 106,
          "sajda": false
        },
        {
          "number": 1855,
          "text": "قَالُوا۟ لَا تَوْجَلْ إِنَّا نُبَشِّرُكَ بِغُلَٰمٍ عَلِيمٍۢ",
          "numberInSurah": 53,
          "juz": 14,
          "manzil": 3,
          "page": 265,
          "ruku": 221,
          "hizbQuarter": 106,
          "sajda": false
        },
        {
          "number": 1856,
          "text": "قَالَ أَبَشَّرْتُمُونِى عَلَىٰٓ أَن مَّسَّنِىَ ٱلْكِبَرُ فَبِمَ تُبَشِّرُونَ",
          "numberInSurah": 54,
          "juz": 14,
          "manzil": 3,
          "page": 265,
          "ruku": 221,
          "hizbQuarter": 106,
          "sajda": false
        },
        {
          "number": 1857,
          "text": "قَالُوا۟ بَشَّرْنَٰكَ بِٱلْحَقِّ فَلَا تَكُن مِّنَ ٱلْقَٰنِطِينَ",
          "numberInSurah": 55,
          "juz": 14,
          "manzil": 3,
          "page": 265,
          "ruku": 221,
          "hizbQuarter": 106,
          "sajda": false
        },
        {
          "number": 1858,
          "text": "قَالَ وَمَن يَقْنَطُ مِن رَّحْمَةِ رَبِّهِۦٓ إِلَّا ٱلضَّآلُّونَ",
          "numberInSurah": 56,
          "juz": 14,
          "manzil": 3,
          "page": 265,
          "ruku": 221,
          "hizbQuarter": 106,
          "sajda": false
        },
        {
          "number": 1859,
          "text": "قَالَ فَمَا خَطْبُكُمْ أَيُّهَا ٱلْمُرْسَلُونَ",
          "numberInSurah": 57,
          "juz": 14,
          "manzil": 3,
          "page": 265,
          "ruku": 221,
          "hizbQuarter": 106,
          "sajda": false
        },
        {
          "number": 1860,
          "text": "قَالُوٓا۟ إِنَّآ أُرْسِلْنَآ إِلَىٰ قَوْمٍۢ مُّجْرِمِينَ",
          "numberInSurah": 58,
          "juz": 14,
          "manzil": 3,
          "page": 265,
          "ruku": 221,
          "hizbQuarter": 106,
          "sajda": false
        },
        {
          "number": 1861,
          "text": "إِلَّآ ءَالَ لُوطٍ إِنَّا لَمُنَجُّوهُمْ أَجْمَعِينَ",
          "numberInSurah": 59,
          "juz": 14,
          "manzil": 3,
          "page": 265,
          "ruku": 221,
          "hizbQuarter": 106,
          "sajda": false
        },
        {
          "number": 1862,
          "text": "إِلَّا ٱمْرَأَتَهُۥ قَدَّرْنَآ ۙ إِنَّهَا لَمِنَ ٱلْغَٰبِرِينَ",
          "numberInSurah": 60,
          "juz": 14,
          "manzil": 3,
          "page": 265,
          "ruku": 221,
          "hizbQuarter": 106,
          "sajda": false
        },
        {
          "number": 1863,
          "text": "فَلَمَّا جَآءَ ءَالَ لُوطٍ ٱلْمُرْسَلُونَ",
          "numberInSurah": 61,
          "juz": 14,
          "manzil": 3,
          "page": 265,
          "ruku": 222,
          "hizbQuarter": 106,
          "sajda": false
        },
        {
          "number": 1864,
          "text": "قَالَ إِنَّكُمْ قَوْمٌۭ مُّنكَرُونَ",
          "numberInSurah": 62,
          "juz": 14,
          "manzil": 3,
          "page": 265,
          "ruku": 222,
          "hizbQuarter": 106,
          "sajda": false
        },
        {
          "number": 1865,
          "text": "قَالُوا۟ بَلْ جِئْنَٰكَ بِمَا كَانُوا۟ فِيهِ يَمْتَرُونَ",
          "numberInSurah": 63,
          "juz": 14,
          "manzil": 3,
          "page": 265,
          "ruku": 222,
          "hizbQuarter": 106,
          "sajda": false
        },
        {
          "number": 1866,
          "text": "وَأَتَيْنَٰكَ بِٱلْحَقِّ وَإِنَّا لَصَٰدِقُونَ",
          "numberInSurah": 64,
          "juz": 14,
          "manzil": 3,
          "page": 265,
          "ruku": 222,
          "hizbQuarter": 106,
          "sajda": false
        },
        {
          "number": 1867,
          "text": "فَأَسْرِ بِأَهْلِكَ بِقِطْعٍۢ مِّنَ ٱلَّيْلِ وَٱتَّبِعْ أَدْبَٰرَهُمْ وَلَا يَلْتَفِتْ مِنكُمْ أَحَدٌۭ وَٱمْضُوا۟ حَيْثُ تُؤْمَرُونَ",
          "numberInSurah": 65,
          "juz": 14,
          "manzil": 3,
          "page": 265,
          "ruku": 222,
          "hizbQuarter": 106,
          "sajda": false
        },
        {
          "number": 1868,
          "text": "وَقَضَيْنَآ إِلَيْهِ ذَٰلِكَ ٱلْأَمْرَ أَنَّ دَابِرَ هَٰٓؤُلَآءِ مَقْطُوعٌۭ مُّصْبِحِينَ",
          "numberInSurah": 66,
          "juz": 14,
          "manzil": 3,
          "page": 265,
          "ruku": 222,
          "hizbQuarter": 106,
          "sajda": false
        },
        {
          "number": 1869,
          "text": "وَجَآءَ أَهْلُ ٱلْمَدِينَةِ يَسْتَبْشِرُونَ",
          "numberInSurah": 67,
          "juz": 14,
          "manzil": 3,
          "page": 265,
          "ruku": 222,
          "hizbQuarter": 106,
          "sajda": false
        },
        {
          "number": 1870,
          "text": "قَالَ إِنَّ هَٰٓؤُلَآءِ ضَيْفِى فَلَا تَفْضَحُونِ",
          "numberInSurah": 68,
          "juz": 14,
          "manzil": 3,
          "page": 265,
          "ruku": 222,
          "hizbQuarter": 106,
          "sajda": false
        },
        {
          "number": 1871,
          "text": "وَٱتَّقُوا۟ ٱللَّهَ وَلَا تُخْزُونِ",
          "numberInSurah": 69,
          "juz": 14,
          "manzil": 3,
          "page": 265,
          "ruku": 222,
          "hizbQuarter": 106,
          "sajda": false
        },
        {
          "number": 1872,
          "text": "قَالُوٓا۟ أَوَلَمْ نَنْهَكَ عَنِ ٱلْعَٰلَمِينَ",
          "numberInSurah": 70,
          "juz": 14,
          "manzil": 3,
          "page": 265,
          "ruku": 222,
          "hizbQuarter": 106,
          "sajda": false
        },
        {
          "number": 1873,
          "text": "قَالَ هَٰٓؤُلَآءِ بَنَاتِىٓ إِن كُنتُمْ فَٰعِلِينَ",
          "numberInSurah": 71,
          "juz": 14,
          "manzil": 3,
          "page": 266,
          "ruku": 222,
          "hizbQuarter": 106,
          "sajda": false
        },
        {
          "number": 1874,
          "text": "لَعَمْرُكَ إِنَّهُمْ لَفِى سَكْرَتِهِمْ يَعْمَهُونَ",
          "numberInSurah": 72,
          "juz": 14,
          "manzil": 3,
          "page": 266,
          "ruku": 222,
          "hizbQuarter": 106,
          "sajda": false
        },
        {
          "number": 1875,
          "text": "فَأَخَذَتْهُمُ ٱلصَّيْحَةُ مُشْرِقِينَ",
          "numberInSurah": 73,
          "juz": 14,
          "manzil": 3,
          "page": 266,
          "ruku": 222,
          "hizbQuarter": 106,
          "sajda": false
        },
        {
          "number": 1876,
          "text": "فَجَعَلْنَا عَٰلِيَهَا سَافِلَهَا وَأَمْطَرْنَا عَلَيْهِمْ حِجَارَةًۭ مِّن سِجِّيلٍ",
          "numberInSurah": 74,
          "juz": 14,
          "manzil": 3,
          "page": 266,
          "ruku": 222,
          "hizbQuarter": 106,
          "sajda": false
        },
        {
          "number": 1877,
          "text": "إِنَّ فِى ذَٰلِكَ لَءَايَٰتٍۢ لِّلْمُتَوَسِّمِينَ",
          "numberInSurah": 75,
          "juz": 14,
          "manzil": 3,
          "page": 266,
          "ruku": 222,
          "hizbQuarter": 106,
          "sajda": false
        },
        {
          "number": 1878,
          "text": "وَإِنَّهَا لَبِسَبِيلٍۢ مُّقِيمٍ",
          "numberInSurah": 76,
          "juz": 14,
          "manzil": 3,
          "page": 266,
          "ruku": 222,
          "hizbQuarter": 106,
          "sajda": false
        },
        {
          "number": 1879,
          "text": "إِنَّ فِى ذَٰلِكَ لَءَايَةًۭ لِّلْمُؤْمِنِينَ",
          "numberInSurah": 77,
          "juz": 14,
          "manzil": 3,
          "page": 266,
          "ruku": 222,
          "hizbQuarter": 106,
          "sajda": false
        },
        {
          "number": 1880,
          "text": "وَإِن كَانَ أَصْحَٰبُ ٱلْأَيْكَةِ لَظَٰلِمِينَ",
          "numberInSurah": 78,
          "juz": 14,
          "manzil": 3,
          "page": 266,
          "ruku": 222,
          "hizbQuarter": 106,
          "sajda": false
        },
        {
          "number": 1881,
          "text": "فَٱنتَقَمْنَا مِنْهُمْ وَإِنَّهُمَا لَبِإِمَامٍۢ مُّبِينٍۢ",
          "numberInSurah": 79,
          "juz": 14,
          "manzil": 3,
          "page": 266,
          "ruku": 222,
          "hizbQuarter": 106,
          "sajda": false
        },
        {
          "number": 1882,
          "text": "وَلَقَدْ كَذَّبَ أَصْحَٰبُ ٱلْحِجْرِ ٱلْمُرْسَلِينَ",
          "numberInSurah": 80,
          "juz": 14,
          "manzil": 3,
          "page": 266,
          "ruku": 223,
          "hizbQuarter": 106,
          "sajda": false
        },
        {
          "number": 1883,
          "text": "وَءَاتَيْنَٰهُمْ ءَايَٰتِنَا فَكَانُوا۟ عَنْهَا مُعْرِضِينَ",
          "numberInSurah": 81,
          "juz": 14,
          "manzil": 3,
          "page": 266,
          "ruku": 223,
          "hizbQuarter": 106,
          "sajda": false
        },
        {
          "number": 1884,
          "text": "وَكَانُوا۟ يَنْحِتُونَ مِنَ ٱلْجِبَالِ بُيُوتًا ءَامِنِينَ",
          "numberInSurah": 82,
          "juz": 14,
          "manzil": 3,
          "page": 266,
          "ruku": 223,
          "hizbQuarter": 106,
          "sajda": false
        },
        {
          "number": 1885,
          "text": "فَأَخَذَتْهُمُ ٱلصَّيْحَةُ مُصْبِحِينَ",
          "numberInSurah": 83,
          "juz": 14,
          "manzil": 3,
          "page": 266,
          "ruku": 223,
          "hizbQuarter": 106,
          "sajda": false
        },
        {
          "number": 1886,
          "text": "فَمَآ أَغْنَىٰ عَنْهُم مَّا كَانُوا۟ يَكْسِبُونَ",
          "numberInSurah": 84,
          "juz": 14,
          "manzil": 3,
          "page": 266,
          "ruku": 223,
          "hizbQuarter": 106,
          "sajda": false
        },
        {
          "number": 1887,
          "text": "وَمَا خَلَقْنَا ٱلسَّمَٰوَٰتِ وَٱلْأَرْضَ وَمَا بَيْنَهُمَآ إِلَّا بِٱلْحَقِّ ۗ وَإِنَّ ٱلسَّاعَةَ لَءَاتِيَةٌۭ ۖ فَٱصْفَحِ ٱلصَّفْحَ ٱلْجَمِيلَ",
          "numberInSurah": 85,
          "juz": 14,
          "manzil": 3,
          "page": 266,
          "ruku": 223,
          "hizbQuarter": 106,
          "sajda": false
        },
        {
          "number": 1888,
          "text": "إِنَّ رَبَّكَ هُوَ ٱلْخَلَّٰقُ ٱلْعَلِيمُ",
          "numberInSurah": 86,
          "juz": 14,
          "manzil": 3,
          "page": 266,
          "ruku": 223,
          "hizbQuarter": 106,
          "sajda": false
        },
        {
          "number": 1889,
          "text": "وَلَقَدْ ءَاتَيْنَٰكَ سَبْعًۭا مِّنَ ٱلْمَثَانِى وَٱلْقُرْءَانَ ٱلْعَظِيمَ",
          "numberInSurah": 87,
          "juz": 14,
          "manzil": 3,
          "page": 266,
          "ruku": 223,
          "hizbQuarter": 106,
          "sajda": false
        },
        {
          "number": 1890,
          "text": "لَا تَمُدَّنَّ عَيْنَيْكَ إِلَىٰ مَا مَتَّعْنَا بِهِۦٓ أَزْوَٰجًۭا مِّنْهُمْ وَلَا تَحْزَنْ عَلَيْهِمْ وَٱخْفِضْ جَنَاحَكَ لِلْمُؤْمِنِينَ",
          "numberInSurah": 88,
          "juz": 14,
          "manzil": 3,
          "page": 266,
          "ruku": 223,
          "hizbQuarter": 106,
          "sajda": false
        },
        {
          "number": 1891,
          "text": "وَقُلْ إِنِّىٓ أَنَا ٱلنَّذِيرُ ٱلْمُبِينُ",
          "numberInSurah": 89,
          "juz": 14,
          "manzil": 3,
          "page": 266,
          "ruku": 223,
          "hizbQuarter": 106,
          "sajda": false
        },
        {
          "number": 1892,
          "text": "كَمَآ أَنزَلْنَا عَلَى ٱلْمُقْتَسِمِينَ",
          "numberInSurah": 90,
          "juz": 14,
          "manzil": 3,
          "page": 266,
          "ruku": 223,
          "hizbQuarter": 106,
          "sajda": false
        },
        {
          "number": 1893,
          "text": "ٱلَّذِينَ جَعَلُوا۟ ٱلْقُرْءَانَ عِضِينَ",
          "numberInSurah": 91,
          "juz": 14,
          "manzil": 3,
          "page": 267,
          "ruku": 223,
          "hizbQuarter": 106,
          "sajda": false
        },
        {
          "number": 1894,
          "text": "فَوَرَبِّكَ لَنَسْـَٔلَنَّهُمْ أَجْمَعِينَ",
          "numberInSurah": 92,
          "juz": 14,
          "manzil": 3,
          "page": 267,
          "ruku": 223,
          "hizbQuarter": 106,
          "sajda": false
        },
        {
          "number": 1895,
          "text": "عَمَّا كَانُوا۟ يَعْمَلُونَ",
          "numberInSurah": 93,
          "juz": 14,
          "manzil": 3,
          "page": 267,
          "ruku": 223,
          "hizbQuarter": 106,
          "sajda": false
        },
        {
          "number": 1896,
          "text": "فَٱصْدَعْ بِمَا تُؤْمَرُ وَأَعْرِضْ عَنِ ٱلْمُشْرِكِينَ",
          "numberInSurah": 94,
          "juz": 14,
          "manzil": 3,
          "page": 267,
          "ruku": 223,
          "hizbQuarter": 106,
          "sajda": false
        },
        {
          "number": 1897,
          "text": "إِنَّا كَفَيْنَٰكَ ٱلْمُسْتَهْزِءِينَ",
          "numberInSurah": 95,
          "juz": 14,
          "manzil": 3,
          "page": 267,
          "ruku": 223,
          "hizbQuarter": 106,
          "sajda": false
        },
        {
          "number": 1898,
          "text": "ٱلَّذِينَ يَجْعَلُونَ مَعَ ٱللَّهِ إِلَٰهًا ءَاخَرَ ۚ فَسَوْفَ يَعْلَمُونَ",
          "numberInSurah": 96,
          "juz": 14,
          "manzil": 3,
          "page": 267,
          "ruku": 223,
          "hizbQuarter": 106,
          "sajda": false
        },
        {
          "number": 1899,
          "text": "وَلَقَدْ نَعْلَمُ أَنَّكَ يَضِيقُ صَدْرُكَ بِمَا يَقُولُونَ",
          "numberInSurah": 97,
          "juz": 14,
          "manzil": 3,
          "page": 267,
          "ruku": 223,
          "hizbQuarter": 106,
          "sajda": false
        },
        {
          "number": 1900,
          "text": "فَسَبِّحْ بِحَمْدِ رَبِّكَ وَكُن مِّنَ ٱلسَّٰجِدِينَ",
          "numberInSurah": 98,
          "juz": 14,
          "manzil": 3,
          "page": 267,
          "ruku": 223,
          "hizbQuarter": 106,
          "sajda": false
        },
