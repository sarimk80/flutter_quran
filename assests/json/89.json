    {
      "number": 89,
      "name": "سورة الفجر",
      "englishName": "Al-Fajr",
      "englishNameTranslation": "The Dawn",
      "revelationType": "Meccan",
      "numberOfAyahs": 30,
      "ayahs": [
        {
          "number": 5994,
          "text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ وَٱلْفَجْرِ",
          "numberInSurah": 1,
          "juz": 30,
          "manzil": 7,
          "page": 593,
          "ruku": 531,
          "hizbQuarter": 237,
          "sajda": false
        },
        {
          "number": 5995,
          "text": "وَلَيَالٍ عَشْرٍۢ",
          "numberInSurah": 2,
          "juz": 30,
          "manzil": 7,
          "page": 593,
          "ruku": 531,
          "hizbQuarter": 237,
          "sajda": false
        },
        {
          "number": 5996,
          "text": "وَٱلشَّفْعِ وَٱلْوَتْرِ",
          "numberInSurah": 3,
          "juz": 30,
          "manzil": 7,
          "page": 593,
          "ruku": 531,
          "hizbQuarter": 237,
          "sajda": false
        },
        {
          "number": 5997,
          "text": "وَٱلَّيْلِ إِذَا يَسْرِ",
          "numberInSurah": 4,
          "juz": 30,
          "manzil": 7,
          "page": 593,
          "ruku": 531,
          "hizbQuarter": 237,
          "sajda": false
        },
        {
          "number": 5998,
          "text": "هَلْ فِى ذَٰلِكَ قَسَمٌۭ لِّذِى حِجْرٍ",
          "numberInSurah": 5,
          "juz": 30,
          "manzil": 7,
          "page": 593,
          "ruku": 531,
          "hizbQuarter": 237,
          "sajda": false
        },
        {
          "number": 5999,
          "text": "أَلَمْ تَرَ كَيْفَ فَعَلَ رَبُّكَ بِعَادٍ",
          "numberInSurah": 6,
          "juz": 30,
          "manzil": 7,
          "page": 593,
          "ruku": 531,
          "hizbQuarter": 237,
          "sajda": false
        },
        {
          "number": 6000,
          "text": "إِرَمَ ذَاتِ ٱلْعِمَادِ",
          "numberInSurah": 7,
          "juz": 30,
          "manzil": 7,
          "page": 593,
          "ruku": 531,
          "hizbQuarter": 237,
          "sajda": false
        },
        {
          "number": 6001,
          "text": "ٱلَّتِى لَمْ يُخْلَقْ مِثْلُهَا فِى ٱلْبِلَٰدِ",
          "numberInSurah": 8,
          "juz": 30,
          "manzil": 7,
          "page": 593,
          "ruku": 531,
          "hizbQuarter": 237,
          "sajda": false
        },
        {
          "number": 6002,
          "text": "وَثَمُودَ ٱلَّذِينَ جَابُوا۟ ٱلصَّخْرَ بِٱلْوَادِ",
          "numberInSurah": 9,
          "juz": 30,
          "manzil": 7,
          "page": 593,
          "ruku": 531,
          "hizbQuarter": 237,
          "sajda": false
        },
        {
          "number": 6003,
          "text": "وَفِرْعَوْنَ ذِى ٱلْأَوْتَادِ",
          "numberInSurah": 10,
          "juz": 30,
          "manzil": 7,
          "page": 593,
          "ruku": 531,
          "hizbQuarter": 237,
          "sajda": false
        },
        {
          "number": 6004,
          "text": "ٱلَّذِينَ طَغَوْا۟ فِى ٱلْبِلَٰدِ",
          "numberInSurah": 11,
          "juz": 30,
          "manzil": 7,
          "page": 593,
          "ruku": 531,
          "hizbQuarter": 237,
          "sajda": false
        },
        {
          "number": 6005,
          "text": "فَأَكْثَرُوا۟ فِيهَا ٱلْفَسَادَ",
          "numberInSurah": 12,
          "juz": 30,
          "manzil": 7,
          "page": 593,
          "ruku": 531,
          "hizbQuarter": 237,
          "sajda": false
        },
        {
          "number": 6006,
          "text": "فَصَبَّ عَلَيْهِمْ رَبُّكَ سَوْطَ عَذَابٍ",
          "numberInSurah": 13,
          "juz": 30,
          "manzil": 7,
          "page": 593,
          "ruku": 531,
          "hizbQuarter": 237,
          "sajda": false
        },
        {
          "number": 6007,
          "text": "إِنَّ رَبَّكَ لَبِٱلْمِرْصَادِ",
          "numberInSurah": 14,
          "juz": 30,
          "manzil": 7,
          "page": 593,
          "ruku": 531,
          "hizbQuarter": 237,
          "sajda": false
        },
        {
          "number": 6008,
          "text": "فَأَمَّا ٱلْإِنسَٰنُ إِذَا مَا ٱبْتَلَىٰهُ رَبُّهُۥ فَأَكْرَمَهُۥ وَنَعَّمَهُۥ فَيَقُولُ رَبِّىٓ أَكْرَمَنِ",
          "numberInSurah": 15,
          "juz": 30,
          "manzil": 7,
          "page": 593,
          "ruku": 531,
          "hizbQuarter": 237,
          "sajda": false
        },
        {
          "number": 6009,
          "text": "وَأَمَّآ إِذَا مَا ٱبْتَلَىٰهُ فَقَدَرَ عَلَيْهِ رِزْقَهُۥ فَيَقُولُ رَبِّىٓ أَهَٰنَنِ",
          "numberInSurah": 16,
          "juz": 30,
          "manzil": 7,
          "page": 593,
          "ruku": 531,
          "hizbQuarter": 237,
          "sajda": false
        },
        {
          "number": 6010,
          "text": "كَلَّا ۖ بَل لَّا تُكْرِمُونَ ٱلْيَتِيمَ",
          "numberInSurah": 17,
          "juz": 30,
          "manzil": 7,
          "page": 593,
          "ruku": 531,
          "hizbQuarter": 237,
          "sajda": false
        },
        {
          "number": 6011,
          "text": "وَلَا تَحَٰٓضُّونَ عَلَىٰ طَعَامِ ٱلْمِسْكِينِ",
          "numberInSurah": 18,
          "juz": 30,
          "manzil": 7,
          "page": 593,
          "ruku": 531,
          "hizbQuarter": 237,
          "sajda": false
        },
        {
          "number": 6012,
          "text": "وَتَأْكُلُونَ ٱلتُّرَاثَ أَكْلًۭا لَّمًّۭا",
          "numberInSurah": 19,
          "juz": 30,
          "manzil": 7,
          "page": 593,
          "ruku": 531,
          "hizbQuarter": 237,
          "sajda": false
        },
        {
          "number": 6013,
          "text": "وَتُحِبُّونَ ٱلْمَالَ حُبًّۭا جَمًّۭا",
          "numberInSurah": 20,
          "juz": 30,
          "manzil": 7,
          "page": 593,
          "ruku": 531,
          "hizbQuarter": 237,
          "sajda": false
        },
        {
          "number": 6014,
          "text": "كَلَّآ إِذَا دُكَّتِ ٱلْأَرْضُ دَكًّۭا دَكًّۭا",
          "numberInSurah": 21,
          "juz": 30,
          "manzil": 7,
          "page": 593,
          "ruku": 531,
          "hizbQuarter": 237,
          "sajda": false
        },
        {
          "number": 6015,
          "text": "وَجَآءَ رَبُّكَ وَٱلْمَلَكُ صَفًّۭا صَفًّۭا",
          "numberInSurah": 22,
          "juz": 30,
          "manzil": 7,
          "page": 593,
          "ruku": 531,
          "hizbQuarter": 237,
          "sajda": false
        },
        {
          "number": 6016,
          "text": "وَجِا۟ىٓءَ يَوْمَئِذٍۭ بِجَهَنَّمَ ۚ يَوْمَئِذٍۢ يَتَذَكَّرُ ٱلْإِنسَٰنُ وَأَنَّىٰ لَهُ ٱلذِّكْرَىٰ",
          "numberInSurah": 23,
          "juz": 30,
          "manzil": 7,
          "page": 593,
          "ruku": 531,
          "hizbQuarter": 237,
          "sajda": false
        },
        {
          "number": 6017,
          "text": "يَقُولُ يَٰلَيْتَنِى قَدَّمْتُ لِحَيَاتِى",
          "numberInSurah": 24,
          "juz": 30,
          "manzil": 7,
          "page": 594,
          "ruku": 531,
          "hizbQuarter": 237,
          "sajda": false
        },
        {
          "number": 6018,
          "text": "فَيَوْمَئِذٍۢ لَّا يُعَذِّبُ عَذَابَهُۥٓ أَحَدٌۭ",
          "numberInSurah": 25,
          "juz": 30,
          "manzil": 7,
          "page": 594,
          "ruku": 531,
          "hizbQuarter": 237,
          "sajda": false
        },
        {
          "number": 6019,
          "text": "وَلَا يُوثِقُ وَثَاقَهُۥٓ أَحَدٌۭ",
          "numberInSurah": 26,
          "juz": 30,
          "manzil": 7,
          "page": 594,
          "ruku": 531,
          "hizbQuarter": 237,
          "sajda": false
        },
        {
          "number": 6020,
          "text": "يَٰٓأَيَّتُهَا ٱلنَّفْسُ ٱلْمُطْمَئِنَّةُ",
          "numberInSurah": 27,
          "juz": 30,
          "manzil": 7,
          "page": 594,
          "ruku": 531,
          "hizbQuarter": 237,
          "sajda": false
        },
        {
          "number": 6021,
          "text": "ٱرْجِعِىٓ إِلَىٰ رَبِّكِ رَاضِيَةًۭ مَّرْضِيَّةًۭ",
          "numberInSurah": 28,
          "juz": 30,
          "manzil": 7,
          "page": 594,
          "ruku": 531,
          "hizbQuarter": 237,
          "sajda": false
        },
        {
          "number": 6022,
          "text": "فَٱدْخُلِى فِى عِبَٰدِى",
          "numberInSurah": 29,
          "juz": 30,
          "manzil": 7,
          "page": 594,
          "ruku": 531,
          "hizbQuarter": 237,
          "sajda": false
        },
        {
          "number": 6023,
          "text": "وَٱدْخُلِى جَنَّتِى",
          "numberInSurah": 30,
          "juz": 30,
          "manzil": 7,
          "page": 594,
          "ruku": 531,
          "hizbQuarter": 237,
          "sajda": false
        }
      ],
