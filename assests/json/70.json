  "data": [
    {
      "number": 70,
      "name": "سورة المعارج",
      "englishName": "Al-Ma'aarij",
      "englishNameTranslation": "The Ascending Stairways",
      "revelationType": "Meccan",
      "numberOfAyahs": 44,
      "ayahs": [
        {
          "number": 5376,
          "text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ سَأَلَ سَآئِلٌۢ بِعَذَابٍۢ وَاقِعٍۢ",
          "numberInSurah": 1,
          "juz": 29,
          "manzil": 7,
          "page": 568,
          "ruku": 502,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5377,
          "text": "لِّلْكَٰفِرِينَ لَيْسَ لَهُۥ دَافِعٌۭ",
          "numberInSurah": 2,
          "juz": 29,
          "manzil": 7,
          "page": 568,
          "ruku": 502,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5378,
          "text": "مِّنَ ٱللَّهِ ذِى ٱلْمَعَارِجِ",
          "numberInSurah": 3,
          "juz": 29,
          "manzil": 7,
          "page": 568,
          "ruku": 502,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5379,
          "text": "تَعْرُجُ ٱلْمَلَٰٓئِكَةُ وَٱلرُّوحُ إِلَيْهِ فِى يَوْمٍۢ كَانَ مِقْدَارُهُۥ خَمْسِينَ أَلْفَ سَنَةٍۢ",
          "numberInSurah": 4,
          "juz": 29,
          "manzil": 7,
          "page": 568,
          "ruku": 502,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5380,
          "text": "فَٱصْبِرْ صَبْرًۭا جَمِيلًا",
          "numberInSurah": 5,
          "juz": 29,
          "manzil": 7,
          "page": 568,
          "ruku": 502,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5381,
          "text": "إِنَّهُمْ يَرَوْنَهُۥ بَعِيدًۭا",
          "numberInSurah": 6,
          "juz": 29,
          "manzil": 7,
          "page": 568,
          "ruku": 502,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5382,
          "text": "وَنَرَىٰهُ قَرِيبًۭا",
          "numberInSurah": 7,
          "juz": 29,
          "manzil": 7,
          "page": 568,
          "ruku": 502,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5383,
          "text": "يَوْمَ تَكُونُ ٱلسَّمَآءُ كَٱلْمُهْلِ",
          "numberInSurah": 8,
          "juz": 29,
          "manzil": 7,
          "page": 568,
          "ruku": 502,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5384,
          "text": "وَتَكُونُ ٱلْجِبَالُ كَٱلْعِهْنِ",
          "numberInSurah": 9,
          "juz": 29,
          "manzil": 7,
          "page": 568,
          "ruku": 502,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5385,
          "text": "وَلَا يَسْـَٔلُ حَمِيمٌ حَمِيمًۭا",
          "numberInSurah": 10,
          "juz": 29,
          "manzil": 7,
          "page": 568,
          "ruku": 502,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5386,
          "text": "يُبَصَّرُونَهُمْ ۚ يَوَدُّ ٱلْمُجْرِمُ لَوْ يَفْتَدِى مِنْ عَذَابِ يَوْمِئِذٍۭ بِبَنِيهِ",
          "numberInSurah": 11,
          "juz": 29,
          "manzil": 7,
          "page": 569,
          "ruku": 502,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5387,
          "text": "وَصَٰحِبَتِهِۦ وَأَخِيهِ",
          "numberInSurah": 12,
          "juz": 29,
          "manzil": 7,
          "page": 569,
          "ruku": 502,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5388,
          "text": "وَفَصِيلَتِهِ ٱلَّتِى تُـْٔوِيهِ",
          "numberInSurah": 13,
          "juz": 29,
          "manzil": 7,
          "page": 569,
          "ruku": 502,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5389,
          "text": "وَمَن فِى ٱلْأَرْضِ جَمِيعًۭا ثُمَّ يُنجِيهِ",
          "numberInSurah": 14,
          "juz": 29,
          "manzil": 7,
          "page": 569,
          "ruku": 502,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5390,
          "text": "كَلَّآ ۖ إِنَّهَا لَظَىٰ",
          "numberInSurah": 15,
          "juz": 29,
          "manzil": 7,
          "page": 569,
          "ruku": 502,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5391,
          "text": "نَزَّاعَةًۭ لِّلشَّوَىٰ",
          "numberInSurah": 16,
          "juz": 29,
          "manzil": 7,
          "page": 569,
          "ruku": 502,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5392,
          "text": "تَدْعُوا۟ مَنْ أَدْبَرَ وَتَوَلَّىٰ",
          "numberInSurah": 17,
          "juz": 29,
          "manzil": 7,
          "page": 569,
          "ruku": 502,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5393,
          "text": "وَجَمَعَ فَأَوْعَىٰٓ",
          "numberInSurah": 18,
          "juz": 29,
          "manzil": 7,
          "page": 569,
          "ruku": 502,
          "hizbQuarter": 227,
          "sajda": false
        },
        {
          "number": 5394,
          "text": "۞ إِنَّ ٱلْإِنسَٰنَ خُلِقَ هَلُوعًا",
          "numberInSurah": 19,
          "juz": 29,
          "manzil": 7,
          "page": 569,
          "ruku": 502,
          "hizbQuarter": 228,
          "sajda": false
        },
        {
          "number": 5395,
          "text": "إِذَا مَسَّهُ ٱلشَّرُّ جَزُوعًۭا",
          "numberInSurah": 20,
          "juz": 29,
          "manzil": 7,
          "page": 569,
          "ruku": 502,
          "hizbQuarter": 228,
          "sajda": false
        },
        {
          "number": 5396,
          "text": "وَإِذَا مَسَّهُ ٱلْخَيْرُ مَنُوعًا",
          "numberInSurah": 21,
          "juz": 29,
          "manzil": 7,
          "page": 569,
          "ruku": 502,
          "hizbQuarter": 228,
          "sajda": false
        },
        {
          "number": 5397,
          "text": "إِلَّا ٱلْمُصَلِّينَ",
          "numberInSurah": 22,
          "juz": 29,
          "manzil": 7,
          "page": 569,
          "ruku": 502,
          "hizbQuarter": 228,
          "sajda": false
        },
        {
          "number": 5398,
          "text": "ٱلَّذِينَ هُمْ عَلَىٰ صَلَاتِهِمْ دَآئِمُونَ",
          "numberInSurah": 23,
          "juz": 29,
          "manzil": 7,
          "page": 569,
          "ruku": 502,
          "hizbQuarter": 228,
          "sajda": false
        },
        {
          "number": 5399,
          "text": "وَٱلَّذِينَ فِىٓ أَمْوَٰلِهِمْ حَقٌّۭ مَّعْلُومٌۭ",
          "numberInSurah": 24,
          "juz": 29,
          "manzil": 7,
          "page": 569,
          "ruku": 502,
          "hizbQuarter": 228,
          "sajda": false
        },
        {
          "number": 5400,
          "text": "لِّلسَّآئِلِ وَٱلْمَحْرُومِ",
          "numberInSurah": 25,
          "juz": 29,
          "manzil": 7,
          "page": 569,
          "ruku": 502,
          "hizbQuarter": 228,
          "sajda": false
        },
        {
          "number": 5401,
          "text": "وَٱلَّذِينَ يُصَدِّقُونَ بِيَوْمِ ٱلدِّينِ",
          "numberInSurah": 26,
          "juz": 29,
          "manzil": 7,
          "page": 569,
          "ruku": 502,
          "hizbQuarter": 228,
          "sajda": false
        },
        {
          "number": 5402,
          "text": "وَٱلَّذِينَ هُم مِّنْ عَذَابِ رَبِّهِم مُّشْفِقُونَ",
          "numberInSurah": 27,
          "juz": 29,
          "manzil": 7,
          "page": 569,
          "ruku": 502,
          "hizbQuarter": 228,
          "sajda": false
        },
        {
          "number": 5403,
          "text": "إِنَّ عَذَابَ رَبِّهِمْ غَيْرُ مَأْمُونٍۢ",
          "numberInSurah": 28,
          "juz": 29,
          "manzil": 7,
          "page": 569,
          "ruku": 502,
          "hizbQuarter": 228,
          "sajda": false
        },
        {
          "number": 5404,
          "text": "وَٱلَّذِينَ هُمْ لِفُرُوجِهِمْ حَٰفِظُونَ",
          "numberInSurah": 29,
          "juz": 29,
          "manzil": 7,
          "page": 569,
          "ruku": 502,
          "hizbQuarter": 228,
          "sajda": false
        },
        {
          "number": 5405,
          "text": "إِلَّا عَلَىٰٓ أَزْوَٰجِهِمْ أَوْ مَا مَلَكَتْ أَيْمَٰنُهُمْ فَإِنَّهُمْ غَيْرُ مَلُومِينَ",
          "numberInSurah": 30,
          "juz": 29,
          "manzil": 7,
          "page": 569,
          "ruku": 502,
          "hizbQuarter": 228,
          "sajda": false
        },
        {
          "number": 5406,
          "text": "فَمَنِ ٱبْتَغَىٰ وَرَآءَ ذَٰلِكَ فَأُو۟لَٰٓئِكَ هُمُ ٱلْعَادُونَ",
          "numberInSurah": 31,
          "juz": 29,
          "manzil": 7,
          "page": 569,
          "ruku": 502,
          "hizbQuarter": 228,
          "sajda": false
        },
        {
          "number": 5407,
          "text": "وَٱلَّذِينَ هُمْ لِأَمَٰنَٰتِهِمْ وَعَهْدِهِمْ رَٰعُونَ",
          "numberInSurah": 32,
          "juz": 29,
          "manzil": 7,
          "page": 569,
          "ruku": 502,
          "hizbQuarter": 228,
          "sajda": false
        },
        {
          "number": 5408,
          "text": "وَٱلَّذِينَ هُم بِشَهَٰدَٰتِهِمْ قَآئِمُونَ",
          "numberInSurah": 33,
          "juz": 29,
          "manzil": 7,
          "page": 569,
          "ruku": 502,
          "hizbQuarter": 228,
          "sajda": false
        },
        {
          "number": 5409,
          "text": "وَٱلَّذِينَ هُمْ عَلَىٰ صَلَاتِهِمْ يُحَافِظُونَ",
          "numberInSurah": 34,
          "juz": 29,
          "manzil": 7,
          "page": 569,
          "ruku": 502,
          "hizbQuarter": 228,
          "sajda": false
        },
        {
          "number": 5410,
          "text": "أُو۟لَٰٓئِكَ فِى جَنَّٰتٍۢ مُّكْرَمُونَ",
          "numberInSurah": 35,
          "juz": 29,
          "manzil": 7,
          "page": 569,
          "ruku": 502,
          "hizbQuarter": 228,
          "sajda": false
        },
        {
          "number": 5411,
          "text": "فَمَالِ ٱلَّذِينَ كَفَرُوا۟ قِبَلَكَ مُهْطِعِينَ",
          "numberInSurah": 36,
          "juz": 29,
          "manzil": 7,
          "page": 569,
          "ruku": 503,
          "hizbQuarter": 228,
          "sajda": false
        },
        {
          "number": 5412,
          "text": "عَنِ ٱلْيَمِينِ وَعَنِ ٱلشِّمَالِ عِزِينَ",
          "numberInSurah": 37,
          "juz": 29,
          "manzil": 7,
          "page": 569,
          "ruku": 503,
          "hizbQuarter": 228,
          "sajda": false
        },
        {
          "number": 5413,
          "text": "أَيَطْمَعُ كُلُّ ٱمْرِئٍۢ مِّنْهُمْ أَن يُدْخَلَ جَنَّةَ نَعِيمٍۢ",
          "numberInSurah": 38,
          "juz": 29,
          "manzil": 7,
          "page": 569,
          "ruku": 503,
          "hizbQuarter": 228,
          "sajda": false
        },
        {
          "number": 5414,
          "text": "كَلَّآ ۖ إِنَّا خَلَقْنَٰهُم مِّمَّا يَعْلَمُونَ",
          "numberInSurah": 39,
          "juz": 29,
          "manzil": 7,
          "page": 569,
          "ruku": 503,
          "hizbQuarter": 228,
          "sajda": false
        },
        {
          "number": 5415,
          "text": "فَلَآ أُقْسِمُ بِرَبِّ ٱلْمَشَٰرِقِ وَٱلْمَغَٰرِبِ إِنَّا لَقَٰدِرُونَ",
          "numberInSurah": 40,
          "juz": 29,
          "manzil": 7,
          "page": 570,
          "ruku": 503,
          "hizbQuarter": 228,
          "sajda": false
        },
        {
          "number": 5416,
          "text": "عَلَىٰٓ أَن نُّبَدِّلَ خَيْرًۭا مِّنْهُمْ وَمَا نَحْنُ بِمَسْبُوقِينَ",
          "numberInSurah": 41,
          "juz": 29,
          "manzil": 7,
          "page": 570,
          "ruku": 503,
          "hizbQuarter": 228,
          "sajda": false
        },
        {
          "number": 5417,
          "text": "فَذَرْهُمْ يَخُوضُوا۟ وَيَلْعَبُوا۟ حَتَّىٰ يُلَٰقُوا۟ يَوْمَهُمُ ٱلَّذِى يُوعَدُونَ",
          "numberInSurah": 42,
          "juz": 29,
          "manzil": 7,
          "page": 570,
          "ruku": 503,
          "hizbQuarter": 228,
          "sajda": false
        },
        {
          "number": 5418,
          "text": "يَوْمَ يَخْرُجُونَ مِنَ ٱلْأَجْدَاثِ سِرَاعًۭا كَأَنَّهُمْ إِلَىٰ نُصُبٍۢ يُوفِضُونَ",
          "numberInSurah": 43,
          "juz": 29,
          "manzil": 7,
          "page": 570,
          "ruku": 503,
          "hizbQuarter": 228,
          "sajda": false
        },
        {
          "number": 5419,
          "text": "خَٰشِعَةً أَبْصَٰرُهُمْ تَرْهَقُهُمْ ذِلَّةٌۭ ۚ ذَٰلِكَ ٱلْيَوْمُ ٱلَّذِى كَانُوا۟ يُوعَدُونَ",
          "numberInSurah": 44,
          "juz": 29,
          "manzil": 7,
          "page": 570,
          "ruku": 503,
          "hizbQuarter": 228,
          "sajda": false
        }
