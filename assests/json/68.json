  "data": [
    {
      "number": 68,
      "name": "سورة القلم",
      "englishName": "Al-Qalam",
      "englishNameTranslation": "The Pen",
      "revelationType": "Meccan",
      "numberOfAyahs": 52,
      "ayahs": [
        {
          "number": 5272,
          "text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ نٓ ۚ وَٱلْقَلَمِ وَمَا يَسْطُرُونَ",
          "numberInSurah": 1,
          "juz": 29,
          "manzil": 7,
          "page": 564,
          "ruku": 498,
          "hizbQuarter": 226,
          "sajda": false
        },
        {
          "number": 5273,
          "text": "مَآ أَنتَ بِنِعْمَةِ رَبِّكَ بِمَجْنُونٍۢ",
          "numberInSurah": 2,
          "juz": 29,
          "manzil": 7,
          "page": 564,
          "ruku": 498,
          "hizbQuarter": 226,
          "sajda": false
        },
        {
          "number": 5274,
          "text": "وَإِنَّ لَكَ لَأَجْرًا غَيْرَ مَمْنُونٍۢ",
          "numberInSurah": 3,
          "juz": 29,
          "manzil": 7,
          "page": 564,
          "ruku": 498,
          "hizbQuarter": 226,
          "sajda": false
        },
        {
          "number": 5275,
          "text": "وَإِنَّكَ لَعَلَىٰ خُلُقٍ عَظِيمٍۢ",
          "numberInSurah": 4,
          "juz": 29,
          "manzil": 7,
          "page": 564,
          "ruku": 498,
          "hizbQuarter": 226,
          "sajda": false
        },
        {
          "number": 5276,
          "text": "فَسَتُبْصِرُ وَيُبْصِرُونَ",
          "numberInSurah": 5,
          "juz": 29,
          "manzil": 7,
          "page": 564,
          "ruku": 498,
          "hizbQuarter": 226,
          "sajda": false
        },
        {
          "number": 5277,
          "text": "بِأَييِّكُمُ ٱلْمَفْتُونُ",
          "numberInSurah": 6,
          "juz": 29,
          "manzil": 7,
          "page": 564,
          "ruku": 498,
          "hizbQuarter": 226,
          "sajda": false
        },
        {
          "number": 5278,
          "text": "إِنَّ رَبَّكَ هُوَ أَعْلَمُ بِمَن ضَلَّ عَن سَبِيلِهِۦ وَهُوَ أَعْلَمُ بِٱلْمُهْتَدِينَ",
          "numberInSurah": 7,
          "juz": 29,
          "manzil": 7,
          "page": 564,
          "ruku": 498,
          "hizbQuarter": 226,
          "sajda": false
        },
        {
          "number": 5279,
          "text": "فَلَا تُطِعِ ٱلْمُكَذِّبِينَ",
          "numberInSurah": 8,
          "juz": 29,
          "manzil": 7,
          "page": 564,
          "ruku": 498,
          "hizbQuarter": 226,
          "sajda": false
        },
        {
          "number": 5280,
          "text": "وَدُّوا۟ لَوْ تُدْهِنُ فَيُدْهِنُونَ",
          "numberInSurah": 9,
          "juz": 29,
          "manzil": 7,
          "page": 564,
          "ruku": 498,
          "hizbQuarter": 226,
          "sajda": false
        },
        {
          "number": 5281,
          "text": "وَلَا تُطِعْ كُلَّ حَلَّافٍۢ مَّهِينٍ",
          "numberInSurah": 10,
          "juz": 29,
          "manzil": 7,
          "page": 564,
          "ruku": 498,
          "hizbQuarter": 226,
          "sajda": false
        },
        {
          "number": 5282,
          "text": "هَمَّازٍۢ مَّشَّآءٍۭ بِنَمِيمٍۢ",
          "numberInSurah": 11,
          "juz": 29,
          "manzil": 7,
          "page": 564,
          "ruku": 498,
          "hizbQuarter": 226,
          "sajda": false
        },
        {
          "number": 5283,
          "text": "مَّنَّاعٍۢ لِّلْخَيْرِ مُعْتَدٍ أَثِيمٍ",
          "numberInSurah": 12,
          "juz": 29,
          "manzil": 7,
          "page": 564,
          "ruku": 498,
          "hizbQuarter": 226,
          "sajda": false
        },
        {
          "number": 5284,
          "text": "عُتُلٍّۭ بَعْدَ ذَٰلِكَ زَنِيمٍ",
          "numberInSurah": 13,
          "juz": 29,
          "manzil": 7,
          "page": 564,
          "ruku": 498,
          "hizbQuarter": 226,
          "sajda": false
        },
        {
          "number": 5285,
          "text": "أَن كَانَ ذَا مَالٍۢ وَبَنِينَ",
          "numberInSurah": 14,
          "juz": 29,
          "manzil": 7,
          "page": 564,
          "ruku": 498,
          "hizbQuarter": 226,
          "sajda": false
        },
        {
          "number": 5286,
          "text": "إِذَا تُتْلَىٰ عَلَيْهِ ءَايَٰتُنَا قَالَ أَسَٰطِيرُ ٱلْأَوَّلِينَ",
          "numberInSurah": 15,
          "juz": 29,
          "manzil": 7,
          "page": 564,
          "ruku": 498,
          "hizbQuarter": 226,
          "sajda": false
        },
        {
          "number": 5287,
          "text": "سَنَسِمُهُۥ عَلَى ٱلْخُرْطُومِ",
          "numberInSurah": 16,
          "juz": 29,
          "manzil": 7,
          "page": 565,
          "ruku": 498,
          "hizbQuarter": 226,
          "sajda": false
        },
        {
          "number": 5288,
          "text": "إِنَّا بَلَوْنَٰهُمْ كَمَا بَلَوْنَآ أَصْحَٰبَ ٱلْجَنَّةِ إِذْ أَقْسَمُوا۟ لَيَصْرِمُنَّهَا مُصْبِحِينَ",
          "numberInSurah": 17,
          "juz": 29,
          "manzil": 7,
          "page": 565,
          "ruku": 498,
          "hizbQuarter": 226,
          "sajda": false
        },
        {
          "number": 5289,
          "text": "وَلَا يَسْتَثْنُونَ",
          "numberInSurah": 18,
          "juz": 29,
          "manzil": 7,
          "page": 565,
          "ruku": 498,
          "hizbQuarter": 226,
          "sajda": false
        },
        {
          "number": 5290,
          "text": "فَطَافَ عَلَيْهَا طَآئِفٌۭ مِّن رَّبِّكَ وَهُمْ نَآئِمُونَ",
          "numberInSurah": 19,
          "juz": 29,
          "manzil": 7,
          "page": 565,
          "ruku": 498,
          "hizbQuarter": 226,
          "sajda": false
        },
        {
          "number": 5291,
          "text": "فَأَصْبَحَتْ كَٱلصَّرِيمِ",
          "numberInSurah": 20,
          "juz": 29,
          "manzil": 7,
          "page": 565,
          "ruku": 498,
          "hizbQuarter": 226,
          "sajda": false
        },
        {
          "number": 5292,
          "text": "فَتَنَادَوْا۟ مُصْبِحِينَ",
          "numberInSurah": 21,
          "juz": 29,
          "manzil": 7,
          "page": 565,
          "ruku": 498,
          "hizbQuarter": 226,
          "sajda": false
        },
        {
          "number": 5293,
          "text": "أَنِ ٱغْدُوا۟ عَلَىٰ حَرْثِكُمْ إِن كُنتُمْ صَٰرِمِينَ",
          "numberInSurah": 22,
          "juz": 29,
          "manzil": 7,
          "page": 565,
          "ruku": 498,
          "hizbQuarter": 226,
          "sajda": false
        },
        {
          "number": 5294,
          "text": "فَٱنطَلَقُوا۟ وَهُمْ يَتَخَٰفَتُونَ",
          "numberInSurah": 23,
          "juz": 29,
          "manzil": 7,
          "page": 565,
          "ruku": 498,
          "hizbQuarter": 226,
          "sajda": false
        },
        {
          "number": 5295,
          "text": "أَن لَّا يَدْخُلَنَّهَا ٱلْيَوْمَ عَلَيْكُم مِّسْكِينٌۭ",
          "numberInSurah": 24,
          "juz": 29,
          "manzil": 7,
          "page": 565,
          "ruku": 498,
          "hizbQuarter": 226,
          "sajda": false
        },
        {
          "number": 5296,
          "text": "وَغَدَوْا۟ عَلَىٰ حَرْدٍۢ قَٰدِرِينَ",
          "numberInSurah": 25,
          "juz": 29,
          "manzil": 7,
          "page": 565,
          "ruku": 498,
          "hizbQuarter": 226,
          "sajda": false
        },
        {
          "number": 5297,
          "text": "فَلَمَّا رَأَوْهَا قَالُوٓا۟ إِنَّا لَضَآلُّونَ",
          "numberInSurah": 26,
          "juz": 29,
          "manzil": 7,
          "page": 565,
          "ruku": 498,
          "hizbQuarter": 226,
          "sajda": false
        },
        {
          "number": 5298,
          "text": "بَلْ نَحْنُ مَحْرُومُونَ",
          "numberInSurah": 27,
          "juz": 29,
          "manzil": 7,
          "page": 565,
          "ruku": 498,
          "hizbQuarter": 226,
          "sajda": false
        },
        {
          "number": 5299,
          "text": "قَالَ أَوْسَطُهُمْ أَلَمْ أَقُل لَّكُمْ لَوْلَا تُسَبِّحُونَ",
          "numberInSurah": 28,
          "juz": 29,
          "manzil": 7,
          "page": 565,
          "ruku": 498,
          "hizbQuarter": 226,
          "sajda": false
        },
        {
          "number": 5300,
          "text": "قَالُوا۟ سُبْحَٰنَ رَبِّنَآ إِنَّا كُنَّا ظَٰلِمِينَ",
          "numberInSurah": 29,
          "juz": 29,
          "manzil": 7,
          "page": 565,
          "ruku": 498,
          "hizbQuarter": 226,
          "sajda": false
        },
        {
          "number": 5301,
          "text": "فَأَقْبَلَ بَعْضُهُمْ عَلَىٰ بَعْضٍۢ يَتَلَٰوَمُونَ",
          "numberInSurah": 30,
          "juz": 29,
          "manzil": 7,
          "page": 565,
          "ruku": 498,
          "hizbQuarter": 226,
          "sajda": false
        },
        {
          "number": 5302,
          "text": "قَالُوا۟ يَٰوَيْلَنَآ إِنَّا كُنَّا طَٰغِينَ",
          "numberInSurah": 31,
          "juz": 29,
          "manzil": 7,
          "page": 565,
          "ruku": 498,
          "hizbQuarter": 226,
          "sajda": false
        },
        {
          "number": 5303,
          "text": "عَسَىٰ رَبُّنَآ أَن يُبْدِلَنَا خَيْرًۭا مِّنْهَآ إِنَّآ إِلَىٰ رَبِّنَا رَٰغِبُونَ",
          "numberInSurah": 32,
          "juz": 29,
          "manzil": 7,
          "page": 565,
          "ruku": 498,
          "hizbQuarter": 226,
          "sajda": false
        },
        {
          "number": 5304,
          "text": "كَذَٰلِكَ ٱلْعَذَابُ ۖ وَلَعَذَابُ ٱلْءَاخِرَةِ أَكْبَرُ ۚ لَوْ كَانُوا۟ يَعْلَمُونَ",
          "numberInSurah": 33,
          "juz": 29,
          "manzil": 7,
          "page": 565,
          "ruku": 498,
          "hizbQuarter": 226,
          "sajda": false
        },
        {
          "number": 5305,
          "text": "إِنَّ لِلْمُتَّقِينَ عِندَ رَبِّهِمْ جَنَّٰتِ ٱلنَّعِيمِ",
          "numberInSurah": 34,
          "juz": 29,
          "manzil": 7,
          "page": 565,
          "ruku": 499,
          "hizbQuarter": 226,
          "sajda": false
        },
        {
          "number": 5306,
          "text": "أَفَنَجْعَلُ ٱلْمُسْلِمِينَ كَٱلْمُجْرِمِينَ",
          "numberInSurah": 35,
          "juz": 29,
          "manzil": 7,
          "page": 565,
          "ruku": 499,
          "hizbQuarter": 226,
          "sajda": false
        },
        {
          "number": 5307,
          "text": "مَا لَكُمْ كَيْفَ تَحْكُمُونَ",
          "numberInSurah": 36,
          "juz": 29,
          "manzil": 7,
          "page": 565,
          "ruku": 499,
          "hizbQuarter": 226,
          "sajda": false
        },
        {
          "number": 5308,
          "text": "أَمْ لَكُمْ كِتَٰبٌۭ فِيهِ تَدْرُسُونَ",
          "numberInSurah": 37,
          "juz": 29,
          "manzil": 7,
          "page": 565,
          "ruku": 499,
          "hizbQuarter": 226,
          "sajda": false
        },
        {
          "number": 5309,
          "text": "إِنَّ لَكُمْ فِيهِ لَمَا تَخَيَّرُونَ",
          "numberInSurah": 38,
          "juz": 29,
          "manzil": 7,
          "page": 565,
          "ruku": 499,
          "hizbQuarter": 226,
          "sajda": false
        },
        {
          "number": 5310,
          "text": "أَمْ لَكُمْ أَيْمَٰنٌ عَلَيْنَا بَٰلِغَةٌ إِلَىٰ يَوْمِ ٱلْقِيَٰمَةِ ۙ إِنَّ لَكُمْ لَمَا تَحْكُمُونَ",
          "numberInSurah": 39,
          "juz": 29,
          "manzil": 7,
          "page": 565,
          "ruku": 499,
          "hizbQuarter": 226,
          "sajda": false
        },
        {
          "number": 5311,
          "text": "سَلْهُمْ أَيُّهُم بِذَٰلِكَ زَعِيمٌ",
          "numberInSurah": 40,
          "juz": 29,
          "manzil": 7,
          "page": 565,
          "ruku": 499,
          "hizbQuarter": 226,
          "sajda": false
        },
        {
          "number": 5312,
          "text": "أَمْ لَهُمْ شُرَكَآءُ فَلْيَأْتُوا۟ بِشُرَكَآئِهِمْ إِن كَانُوا۟ صَٰدِقِينَ",
          "numberInSurah": 41,
          "juz": 29,
          "manzil": 7,
          "page": 565,
          "ruku": 499,
          "hizbQuarter": 226,
          "sajda": false
        },
        {
          "number": 5313,
          "text": "يَوْمَ يُكْشَفُ عَن سَاقٍۢ وَيُدْعَوْنَ إِلَى ٱلسُّجُودِ فَلَا يَسْتَطِيعُونَ",
          "numberInSurah": 42,
          "juz": 29,
          "manzil": 7,
          "page": 565,
          "ruku": 499,
          "hizbQuarter": 226,
          "sajda": false
        },
        {
          "number": 5314,
          "text": "خَٰشِعَةً أَبْصَٰرُهُمْ تَرْهَقُهُمْ ذِلَّةٌۭ ۖ وَقَدْ كَانُوا۟ يُدْعَوْنَ إِلَى ٱلسُّجُودِ وَهُمْ سَٰلِمُونَ",
          "numberInSurah": 43,
          "juz": 29,
          "manzil": 7,
          "page": 566,
          "ruku": 499,
          "hizbQuarter": 226,
          "sajda": false
        },
        {
          "number": 5315,
          "text": "فَذَرْنِى وَمَن يُكَذِّبُ بِهَٰذَا ٱلْحَدِيثِ ۖ سَنَسْتَدْرِجُهُم مِّنْ حَيْثُ لَا يَعْلَمُونَ",
          "numberInSurah": 44,
          "juz": 29,
          "manzil": 7,
          "page": 566,
          "ruku": 499,
          "hizbQuarter": 226,
          "sajda": false
        },
        {
          "number": 5316,
          "text": "وَأُمْلِى لَهُمْ ۚ إِنَّ كَيْدِى مَتِينٌ",
          "numberInSurah": 45,
          "juz": 29,
          "manzil": 7,
          "page": 566,
          "ruku": 499,
          "hizbQuarter": 226,
          "sajda": false
        },
        {
          "number": 5317,
          "text": "أَمْ تَسْـَٔلُهُمْ أَجْرًۭا فَهُم مِّن مَّغْرَمٍۢ مُّثْقَلُونَ",
          "numberInSurah": 46,
          "juz": 29,
          "manzil": 7,
          "page": 566,
          "ruku": 499,
          "hizbQuarter": 226,
          "sajda": false
        },
        {
          "number": 5318,
          "text": "أَمْ عِندَهُمُ ٱلْغَيْبُ فَهُمْ يَكْتُبُونَ",
          "numberInSurah": 47,
          "juz": 29,
          "manzil": 7,
          "page": 566,
          "ruku": 499,
          "hizbQuarter": 226,
          "sajda": false
        },
        {
          "number": 5319,
          "text": "فَٱصْبِرْ لِحُكْمِ رَبِّكَ وَلَا تَكُن كَصَاحِبِ ٱلْحُوتِ إِذْ نَادَىٰ وَهُوَ مَكْظُومٌۭ",
          "numberInSurah": 48,
          "juz": 29,
          "manzil": 7,
          "page": 566,
          "ruku": 499,
          "hizbQuarter": 226,
          "sajda": false
        },
        {
          "number": 5320,
          "text": "لَّوْلَآ أَن تَدَٰرَكَهُۥ نِعْمَةٌۭ مِّن رَّبِّهِۦ لَنُبِذَ بِٱلْعَرَآءِ وَهُوَ مَذْمُومٌۭ",
          "numberInSurah": 49,
          "juz": 29,
          "manzil": 7,
          "page": 566,
          "ruku": 499,
          "hizbQuarter": 226,
          "sajda": false
        },
        {
          "number": 5321,
          "text": "فَٱجْتَبَٰهُ رَبُّهُۥ فَجَعَلَهُۥ مِنَ ٱلصَّٰلِحِينَ",
          "numberInSurah": 50,
          "juz": 29,
          "manzil": 7,
          "page": 566,
          "ruku": 499,
          "hizbQuarter": 226,
          "sajda": false
        },
        {
          "number": 5322,
          "text": "وَإِن يَكَادُ ٱلَّذِينَ كَفَرُوا۟ لَيُزْلِقُونَكَ بِأَبْصَٰرِهِمْ لَمَّا سَمِعُوا۟ ٱلذِّكْرَ وَيَقُولُونَ إِنَّهُۥ لَمَجْنُونٌۭ",
          "numberInSurah": 51,
          "juz": 29,
          "manzil": 7,
          "page": 566,
          "ruku": 499,
          "hizbQuarter": 226,
          "sajda": false
        },
        {
          "number": 5323,
          "text": "وَمَا هُوَ إِلَّا ذِكْرٌۭ لِّلْعَٰلَمِينَ",
          "numberInSurah": 52,
          "juz": 29,
          "manzil": 7,
          "page": 566,
