  "data": [
    {
      "number": 76,
      "name": "سورة الانسان",
      "englishName": "Al-Insaan",
      "englishNameTranslation": "Man",
      "revelationType": "Medinan",
      "numberOfAyahs": 31,
      "ayahs": [
        {
          "number": 5592,
          "text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ هَلْ أَتَىٰ عَلَى ٱلْإِنسَٰنِ حِينٌۭ مِّنَ ٱلدَّهْرِ لَمْ يَكُن شَيْـًۭٔا مَّذْكُورًا",
          "numberInSurah": 1,
          "juz": 29,
          "manzil": 7,
          "page": 578,
          "ruku": 514,
          "hizbQuarter": 231,
          "sajda": false
        },
        {
          "number": 5593,
          "text": "إِنَّا خَلَقْنَا ٱلْإِنسَٰنَ مِن نُّطْفَةٍ أَمْشَاجٍۢ نَّبْتَلِيهِ فَجَعَلْنَٰهُ سَمِيعًۢا بَصِيرًا",
          "numberInSurah": 2,
          "juz": 29,
          "manzil": 7,
          "page": 578,
          "ruku": 514,
          "hizbQuarter": 231,
          "sajda": false
        },
        {
          "number": 5594,
          "text": "إِنَّا هَدَيْنَٰهُ ٱلسَّبِيلَ إِمَّا شَاكِرًۭا وَإِمَّا كَفُورًا",
          "numberInSurah": 3,
          "juz": 29,
          "manzil": 7,
          "page": 578,
          "ruku": 514,
          "hizbQuarter": 231,
          "sajda": false
        },
        {
          "number": 5595,
          "text": "إِنَّآ أَعْتَدْنَا لِلْكَٰفِرِينَ سَلَٰسِلَا۟ وَأَغْلَٰلًۭا وَسَعِيرًا",
          "numberInSurah": 4,
          "juz": 29,
          "manzil": 7,
          "page": 578,
          "ruku": 514,
          "hizbQuarter": 231,
          "sajda": false
        },
        {
          "number": 5596,
          "text": "إِنَّ ٱلْأَبْرَارَ يَشْرَبُونَ مِن كَأْسٍۢ كَانَ مِزَاجُهَا كَافُورًا",
          "numberInSurah": 5,
          "juz": 29,
          "manzil": 7,
          "page": 578,
          "ruku": 514,
          "hizbQuarter": 231,
          "sajda": false
        },
        {
          "number": 5597,
          "text": "عَيْنًۭا يَشْرَبُ بِهَا عِبَادُ ٱللَّهِ يُفَجِّرُونَهَا تَفْجِيرًۭا",
          "numberInSurah": 6,
          "juz": 29,
          "manzil": 7,
          "page": 579,
          "ruku": 514,
          "hizbQuarter": 231,
          "sajda": false
        },
        {
          "number": 5598,
          "text": "يُوفُونَ بِٱلنَّذْرِ وَيَخَافُونَ يَوْمًۭا كَانَ شَرُّهُۥ مُسْتَطِيرًۭا",
          "numberInSurah": 7,
          "juz": 29,
          "manzil": 7,
          "page": 579,
          "ruku": 514,
          "hizbQuarter": 231,
          "sajda": false
        },
        {
          "number": 5599,
          "text": "وَيُطْعِمُونَ ٱلطَّعَامَ عَلَىٰ حُبِّهِۦ مِسْكِينًۭا وَيَتِيمًۭا وَأَسِيرًا",
          "numberInSurah": 8,
          "juz": 29,
          "manzil": 7,
          "page": 579,
          "ruku": 514,
          "hizbQuarter": 231,
          "sajda": false
        },
        {
          "number": 5600,
          "text": "إِنَّمَا نُطْعِمُكُمْ لِوَجْهِ ٱللَّهِ لَا نُرِيدُ مِنكُمْ جَزَآءًۭ وَلَا شُكُورًا",
          "numberInSurah": 9,
          "juz": 29,
          "manzil": 7,
          "page": 579,
          "ruku": 514,
          "hizbQuarter": 231,
          "sajda": false
        },
        {
          "number": 5601,
          "text": "إِنَّا نَخَافُ مِن رَّبِّنَا يَوْمًا عَبُوسًۭا قَمْطَرِيرًۭا",
          "numberInSurah": 10,
          "juz": 29,
          "manzil": 7,
          "page": 579,
          "ruku": 514,
          "hizbQuarter": 231,
          "sajda": false
        },
        {
          "number": 5602,
          "text": "فَوَقَىٰهُمُ ٱللَّهُ شَرَّ ذَٰلِكَ ٱلْيَوْمِ وَلَقَّىٰهُمْ نَضْرَةًۭ وَسُرُورًۭا",
          "numberInSurah": 11,
          "juz": 29,
          "manzil": 7,
          "page": 579,
          "ruku": 514,
          "hizbQuarter": 231,
          "sajda": false
        },
        {
          "number": 5603,
          "text": "وَجَزَىٰهُم بِمَا صَبَرُوا۟ جَنَّةًۭ وَحَرِيرًۭا",
          "numberInSurah": 12,
          "juz": 29,
          "manzil": 7,
          "page": 579,
          "ruku": 514,
          "hizbQuarter": 231,
          "sajda": false
        },
        {
          "number": 5604,
          "text": "مُّتَّكِـِٔينَ فِيهَا عَلَى ٱلْأَرَآئِكِ ۖ لَا يَرَوْنَ فِيهَا شَمْسًۭا وَلَا زَمْهَرِيرًۭا",
          "numberInSurah": 13,
          "juz": 29,
          "manzil": 7,
          "page": 579,
          "ruku": 514,
          "hizbQuarter": 231,
          "sajda": false
        },
        {
          "number": 5605,
          "text": "وَدَانِيَةً عَلَيْهِمْ ظِلَٰلُهَا وَذُلِّلَتْ قُطُوفُهَا تَذْلِيلًۭا",
          "numberInSurah": 14,
          "juz": 29,
          "manzil": 7,
          "page": 579,
          "ruku": 514,
          "hizbQuarter": 231,
          "sajda": false
        },
        {
          "number": 5606,
          "text": "وَيُطَافُ عَلَيْهِم بِـَٔانِيَةٍۢ مِّن فِضَّةٍۢ وَأَكْوَابٍۢ كَانَتْ قَوَارِيرَا۠",
          "numberInSurah": 15,
          "juz": 29,
          "manzil": 7,
          "page": 579,
          "ruku": 514,
          "hizbQuarter": 231,
          "sajda": false
        },
        {
          "number": 5607,
          "text": "قَوَارِيرَا۟ مِن فِضَّةٍۢ قَدَّرُوهَا تَقْدِيرًۭا",
          "numberInSurah": 16,
          "juz": 29,
          "manzil": 7,
          "page": 579,
          "ruku": 514,
          "hizbQuarter": 231,
          "sajda": false
        },
        {
          "number": 5608,
          "text": "وَيُسْقَوْنَ فِيهَا كَأْسًۭا كَانَ مِزَاجُهَا زَنجَبِيلًا",
          "numberInSurah": 17,
          "juz": 29,
          "manzil": 7,
          "page": 579,
          "ruku": 514,
          "hizbQuarter": 231,
          "sajda": false
        },
        {
          "number": 5609,
          "text": "عَيْنًۭا فِيهَا تُسَمَّىٰ سَلْسَبِيلًۭا",
          "numberInSurah": 18,
          "juz": 29,
          "manzil": 7,
          "page": 579,
          "ruku": 514,
          "hizbQuarter": 231,
          "sajda": false
        },
        {
          "number": 5610,
          "text": "۞ وَيَطُوفُ عَلَيْهِمْ وِلْدَٰنٌۭ مُّخَلَّدُونَ إِذَا رَأَيْتَهُمْ حَسِبْتَهُمْ لُؤْلُؤًۭا مَّنثُورًۭا",
          "numberInSurah": 19,
          "juz": 29,
          "manzil": 7,
          "page": 579,
          "ruku": 514,
          "hizbQuarter": 232,
          "sajda": false
        },
        {
          "number": 5611,
          "text": "وَإِذَا رَأَيْتَ ثَمَّ رَأَيْتَ نَعِيمًۭا وَمُلْكًۭا كَبِيرًا",
          "numberInSurah": 20,
          "juz": 29,
          "manzil": 7,
          "page": 579,
          "ruku": 514,
          "hizbQuarter": 232,
          "sajda": false
        },
        {
          "number": 5612,
          "text": "عَٰلِيَهُمْ ثِيَابُ سُندُسٍ خُضْرٌۭ وَإِسْتَبْرَقٌۭ ۖ وَحُلُّوٓا۟ أَسَاوِرَ مِن فِضَّةٍۢ وَسَقَىٰهُمْ رَبُّهُمْ شَرَابًۭا طَهُورًا",
          "numberInSurah": 21,
          "juz": 29,
          "manzil": 7,
          "page": 579,
          "ruku": 514,
          "hizbQuarter": 232,
          "sajda": false
        },
        {
          "number": 5613,
          "text": "إِنَّ هَٰذَا كَانَ لَكُمْ جَزَآءًۭ وَكَانَ سَعْيُكُم مَّشْكُورًا",
          "numberInSurah": 22,
          "juz": 29,
          "manzil": 7,
          "page": 579,
          "ruku": 514,
          "hizbQuarter": 232,
          "sajda": false
        },
        {
          "number": 5614,
          "text": "إِنَّا نَحْنُ نَزَّلْنَا عَلَيْكَ ٱلْقُرْءَانَ تَنزِيلًۭا",
          "numberInSurah": 23,
          "juz": 29,
          "manzil": 7,
          "page": 579,
          "ruku": 515,
          "hizbQuarter": 232,
          "sajda": false
        },
        {
          "number": 5615,
          "text": "فَٱصْبِرْ لِحُكْمِ رَبِّكَ وَلَا تُطِعْ مِنْهُمْ ءَاثِمًا أَوْ كَفُورًۭا",
          "numberInSurah": 24,
          "juz": 29,
          "manzil": 7,
          "page": 579,
          "ruku": 515,
          "hizbQuarter": 232,
          "sajda": false
        },
        {
          "number": 5616,
          "text": "وَٱذْكُرِ ٱسْمَ رَبِّكَ بُكْرَةًۭ وَأَصِيلًۭا",
          "numberInSurah": 25,
          "juz": 29,
          "manzil": 7,
          "page": 579,
          "ruku": 515,
          "hizbQuarter": 232,
          "sajda": false
        },
        {
          "number": 5617,
          "text": "وَمِنَ ٱلَّيْلِ فَٱسْجُدْ لَهُۥ وَسَبِّحْهُ لَيْلًۭا طَوِيلًا",
          "numberInSurah": 26,
          "juz": 29,
          "manzil": 7,
          "page": 580,
          "ruku": 515,
          "hizbQuarter": 232,
          "sajda": false
        },
        {
          "number": 5618,
          "text": "إِنَّ هَٰٓؤُلَآءِ يُحِبُّونَ ٱلْعَاجِلَةَ وَيَذَرُونَ وَرَآءَهُمْ يَوْمًۭا ثَقِيلًۭا",
          "numberInSurah": 27,
          "juz": 29,
          "manzil": 7,
          "page": 580,
          "ruku": 515,
          "hizbQuarter": 232,
          "sajda": false
        },
        {
          "number": 5619,
          "text": "نَّحْنُ خَلَقْنَٰهُمْ وَشَدَدْنَآ أَسْرَهُمْ ۖ وَإِذَا شِئْنَا بَدَّلْنَآ أَمْثَٰلَهُمْ تَبْدِيلًا",
          "numberInSurah": 28,
          "juz": 29,
          "manzil": 7,
          "page": 580,
          "ruku": 515,
          "hizbQuarter": 232,
          "sajda": false
        },
        {
          "number": 5620,
          "text": "إِنَّ هَٰذِهِۦ تَذْكِرَةٌۭ ۖ فَمَن شَآءَ ٱتَّخَذَ إِلَىٰ رَبِّهِۦ سَبِيلًۭا",
          "numberInSurah": 29,
          "juz": 29,
          "manzil": 7,
          "page": 580,
          "ruku": 515,
          "hizbQuarter": 232,
          "sajda": false
        },
        {
          "number": 5621,
          "text": "وَمَا تَشَآءُونَ إِلَّآ أَن يَشَآءَ ٱللَّهُ ۚ إِنَّ ٱللَّهَ كَانَ عَلِيمًا حَكِيمًۭا",
          "numberInSurah": 30,
          "juz": 29,
          "manzil": 7,
          "page": 580,
          "ruku": 515,
          "hizbQuarter": 232,
          "sajda": false
        },
        {
          "number": 5622,
          "text": "يُدْخِلُ مَن يَشَآءُ فِى رَحْمَتِهِۦ ۚ وَٱلظَّٰلِمِينَ أَعَدَّ لَهُمْ عَذَابًا أَلِيمًۢا",
          "numberInSurah": 31,
          "juz": 29,
          "manzil": 7,
          "page": 580,
          "ruku": 515,
          "hizbQuarter": 232,
          "sajda": false
        }
      ],
