      "number": 54,
      "name": "سورة القمر",
      "englishName": "Al-Qamar",
      "englishNameTranslation": "The Moon",
      "revelationType": "Meccan",
      "numberOfAyahs": 55,
      "ayahs": [
        {
          "number": 4847,
          "text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ ٱقْتَرَبَتِ ٱلسَّاعَةُ وَٱنشَقَّ ٱلْقَمَرُ",
          "numberInSurah": 1,
          "juz": 27,
          "manzil": 7,
          "page": 528,
          "ruku": 463,
          "hizbQuarter": 211,
          "sajda": false
        },
        {
          "number": 4848,
          "text": "وَإِن يَرَوْا۟ ءَايَةًۭ يُعْرِضُوا۟ وَيَقُولُوا۟ سِحْرٌۭ مُّسْتَمِرٌّۭ",
          "numberInSurah": 2,
          "juz": 27,
          "manzil": 7,
          "page": 528,
          "ruku": 463,
          "hizbQuarter": 211,
          "sajda": false
        },
        {
          "number": 4849,
          "text": "وَكَذَّبُوا۟ وَٱتَّبَعُوٓا۟ أَهْوَآءَهُمْ ۚ وَكُلُّ أَمْرٍۢ مُّسْتَقِرٌّۭ",
          "numberInSurah": 3,
          "juz": 27,
          "manzil": 7,
          "page": 528,
          "ruku": 463,
          "hizbQuarter": 211,
          "sajda": false
        },
        {
          "number": 4850,
          "text": "وَلَقَدْ جَآءَهُم مِّنَ ٱلْأَنۢبَآءِ مَا فِيهِ مُزْدَجَرٌ",
          "numberInSurah": 4,
          "juz": 27,
          "manzil": 7,
          "page": 528,
          "ruku": 463,
          "hizbQuarter": 211,
          "sajda": false
        },
        {
          "number": 4851,
          "text": "حِكْمَةٌۢ بَٰلِغَةٌۭ ۖ فَمَا تُغْنِ ٱلنُّذُرُ",
          "numberInSurah": 5,
          "juz": 27,
          "manzil": 7,
          "page": 528,
          "ruku": 463,
          "hizbQuarter": 211,
          "sajda": false
        },
        {
          "number": 4852,
          "text": "فَتَوَلَّ عَنْهُمْ ۘ يَوْمَ يَدْعُ ٱلدَّاعِ إِلَىٰ شَىْءٍۢ نُّكُرٍ",
          "numberInSurah": 6,
          "juz": 27,
          "manzil": 7,
          "page": 528,
          "ruku": 463,
          "hizbQuarter": 211,
          "sajda": false
        },
        {
          "number": 4853,
          "text": "خُشَّعًا أَبْصَٰرُهُمْ يَخْرُجُونَ مِنَ ٱلْأَجْدَاثِ كَأَنَّهُمْ جَرَادٌۭ مُّنتَشِرٌۭ",
          "numberInSurah": 7,
          "juz": 27,
          "manzil": 7,
          "page": 529,
          "ruku": 463,
          "hizbQuarter": 211,
          "sajda": false
        },
        {
          "number": 4854,
          "text": "مُّهْطِعِينَ إِلَى ٱلدَّاعِ ۖ يَقُولُ ٱلْكَٰفِرُونَ هَٰذَا يَوْمٌ عَسِرٌۭ",
          "numberInSurah": 8,
          "juz": 27,
          "manzil": 7,
          "page": 529,
          "ruku": 463,
          "hizbQuarter": 211,
          "sajda": false
        },
        {
          "number": 4855,
          "text": "۞ كَذَّبَتْ قَبْلَهُمْ قَوْمُ نُوحٍۢ فَكَذَّبُوا۟ عَبْدَنَا وَقَالُوا۟ مَجْنُونٌۭ وَٱزْدُجِرَ",
          "numberInSurah": 9,
          "juz": 27,
          "manzil": 7,
          "page": 529,
          "ruku": 463,
          "hizbQuarter": 212,
          "sajda": false
        },
        {
          "number": 4856,
          "text": "فَدَعَا رَبَّهُۥٓ أَنِّى مَغْلُوبٌۭ فَٱنتَصِرْ",
          "numberInSurah": 10,
          "juz": 27,
          "manzil": 7,
          "page": 529,
          "ruku": 463,
          "hizbQuarter": 212,
          "sajda": false
        },
        {
          "number": 4857,
          "text": "فَفَتَحْنَآ أَبْوَٰبَ ٱلسَّمَآءِ بِمَآءٍۢ مُّنْهَمِرٍۢ",
          "numberInSurah": 11,
          "juz": 27,
          "manzil": 7,
          "page": 529,
          "ruku": 463,
          "hizbQuarter": 212,
          "sajda": false
        },
        {
          "number": 4858,
          "text": "وَفَجَّرْنَا ٱلْأَرْضَ عُيُونًۭا فَٱلْتَقَى ٱلْمَآءُ عَلَىٰٓ أَمْرٍۢ قَدْ قُدِرَ",
          "numberInSurah": 12,
          "juz": 27,
          "manzil": 7,
          "page": 529,
          "ruku": 463,
          "hizbQuarter": 212,
          "sajda": false
        },
        {
          "number": 4859,
          "text": "وَحَمَلْنَٰهُ عَلَىٰ ذَاتِ أَلْوَٰحٍۢ وَدُسُرٍۢ",
          "numberInSurah": 13,
          "juz": 27,
          "manzil": 7,
          "page": 529,
          "ruku": 463,
          "hizbQuarter": 212,
          "sajda": false
        },
        {
          "number": 4860,
          "text": "تَجْرِى بِأَعْيُنِنَا جَزَآءًۭ لِّمَن كَانَ كُفِرَ",
          "numberInSurah": 14,
          "juz": 27,
          "manzil": 7,
          "page": 529,
          "ruku": 463,
          "hizbQuarter": 212,
          "sajda": false
        },
        {
          "number": 4861,
          "text": "وَلَقَد تَّرَكْنَٰهَآ ءَايَةًۭ فَهَلْ مِن مُّدَّكِرٍۢ",
          "numberInSurah": 15,
          "juz": 27,
          "manzil": 7,
          "page": 529,
          "ruku": 463,
          "hizbQuarter": 212,
          "sajda": false
        },
        {
          "number": 4862,
          "text": "فَكَيْفَ كَانَ عَذَابِى وَنُذُرِ",
          "numberInSurah": 16,
          "juz": 27,
          "manzil": 7,
          "page": 529,
          "ruku": 463,
          "hizbQuarter": 212,
          "sajda": false
        },
        {
          "number": 4863,
          "text": "وَلَقَدْ يَسَّرْنَا ٱلْقُرْءَانَ لِلذِّكْرِ فَهَلْ مِن مُّدَّكِرٍۢ",
          "numberInSurah": 17,
          "juz": 27,
          "manzil": 7,
          "page": 529,
          "ruku": 463,
          "hizbQuarter": 212,
          "sajda": false
        },
        {
          "number": 4864,
          "text": "كَذَّبَتْ عَادٌۭ فَكَيْفَ كَانَ عَذَابِى وَنُذُرِ",
          "numberInSurah": 18,
          "juz": 27,
          "manzil": 7,
          "page": 529,
          "ruku": 463,
          "hizbQuarter": 212,
          "sajda": false
        },
        {
          "number": 4865,
          "text": "إِنَّآ أَرْسَلْنَا عَلَيْهِمْ رِيحًۭا صَرْصَرًۭا فِى يَوْمِ نَحْسٍۢ مُّسْتَمِرٍّۢ",
          "numberInSurah": 19,
          "juz": 27,
          "manzil": 7,
          "page": 529,
          "ruku": 463,
          "hizbQuarter": 212,
          "sajda": false
        },
        {
          "number": 4866,
          "text": "تَنزِعُ ٱلنَّاسَ كَأَنَّهُمْ أَعْجَازُ نَخْلٍۢ مُّنقَعِرٍۢ",
          "numberInSurah": 20,
          "juz": 27,
          "manzil": 7,
          "page": 529,
          "ruku": 463,
          "hizbQuarter": 212,
          "sajda": false
        },
        {
          "number": 4867,
          "text": "فَكَيْفَ كَانَ عَذَابِى وَنُذُرِ",
          "numberInSurah": 21,
          "juz": 27,
