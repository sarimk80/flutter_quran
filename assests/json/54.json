          "ruku": 463,
          "hizbQuarter": 212,
          "sajda": false
        },
        {
          "number": 4869,
          "text": "كَذَّبَتْ ثَمُودُ بِٱلنُّذُرِ",
          "numberInSurah": 23,
          "juz": 27,
          "manzil": 7,
          "page": 529,
          "ruku": 464,
          "hizbQuarter": 212,
          "sajda": false
        },
        {
          "number": 4870,
          "text": "فَقَالُوٓا۟ أَبَشَرًۭا مِّنَّا وَٰحِدًۭا نَّتَّبِعُهُۥٓ إِنَّآ إِذًۭا لَّفِى ضَلَٰلٍۢ وَسُعُرٍ",
          "numberInSurah": 24,
          "juz": 27,
          "manzil": 7,
          "page": 529,
          "ruku": 464,
          "hizbQuarter": 212,
          "sajda": false
        },
        {
          "number": 4871,
          "text": "أَءُلْقِىَ ٱلذِّكْرُ عَلَيْهِ مِنۢ بَيْنِنَا بَلْ هُوَ كَذَّابٌ أَشِرٌۭ",
          "numberInSurah": 25,
