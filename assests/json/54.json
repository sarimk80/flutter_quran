          "manzil": 7,
          "page": 529,
          "ruku": 464,
          "hizbQuarter": 212,
          "sajda": false
        },
        {
          "number": 4872,
          "text": "سَيَعْلَمُونَ غَدًۭا مَّنِ ٱلْكَذَّابُ ٱلْأَشِرُ",
          "numberInSurah": 26,
          "juz": 27,
