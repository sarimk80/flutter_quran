          "hizbQuarter": 182,
          "sajda": false
        },
        {
          "number": 4010,
          "text": "وَإِنَّ لَهُۥ عِندَنَا لَزُلْفَىٰ وَحُسْنَ مَـَٔابٍۢ",
          "numberInSurah": 40,
          "juz": 23,
          "manzil": 6,
          "page": 455,
          "ruku": 393,
          "hizbQuarter": 182,
          "sajda": false
        },
        {
          "number": 4011,
          "text": "وَٱذْكُرْ عَبْدَنَآ أَيُّوبَ إِذْ نَادَىٰ رَبَّهُۥٓ أَنِّى مَسَّنِىَ ٱلشَّيْطَٰنُ بِنُصْبٍۢ وَعَذَابٍ",
          "numberInSurah": 41,
          "juz": 23,
          "manzil": 6,
          "page": 455,
          "ruku": 394,
          "hizbQuarter": 182,
          "sajda": false
        },
        {
          "number": 4012,
          "text": "ٱرْكُضْ بِرِجْلِكَ ۖ هَٰذَا مُغْتَسَلٌۢ بَارِدٌۭ وَشَرَابٌۭ",
          "numberInSurah": 42,
          "juz": 23,
          "manzil": 6,
          "page": 455,
          "ruku": 394,
          "hizbQuarter": 182,
          "sajda": false
        },
        {
          "number": 4013,
          "text": "وَوَهَبْنَا لَهُۥٓ أَهْلَهُۥ وَمِثْلَهُم مَّعَهُمْ رَحْمَةًۭ مِّنَّا وَذِكْرَىٰ لِأُو۟لِى ٱلْأَلْبَٰبِ",
          "numberInSurah": 43,
          "juz": 23,
          "manzil": 6,
          "page": 456,
          "ruku": 394,
          "hizbQuarter": 182,
          "sajda": false
        },
        {
          "number": 4014,
          "text": "وَخُذْ بِيَدِكَ ضِغْثًۭا فَٱضْرِب بِّهِۦ وَلَا تَحْنَثْ ۗ إِنَّا وَجَدْنَٰهُ صَابِرًۭا ۚ نِّعْمَ ٱلْعَبْدُ ۖ إِنَّهُۥٓ أَوَّابٌۭ",
          "numberInSurah": 44,
          "juz": 23,
          "manzil": 6,
          "page": 456,
          "ruku": 394,
          "hizbQuarter": 182,
          "sajda": false
        },
        {
          "number": 4015,
          "text": "وَٱذْكُرْ عِبَٰدَنَآ إِبْرَٰهِيمَ وَإِسْحَٰقَ وَيَعْقُوبَ أُو۟لِى ٱلْأَيْدِى وَٱلْأَبْصَٰرِ",
          "numberInSurah": 45,
          "juz": 23,
          "manzil": 6,
          "page": 456,
          "ruku": 394,
          "hizbQuarter": 182,
          "sajda": false
        },
        {
          "number": 4016,
          "text": "إِنَّآ أَخْلَصْنَٰهُم بِخَالِصَةٍۢ ذِكْرَى ٱلدَّارِ",
          "numberInSurah": 46,
          "juz": 23,
          "manzil": 6,
          "page": 456,
          "ruku": 394,
          "hizbQuarter": 182,
          "sajda": false
        },
        {
          "number": 4017,
          "text": "وَإِنَّهُمْ عِندَنَا لَمِنَ ٱلْمُصْطَفَيْنَ ٱلْأَخْيَارِ",
          "numberInSurah": 47,
          "juz": 23,
          "manzil": 6,
          "page": 456,
          "ruku": 394,
          "hizbQuarter": 182,
          "sajda": false
        },
        {
          "number": 4018,
          "text": "وَٱذْكُرْ إِسْمَٰعِيلَ وَٱلْيَسَعَ وَذَا ٱلْكِفْلِ ۖ وَكُلٌّۭ مِّنَ ٱلْأَخْيَارِ",
          "numberInSurah": 48,
          "juz": 23,
          "manzil": 6,
          "page": 456,
          "ruku": 394,
          "hizbQuarter": 182,
          "sajda": false
        },
        {
          "number": 4019,
          "text": "هَٰذَا ذِكْرٌۭ ۚ وَإِنَّ لِلْمُتَّقِينَ لَحُسْنَ مَـَٔابٍۢ",
          "numberInSurah": 49,
          "juz": 23,
          "manzil": 6,
          "page": 456,
          "ruku": 394,
          "hizbQuarter": 182,
          "sajda": false
        },
        {
          "number": 4020,
          "text": "جَنَّٰتِ عَدْنٍۢ مُّفَتَّحَةًۭ لَّهُمُ ٱلْأَبْوَٰبُ",
          "numberInSurah": 50,
          "juz": 23,
          "manzil": 6,
          "page": 456,
          "ruku": 394,
          "hizbQuarter": 182,
          "sajda": false
        },
        {
          "number": 4021,
          "text": "مُتَّكِـِٔينَ فِيهَا يَدْعُونَ فِيهَا بِفَٰكِهَةٍۢ كَثِيرَةٍۢ وَشَرَابٍۢ",
          "numberInSurah": 51,
          "juz": 23,
          "manzil": 6,
          "page": 456,
          "ruku": 394,
          "hizbQuarter": 182,
          "sajda": false
        },
        {
          "number": 4022,
          "text": "۞ وَعِندَهُمْ قَٰصِرَٰتُ ٱلطَّرْفِ أَتْرَابٌ",
          "numberInSurah": 52,
          "juz": 23,
          "manzil": 6,
          "page": 456,
          "ruku": 394,
          "hizbQuarter": 183,
          "sajda": false
        },
        {
          "number": 4023,
          "text": "هَٰذَا مَا تُوعَدُونَ لِيَوْمِ ٱلْحِسَابِ",
          "numberInSurah": 53,
          "juz": 23,
          "manzil": 6,
          "page": 456,
          "ruku": 394,
          "hizbQuarter": 183,
          "sajda": false
        },
        {
          "number": 4024,
          "text": "إِنَّ هَٰذَا لَرِزْقُنَا مَا لَهُۥ مِن نَّفَادٍ",
          "numberInSurah": 54,
          "juz": 23,
          "manzil": 6,
          "page": 456,
          "ruku": 394,
          "hizbQuarter": 183,
          "sajda": false
        },
        {
          "number": 4025,
          "text": "هَٰذَا ۚ وَإِنَّ لِلطَّٰغِينَ لَشَرَّ مَـَٔابٍۢ",
          "numberInSurah": 55,
          "juz": 23,
          "manzil": 6,
          "page": 456,
          "ruku": 394,
          "hizbQuarter": 183,
          "sajda": false
        },
        {
          "number": 4026,
          "text": "جَهَنَّمَ يَصْلَوْنَهَا فَبِئْسَ ٱلْمِهَادُ",
          "numberInSurah": 56,
          "juz": 23,
          "manzil": 6,
          "page": 456,
          "ruku": 394,
          "hizbQuarter": 183,
          "sajda": false
        },
        {
          "number": 4027,
          "text": "هَٰذَا فَلْيَذُوقُوهُ حَمِيمٌۭ وَغَسَّاقٌۭ",
          "numberInSurah": 57,
          "juz": 23,
          "manzil": 6,
          "page": 456,
          "ruku": 394,
          "hizbQuarter": 183,
          "sajda": false
        },
        {
          "number": 4028,
          "text": "وَءَاخَرُ مِن شَكْلِهِۦٓ أَزْوَٰجٌ",
          "numberInSurah": 58,
          "juz": 23,
          "manzil": 6,
          "page": 456,
          "ruku": 394,
          "hizbQuarter": 183,
          "sajda": false
        },
        {
          "number": 4029,
          "text": "هَٰذَا فَوْجٌۭ مُّقْتَحِمٌۭ مَّعَكُمْ ۖ لَا مَرْحَبًۢا بِهِمْ ۚ إِنَّهُمْ صَالُوا۟ ٱلنَّارِ",
          "numberInSurah": 59,
          "juz": 23,
          "manzil": 6,
          "page": 456,
          "ruku": 394,
          "hizbQuarter": 183,
          "sajda": false
        },
        {
          "number": 4030,
          "text": "قَالُوا۟ بَلْ أَنتُمْ لَا مَرْحَبًۢا بِكُمْ ۖ أَنتُمْ قَدَّمْتُمُوهُ لَنَا ۖ فَبِئْسَ ٱلْقَرَارُ",
          "numberInSurah": 60,
          "juz": 23,
          "manzil": 6,
          "page": 456,
          "ruku": 394,
          "hizbQuarter": 183,
          "sajda": false
        },
        {
          "number": 4031,
          "text": "قَالُوا۟ رَبَّنَا مَن قَدَّمَ لَنَا هَٰذَا فَزِدْهُ عَذَابًۭا ضِعْفًۭا فِى ٱلنَّارِ",
          "numberInSurah": 61,
          "juz": 23,
          "manzil": 6,
          "page": 456,
          "ruku": 394,
          "hizbQuarter": 183,
          "sajda": false
        },
        {
          "number": 4032,
          "text": "وَقَالُوا۟ مَا لَنَا لَا نَرَىٰ رِجَالًۭا كُنَّا نَعُدُّهُم مِّنَ ٱلْأَشْرَارِ",
          "numberInSurah": 62,
          "juz": 23,
          "manzil": 6,
          "page": 457,
          "ruku": 394,
          "hizbQuarter": 183,
          "sajda": false
        },
        {
          "number": 4033,
          "text": "أَتَّخَذْنَٰهُمْ سِخْرِيًّا أَمْ زَاغَتْ عَنْهُمُ ٱلْأَبْصَٰرُ",
          "numberInSurah": 63,
          "juz": 23,
          "manzil": 6,
          "page": 457,
          "ruku": 394,
          "hizbQuarter": 183,
          "sajda": false
        },
        {
          "number": 4034,
          "text": "إِنَّ ذَٰلِكَ لَحَقٌّۭ تَخَاصُمُ أَهْلِ ٱلنَّارِ",
          "numberInSurah": 64,
          "juz": 23,
          "manzil": 6,
          "page": 457,
          "ruku": 394,
          "hizbQuarter": 183,
          "sajda": false
        },
        {
          "number": 4035,
          "text": "قُلْ إِنَّمَآ أَنَا۠ مُنذِرٌۭ ۖ وَمَا مِنْ إِلَٰهٍ إِلَّا ٱللَّهُ ٱلْوَٰحِدُ ٱلْقَهَّارُ",
          "numberInSurah": 65,
          "juz": 23,
          "manzil": 6,
          "page": 457,
          "ruku": 395,
          "hizbQuarter": 183,
          "sajda": false
        },
        {
          "number": 4036,
          "text": "رَبُّ ٱلسَّمَٰوَٰتِ وَٱلْأَرْضِ وَمَا بَيْنَهُمَا ٱلْعَزِيزُ ٱلْغَفَّٰرُ",
          "numberInSurah": 66,
          "juz": 23,
          "manzil": 6,
          "page": 457,
          "ruku": 395,
          "hizbQuarter": 183,
          "sajda": false
        },
        {
          "number": 4037,
          "text": "قُلْ هُوَ نَبَؤٌا۟ عَظِيمٌ",
          "numberInSurah": 67,
          "juz": 23,
          "manzil": 6,
          "page": 457,
          "ruku": 395,
          "hizbQuarter": 183,
          "sajda": false
        },
        {
          "number": 4038,
          "text": "أَنتُمْ عَنْهُ مُعْرِضُونَ",
          "numberInSurah": 68,
          "juz": 23,
          "manzil": 6,
          "page": 457,
          "ruku": 395,
          "hizbQuarter": 183,
          "sajda": false
        },
        {
          "number": 4039,
          "text": "مَا كَانَ لِىَ مِنْ عِلْمٍۭ بِٱلْمَلَإِ ٱلْأَعْلَىٰٓ إِذْ يَخْتَصِمُونَ",
          "numberInSurah": 69,
          "juz": 23,
          "manzil": 6,
          "page": 457,
          "ruku": 395,
          "hizbQuarter": 183,
          "sajda": false
        },
        {
          "number": 4040,
          "text": "إِن يُوحَىٰٓ إِلَىَّ إِلَّآ أَنَّمَآ أَنَا۠ نَذِيرٌۭ مُّبِينٌ",
          "numberInSurah": 70,
          "juz": 23,
          "manzil": 6,
          "page": 457,
          "ruku": 395,
          "hizbQuarter": 183,
          "sajda": false
        },
        {
          "number": 4041,
          "text": "إِذْ قَالَ رَبُّكَ لِلْمَلَٰٓئِكَةِ إِنِّى خَٰلِقٌۢ بَشَرًۭا مِّن طِينٍۢ",
          "numberInSurah": 71,
          "juz": 23,
          "manzil": 6,
          "page": 457,
          "ruku": 395,
          "hizbQuarter": 183,
          "sajda": false
        },
        {
          "number": 4042,
          "text": "فَإِذَا سَوَّيْتُهُۥ وَنَفَخْتُ فِيهِ مِن رُّوحِى فَقَعُوا۟ لَهُۥ سَٰجِدِينَ",
          "numberInSurah": 72,
          "juz": 23,
          "manzil": 6,
          "page": 457,
          "ruku": 395,
          "hizbQuarter": 183,
          "sajda": false
        },
        {
          "number": 4043,
          "text": "فَسَجَدَ ٱلْمَلَٰٓئِكَةُ كُلُّهُمْ أَجْمَعُونَ",
          "numberInSurah": 73,
          "juz": 23,
          "manzil": 6,
          "page": 457,
          "ruku": 395,
          "hizbQuarter": 183,
          "sajda": false
        },
        {
          "number": 4044,
          "text": "إِلَّآ إِبْلِيسَ ٱسْتَكْبَرَ وَكَانَ مِنَ ٱلْكَٰفِرِينَ",
          "numberInSurah": 74,
          "juz": 23,
          "manzil": 6,
          "page": 457,
          "ruku": 395,
          "hizbQuarter": 183,
          "sajda": false
        },
        {
          "number": 4045,
          "text": "قَالَ يَٰٓإِبْلِيسُ مَا مَنَعَكَ أَن تَسْجُدَ لِمَا خَلَقْتُ بِيَدَىَّ ۖ أَسْتَكْبَرْتَ أَمْ كُنتَ مِنَ ٱلْعَالِينَ",
          "numberInSurah": 75,
          "juz": 23,
          "manzil": 6,
          "page": 457,
          "ruku": 395,
          "hizbQuarter": 183,
          "sajda": false
        },
        {
          "number": 4046,
          "text": "قَالَ أَنَا۠ خَيْرٌۭ مِّنْهُ ۖ خَلَقْتَنِى مِن نَّارٍۢ وَخَلَقْتَهُۥ مِن طِينٍۢ",
          "numberInSurah": 76,
          "juz": 23,
          "manzil": 6,
          "page": 457,
          "ruku": 395,
          "hizbQuarter": 183,
          "sajda": false
        },
        {
          "number": 4047,
          "text": "قَالَ فَٱخْرُجْ مِنْهَا فَإِنَّكَ رَجِيمٌۭ",
          "numberInSurah": 77,
          "juz": 23,
          "manzil": 6,
          "page": 457,
          "ruku": 395,
          "hizbQuarter": 183,
          "sajda": false
        },
        {
          "number": 4048,
          "text": "وَإِنَّ عَلَيْكَ لَعْنَتِىٓ إِلَىٰ يَوْمِ ٱلدِّينِ",
          "numberInSurah": 78,
          "juz": 23,
          "manzil": 6,
          "page": 457,
          "ruku": 395,
          "hizbQuarter": 183,
          "sajda": false
        },
        {
          "number": 4049,
          "text": "قَالَ رَبِّ فَأَنظِرْنِىٓ إِلَىٰ يَوْمِ يُبْعَثُونَ",
          "numberInSurah": 79,
          "juz": 23,
          "manzil": 6,
          "page": 457,
          "ruku": 395,
          "hizbQuarter": 183,
          "sajda": false
        },
        {
          "number": 4050,
          "text": "قَالَ فَإِنَّكَ مِنَ ٱلْمُنظَرِينَ",
          "numberInSurah": 80,
          "juz": 23,
          "manzil": 6,
          "page": 457,
          "ruku": 395,
          "hizbQuarter": 183,
          "sajda": false
        },
        {
          "number": 4051,
          "text": "إِلَىٰ يَوْمِ ٱلْوَقْتِ ٱلْمَعْلُومِ",
          "numberInSurah": 81,
          "juz": 23,
          "manzil": 6,
          "page": 457,
          "ruku": 395,
          "hizbQuarter": 183,
          "sajda": false
        },
        {
          "number": 4052,
          "text": "قَالَ فَبِعِزَّتِكَ لَأُغْوِيَنَّهُمْ أَجْمَعِينَ",
          "numberInSurah": 82,
          "juz": 23,
          "manzil": 6,
          "page": 457,
          "ruku": 395,
          "hizbQuarter": 183,
          "sajda": false
        },
        {
          "number": 4053,
          "text": "إِلَّا عِبَادَكَ مِنْهُمُ ٱلْمُخْلَصِينَ",
          "numberInSurah": 83,
          "juz": 23,
          "manzil": 6,
          "page": 457,
          "ruku": 395,
          "hizbQuarter": 183,
          "sajda": false
        },
        {
          "number": 4054,
          "text": "قَالَ فَٱلْحَقُّ وَٱلْحَقَّ أَقُولُ",
          "numberInSurah": 84,
          "juz": 23,
          "manzil": 6,
          "page": 458,
          "ruku": 395,
          "hizbQuarter": 183,
          "sajda": false
        },
        {
          "number": 4055,
          "text": "لَأَمْلَأَنَّ جَهَنَّمَ مِنكَ وَمِمَّن تَبِعَكَ مِنْهُمْ أَجْمَعِينَ",
          "numberInSurah": 85,
          "juz": 23,
          "manzil": 6,
          "page": 458,
          "ruku": 395,
          "hizbQuarter": 183,
          "sajda": false
        },
        {
          "number": 4056,
          "text": "قُلْ مَآ أَسْـَٔلُكُمْ عَلَيْهِ مِنْ أَجْرٍۢ وَمَآ أَنَا۠ مِنَ ٱلْمُتَكَلِّفِينَ",
          "numberInSurah": 86,
          "juz": 23,
          "manzil": 6,
          "page": 458,
          "ruku": 395,
          "hizbQuarter": 183,
          "sajda": false
        },
        {
          "number": 4057,
          "text": "إِنْ هُوَ إِلَّا ذِكْرٌۭ لِّلْعَٰلَمِينَ",
          "numberInSurah": 87,
          "juz": 23,
          "manzil": 6,
          "page": 458,
          "ruku": 395,
          "hizbQuarter": 183,
          "sajda": false
        },
        {
          "number": 4058,
          "text": "وَلَتَعْلَمُنَّ نَبَأَهُۥ بَعْدَ حِينٍۭ",
          "numberInSurah": 88,
          "juz": 23,
          "manzil": 6,
          "page": 458,
          "ruku": 395,
          "hizbQuarter": 183,
          "sajda": false
        }
