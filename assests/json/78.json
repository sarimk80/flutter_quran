  "data": [
    {
      "number": 78,
      "name": "سورة النبإ",
      "englishName": "An-Naba",
      "englishNameTranslation": "The Announcement",
      "revelationType": "Meccan",
      "numberOfAyahs": 40,
      "ayahs": [
        {
          "number": 5673,
          "text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ عَمَّ يَتَسَآءَلُونَ",
          "numberInSurah": 1,
          "juz": 30,
          "manzil": 7,
          "page": 582,
          "ruku": 518,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5674,
          "text": "عَنِ ٱلنَّبَإِ ٱلْعَظِيمِ",
          "numberInSurah": 2,
          "juz": 30,
          "manzil": 7,
          "page": 582,
          "ruku": 518,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5675,
          "text": "ٱلَّذِى هُمْ فِيهِ مُخْتَلِفُونَ",
          "numberInSurah": 3,
          "juz": 30,
          "manzil": 7,
          "page": 582,
          "ruku": 518,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5676,
          "text": "كَلَّا سَيَعْلَمُونَ",
          "numberInSurah": 4,
          "juz": 30,
          "manzil": 7,
          "page": 582,
          "ruku": 518,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5677,
          "text": "ثُمَّ كَلَّا سَيَعْلَمُونَ",
          "numberInSurah": 5,
          "juz": 30,
          "manzil": 7,
          "page": 582,
          "ruku": 518,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5678,
          "text": "أَلَمْ نَجْعَلِ ٱلْأَرْضَ مِهَٰدًۭا",
          "numberInSurah": 6,
          "juz": 30,
          "manzil": 7,
          "page": 582,
          "ruku": 518,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5679,
          "text": "وَٱلْجِبَالَ أَوْتَادًۭا",
          "numberInSurah": 7,
          "juz": 30,
          "manzil": 7,
          "page": 582,
          "ruku": 518,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5680,
          "text": "وَخَلَقْنَٰكُمْ أَزْوَٰجًۭا",
          "numberInSurah": 8,
          "juz": 30,
          "manzil": 7,
          "page": 582,
          "ruku": 518,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5681,
          "text": "وَجَعَلْنَا نَوْمَكُمْ سُبَاتًۭا",
          "numberInSurah": 9,
          "juz": 30,
          "manzil": 7,
          "page": 582,
          "ruku": 518,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5682,
          "text": "وَجَعَلْنَا ٱلَّيْلَ لِبَاسًۭا",
          "numberInSurah": 10,
          "juz": 30,
          "manzil": 7,
          "page": 582,
          "ruku": 518,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5683,
          "text": "وَجَعَلْنَا ٱلنَّهَارَ مَعَاشًۭا",
          "numberInSurah": 11,
          "juz": 30,
          "manzil": 7,
          "page": 582,
          "ruku": 518,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5684,
          "text": "وَبَنَيْنَا فَوْقَكُمْ سَبْعًۭا شِدَادًۭا",
          "numberInSurah": 12,
          "juz": 30,
          "manzil": 7,
          "page": 582,
          "ruku": 518,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5685,
          "text": "وَجَعَلْنَا سِرَاجًۭا وَهَّاجًۭا",
          "numberInSurah": 13,
          "juz": 30,
          "manzil": 7,
          "page": 582,
          "ruku": 518,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5686,
          "text": "وَأَنزَلْنَا مِنَ ٱلْمُعْصِرَٰتِ مَآءًۭ ثَجَّاجًۭا",
          "numberInSurah": 14,
          "juz": 30,
          "manzil": 7,
          "page": 582,
          "ruku": 518,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5687,
          "text": "لِّنُخْرِجَ بِهِۦ حَبًّۭا وَنَبَاتًۭا",
          "numberInSurah": 15,
          "juz": 30,
          "manzil": 7,
          "page": 582,
          "ruku": 518,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5688,
          "text": "وَجَنَّٰتٍ أَلْفَافًا",
          "numberInSurah": 16,
          "juz": 30,
          "manzil": 7,
          "page": 582,
          "ruku": 518,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5689,
          "text": "إِنَّ يَوْمَ ٱلْفَصْلِ كَانَ مِيقَٰتًۭا",
          "numberInSurah": 17,
          "juz": 30,
          "manzil": 7,
          "page": 582,
          "ruku": 518,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5690,
          "text": "يَوْمَ يُنفَخُ فِى ٱلصُّورِ فَتَأْتُونَ أَفْوَاجًۭا",
          "numberInSurah": 18,
          "juz": 30,
          "manzil": 7,
          "page": 582,
          "ruku": 518,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5691,
          "text": "وَفُتِحَتِ ٱلسَّمَآءُ فَكَانَتْ أَبْوَٰبًۭا",
          "numberInSurah": 19,
          "juz": 30,
          "manzil": 7,
          "page": 582,
          "ruku": 518,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5692,
          "text": "وَسُيِّرَتِ ٱلْجِبَالُ فَكَانَتْ سَرَابًا",
          "numberInSurah": 20,
          "juz": 30,
          "manzil": 7,
          "page": 582,
          "ruku": 518,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5693,
          "text": "إِنَّ جَهَنَّمَ كَانَتْ مِرْصَادًۭا",
          "numberInSurah": 21,
          "juz": 30,
          "manzil": 7,
          "page": 582,
          "ruku": 518,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5694,
          "text": "لِّلطَّٰغِينَ مَـَٔابًۭا",
          "numberInSurah": 22,
          "juz": 30,
          "manzil": 7,
          "page": 582,
          "ruku": 518,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5695,
          "text": "لَّٰبِثِينَ فِيهَآ أَحْقَابًۭا",
          "numberInSurah": 23,
          "juz": 30,
          "manzil": 7,
          "page": 582,
          "ruku": 518,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5696,
          "text": "لَّا يَذُوقُونَ فِيهَا بَرْدًۭا وَلَا شَرَابًا",
          "numberInSurah": 24,
          "juz": 30,
          "manzil": 7,
          "page": 582,
          "ruku": 518,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5697,
          "text": "إِلَّا حَمِيمًۭا وَغَسَّاقًۭا",
          "numberInSurah": 25,
          "juz": 30,
          "manzil": 7,
          "page": 582,
          "ruku": 518,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5698,
          "text": "جَزَآءًۭ وِفَاقًا",
          "numberInSurah": 26,
          "juz": 30,
          "manzil": 7,
          "page": 582,
          "ruku": 518,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5699,
          "text": "إِنَّهُمْ كَانُوا۟ لَا يَرْجُونَ حِسَابًۭا",
          "numberInSurah": 27,
          "juz": 30,
          "manzil": 7,
          "page": 582,
          "ruku": 518,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5700,
          "text": "وَكَذَّبُوا۟ بِـَٔايَٰتِنَا كِذَّابًۭا",
          "numberInSurah": 28,
          "juz": 30,
          "manzil": 7,
          "page": 582,
          "ruku": 518,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5701,
          "text": "وَكُلَّ شَىْءٍ أَحْصَيْنَٰهُ كِتَٰبًۭا",
          "numberInSurah": 29,
          "juz": 30,
          "manzil": 7,
          "page": 582,
          "ruku": 518,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5702,
          "text": "فَذُوقُوا۟ فَلَن نَّزِيدَكُمْ إِلَّا عَذَابًا",
          "numberInSurah": 30,
          "juz": 30,
          "manzil": 7,
          "page": 582,
          "ruku": 518,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5703,
          "text": "إِنَّ لِلْمُتَّقِينَ مَفَازًا",
          "numberInSurah": 31,
          "juz": 30,
          "manzil": 7,
          "page": 583,
          "ruku": 519,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5704,
          "text": "حَدَآئِقَ وَأَعْنَٰبًۭا",
          "numberInSurah": 32,
          "juz": 30,
          "manzil": 7,
          "page": 583,
          "ruku": 519,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5705,
          "text": "وَكَوَاعِبَ أَتْرَابًۭا",
          "numberInSurah": 33,
          "juz": 30,
          "manzil": 7,
          "page": 583,
          "ruku": 519,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5706,
          "text": "وَكَأْسًۭا دِهَاقًۭا",
          "numberInSurah": 34,
          "juz": 30,
          "manzil": 7,
          "page": 583,
          "ruku": 519,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5707,
          "text": "لَّا يَسْمَعُونَ فِيهَا لَغْوًۭا وَلَا كِذَّٰبًۭا",
          "numberInSurah": 35,
          "juz": 30,
          "manzil": 7,
          "page": 583,
          "ruku": 519,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5708,
          "text": "جَزَآءًۭ مِّن رَّبِّكَ عَطَآءً حِسَابًۭا",
          "numberInSurah": 36,
          "juz": 30,
          "manzil": 7,
          "page": 583,
          "ruku": 519,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5709,
          "text": "رَّبِّ ٱلسَّمَٰوَٰتِ وَٱلْأَرْضِ وَمَا بَيْنَهُمَا ٱلرَّحْمَٰنِ ۖ لَا يَمْلِكُونَ مِنْهُ خِطَابًۭا",
          "numberInSurah": 37,
          "juz": 30,
          "manzil": 7,
          "page": 583,
          "ruku": 519,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5710,
          "text": "يَوْمَ يَقُومُ ٱلرُّوحُ وَٱلْمَلَٰٓئِكَةُ صَفًّۭا ۖ لَّا يَتَكَلَّمُونَ إِلَّا مَنْ أَذِنَ لَهُ ٱلرَّحْمَٰنُ وَقَالَ صَوَابًۭا",
          "numberInSurah": 38,
          "juz": 30,
          "manzil": 7,
          "page": 583,
          "ruku": 519,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5711,
          "text": "ذَٰلِكَ ٱلْيَوْمُ ٱلْحَقُّ ۖ فَمَن شَآءَ ٱتَّخَذَ إِلَىٰ رَبِّهِۦ مَـَٔابًا",
          "numberInSurah": 39,
          "juz": 30,
          "manzil": 7,
          "page": 583,
          "ruku": 519,
          "hizbQuarter": 233,
          "sajda": false
        },
        {
          "number": 5712,
          "text": "إِنَّآ أَنذَرْنَٰكُمْ عَذَابًۭا قَرِيبًۭا يَوْمَ يَنظُرُ ٱلْمَرْءُ مَا قَدَّمَتْ يَدَاهُ وَيَقُولُ ٱلْكَافِرُ يَٰلَيْتَنِى كُنتُ تُرَٰبًۢا",
          "numberInSurah": 40,
          "juz": 30,
          "manzil": 7,
          "page": 583,
          "ruku": 519,
          "hizbQuarter": 233,
          "sajda": false
        }
