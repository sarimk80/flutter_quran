      "number": 50,
      "name": "سورة ق",
      "englishName": "Qaaf",
      "englishNameTranslation": "The letter Qaaf",
      "revelationType": "Meccan",
      "numberOfAyahs": 45,
      "ayahs": [
        {
          "number": 4631,
          "text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ قٓ ۚ وَٱلْقُرْءَانِ ٱلْمَجِيدِ",
          "numberInSurah": 1,
          "juz": 26,
          "manzil": 7,
          "page": 518,
          "ruku": 452,
          "hizbQuarter": 207,
          "sajda": false
        },
        {
          "number": 4632,
          "text": "بَلْ عَجِبُوٓا۟ أَن جَآءَهُم مُّنذِرٌۭ مِّنْهُمْ فَقَالَ ٱلْكَٰفِرُونَ هَٰذَا شَىْءٌ عَجِيبٌ",
          "numberInSurah": 2,
          "juz": 26,
          "manzil": 7,
          "page": 518,
          "ruku": 452,
          "hizbQuarter": 207,
          "sajda": false
        },
        {
          "number": 4633,
          "text": "أَءِذَا مِتْنَا وَكُنَّا تُرَابًۭا ۖ ذَٰلِكَ رَجْعٌۢ بَعِيدٌۭ",
          "numberInSurah": 3,
          "juz": 26,
          "manzil": 7,
          "page": 518,
          "ruku": 452,
          "hizbQuarter": 207,
          "sajda": false
        },
        {
          "number": 4634,
          "text": "قَدْ عَلِمْنَا مَا تَنقُصُ ٱلْأَرْضُ مِنْهُمْ ۖ وَعِندَنَا كِتَٰبٌ حَفِيظٌۢ",
          "numberInSurah": 4,
          "juz": 26,
          "manzil": 7,
          "page": 518,
          "ruku": 452,
          "hizbQuarter": 207,
          "sajda": false
        },
        {
          "number": 4635,
          "text": "بَلْ كَذَّبُوا۟ بِٱلْحَقِّ لَمَّا جَآءَهُمْ فَهُمْ فِىٓ أَمْرٍۢ مَّرِيجٍ",
          "numberInSurah": 5,
          "juz": 26,
          "manzil": 7,
          "page": 518,
          "ruku": 452,
          "hizbQuarter": 207,
          "sajda": false
        },
        {
          "number": 4636,
          "text": "أَفَلَمْ يَنظُرُوٓا۟ إِلَى ٱلسَّمَآءِ فَوْقَهُمْ كَيْفَ بَنَيْنَٰهَا وَزَيَّنَّٰهَا وَمَا لَهَا مِن فُرُوجٍۢ",
          "numberInSurah": 6,
          "juz": 26,
          "manzil": 7,
          "page": 518,
          "ruku": 452,
          "hizbQuarter": 207,
          "sajda": false
        },
        {
          "number": 4637,
          "text": "وَٱلْأَرْضَ مَدَدْنَٰهَا وَأَلْقَيْنَا فِيهَا رَوَٰسِىَ وَأَنۢبَتْنَا فِيهَا مِن كُلِّ زَوْجٍۭ بَهِيجٍۢ",
          "numberInSurah": 7,
          "juz": 26,
          "manzil": 7,
          "page": 518,
          "ruku": 452,
          "hizbQuarter": 207,
          "sajda": false
        },
        {
          "number": 4638,
          "text": "تَبْصِرَةًۭ وَذِكْرَىٰ لِكُلِّ عَبْدٍۢ مُّنِيبٍۢ",
          "numberInSurah": 8,
          "juz": 26,
          "manzil": 7,
          "page": 518,
          "ruku": 452,
          "hizbQuarter": 207,
          "sajda": false
        },
        {
          "number": 4639,
          "text": "وَنَزَّلْنَا مِنَ ٱلسَّمَآءِ مَآءًۭ مُّبَٰرَكًۭا فَأَنۢبَتْنَا بِهِۦ جَنَّٰتٍۢ وَحَبَّ ٱلْحَصِيدِ",
          "numberInSurah": 9,
          "juz": 26,
          "manzil": 7,
          "page": 518,
          "ruku": 452,
          "hizbQuarter": 207,
          "sajda": false
        },
        {
          "number": 4640,
          "text": "وَٱلنَّخْلَ بَاسِقَٰتٍۢ لَّهَا طَلْعٌۭ نَّضِيدٌۭ",
          "numberInSurah": 10,
          "juz": 26,
          "manzil": 7,
          "page": 518,
          "ruku": 452,
          "hizbQuarter": 207,
          "sajda": false
        },
        {
          "number": 4641,
          "text": "رِّزْقًۭا لِّلْعِبَادِ ۖ وَأَحْيَيْنَا بِهِۦ بَلْدَةًۭ مَّيْتًۭا ۚ كَذَٰلِكَ ٱلْخُرُوجُ",
          "numberInSurah": 11,
          "juz": 26,
          "manzil": 7,
          "page": 518,
          "ruku": 452,
          "hizbQuarter": 207,
          "sajda": false
        },
        {
          "number": 4642,
          "text": "كَذَّبَتْ قَبْلَهُمْ قَوْمُ نُوحٍۢ وَأَصْحَٰبُ ٱلرَّسِّ وَثَمُودُ",
          "numberInSurah": 12,
          "juz": 26,
          "manzil": 7,
          "page": 518,
          "ruku": 452,
          "hizbQuarter": 207,
          "sajda": false
        },
        {
          "number": 4643,
          "text": "وَعَادٌۭ وَفِرْعَوْنُ وَإِخْوَٰنُ لُوطٍۢ",
          "numberInSurah": 13,
          "juz": 26,
          "manzil": 7,
          "page": 518,
          "ruku": 452,
          "hizbQuarter": 207,
          "sajda": false
        },
        {
          "number": 4644,
          "text": "وَأَصْحَٰبُ ٱلْأَيْكَةِ وَقَوْمُ تُبَّعٍۢ ۚ كُلٌّۭ كَذَّبَ ٱلرُّسُلَ فَحَقَّ وَعِيدِ",
          "numberInSurah": 14,
          "juz": 26,
          "manzil": 7,
          "page": 518,
          "ruku": 452,
          "hizbQuarter": 207,
          "sajda": false
        },
        {
          "number": 4645,
          "text": "أَفَعَيِينَا بِٱلْخَلْقِ ٱلْأَوَّلِ ۚ بَلْ هُمْ فِى لَبْسٍۢ مِّنْ خَلْقٍۢ جَدِيدٍۢ",
          "numberInSurah": 15,
          "juz": 26,
          "manzil": 7,
          "page": 518,
          "ruku": 452,
          "hizbQuarter": 207,
          "sajda": false
        },
        {
          "number": 4646,
          "text": "وَلَقَدْ خَلَقْنَا ٱلْإِنسَٰنَ وَنَعْلَمُ مَا تُوَسْوِسُ بِهِۦ نَفْسُهُۥ ۖ وَنَحْنُ أَقْرَبُ إِلَيْهِ مِنْ حَبْلِ ٱلْوَرِيدِ",
          "numberInSurah": 16,
          "juz": 26,
          "manzil": 7,
          "page": 519,
          "ruku": 453,
          "hizbQuarter": 207,
          "sajda": false
        },
        {
          "number": 4647,
          "text": "إِذْ يَتَلَقَّى ٱلْمُتَلَقِّيَانِ عَنِ ٱلْيَمِينِ وَعَنِ ٱلشِّمَالِ قَعِيدٌۭ",
          "numberInSurah": 17,
          "juz": 26,
          "manzil": 7,
          "page": 519,
          "ruku": 453,
          "hizbQuarter": 207,
          "sajda": false
        },
        {
          "number": 4648,
          "text": "مَّا يَلْفِظُ مِن قَوْلٍ إِلَّا لَدَيْهِ رَقِيبٌ عَتِيدٌۭ",
          "numberInSurah": 18,
          "juz": 26,
          "manzil": 7,
          "page": 519,
          "ruku": 453,
          "hizbQuarter": 207,
          "sajda": false
        },
        {
          "number": 4649,
          "text": "وَجَآءَتْ سَكْرَةُ ٱلْمَوْتِ بِٱلْحَقِّ ۖ ذَٰلِكَ مَا كُنتَ مِنْهُ تَحِيدُ",
          "numberInSurah": 19,
          "juz": 26,
          "manzil": 7,
          "page": 519,
          "ruku": 453,
          "hizbQuarter": 207,
          "sajda": false
        },
        {
          "number": 4650,
          "text": "وَنُفِخَ فِى ٱلصُّورِ ۚ ذَٰلِكَ يَوْمُ ٱلْوَعِيدِ",
          "numberInSurah": 20,
          "juz": 26,
          "manzil": 7,
          "page": 519,
          "ruku": 453,
          "hizbQuarter": 207,
          "sajda": false
        },
        {
          "number": 4651,
          "text": "وَجَآءَتْ كُلُّ نَفْسٍۢ مَّعَهَا سَآئِقٌۭ وَشَهِيدٌۭ",
          "numberInSurah": 21,
          "juz": 26,
          "manzil": 7,
          "page": 519,
          "ruku": 453,
          "hizbQuarter": 207,
          "sajda": false
        },
        {
          "number": 4652,
          "text": "لَّقَدْ كُنتَ فِى غَفْلَةٍۢ مِّنْ هَٰذَا فَكَشَفْنَا عَنكَ غِطَآءَكَ فَبَصَرُكَ ٱلْيَوْمَ حَدِيدٌۭ",
          "numberInSurah": 22,
          "juz": 26,
          "manzil": 7,
          "page": 519,
          "ruku": 453,
          "hizbQuarter": 207,
          "sajda": false
        },
        {
          "number": 4653,
          "text": "وَقَالَ قَرِينُهُۥ هَٰذَا مَا لَدَىَّ عَتِيدٌ",
          "numberInSurah": 23,
          "juz": 26,
          "manzil": 7,
          "page": 519,
          "ruku": 453,
          "hizbQuarter": 207,
          "sajda": false
        },
        {
          "number": 4654,
          "text": "أَلْقِيَا فِى جَهَنَّمَ كُلَّ كَفَّارٍ عَنِيدٍۢ",
          "numberInSurah": 24,
          "juz": 26,
          "manzil": 7,
          "page": 519,
          "ruku": 453,
          "hizbQuarter": 207,
          "sajda": false
        },
        {
          "number": 4655,
          "text": "مَّنَّاعٍۢ لِّلْخَيْرِ مُعْتَدٍۢ مُّرِيبٍ",
          "numberInSurah": 25,
          "juz": 26,
          "manzil": 7,
          "page": 519,
          "ruku": 453,
          "hizbQuarter": 207,
          "sajda": false
        },
        {
          "number": 4656,
          "text": "ٱلَّذِى جَعَلَ مَعَ ٱللَّهِ إِلَٰهًا ءَاخَرَ فَأَلْقِيَاهُ فِى ٱلْعَذَابِ ٱلشَّدِيدِ",
          "numberInSurah": 26,
          "juz": 26,
          "manzil": 7,
          "page": 519,
          "ruku": 453,
          "hizbQuarter": 207,
          "sajda": false
        },
        {
          "number": 4657,
          "text": "۞ قَالَ قَرِينُهُۥ رَبَّنَا مَآ أَطْغَيْتُهُۥ وَلَٰكِن كَانَ فِى ضَلَٰلٍۭ بَعِيدٍۢ",
          "numberInSurah": 27,
          "juz": 26,
          "manzil": 7,
          "page": 519,
          "ruku": 453,
          "hizbQuarter": 208,
          "sajda": false
        },
        {
          "number": 4658,
          "text": "قَالَ لَا تَخْتَصِمُوا۟ لَدَىَّ وَقَدْ قَدَّمْتُ إِلَيْكُم بِٱلْوَعِيدِ",
          "numberInSurah": 28,
          "juz": 26,
          "manzil": 7,
          "page": 519,
          "ruku": 453,
          "hizbQuarter": 208,
          "sajda": false
        },
        {
          "number": 4659,
          "text": "مَا يُبَدَّلُ ٱلْقَوْلُ لَدَىَّ وَمَآ أَنَا۠ بِظَلَّٰمٍۢ لِّلْعَبِيدِ",
          "numberInSurah": 29,
          "juz": 26,
          "manzil": 7,
          "page": 519,
          "ruku": 453,
          "hizbQuarter": 208,
          "sajda": false
        },
        {
          "number": 4660,
          "text": "يَوْمَ نَقُولُ لِجَهَنَّمَ هَلِ ٱمْتَلَأْتِ وَتَقُولُ هَلْ مِن مَّزِيدٍۢ",
          "numberInSurah": 30,
          "juz": 26,
          "manzil": 7,
          "page": 519,
          "ruku": 454,
          "hizbQuarter": 208,
          "sajda": false
        },
        {
          "number": 4661,
          "text": "وَأُزْلِفَتِ ٱلْجَنَّةُ لِلْمُتَّقِينَ غَيْرَ بَعِيدٍ",
          "numberInSurah": 31,
          "juz": 26,
          "manzil": 7,
          "page": 519,
          "ruku": 454,
          "hizbQuarter": 208,
          "sajda": false
        },
        {
          "number": 4662,
          "text": "هَٰذَا مَا تُوعَدُونَ لِكُلِّ أَوَّابٍ حَفِيظٍۢ",
          "numberInSurah": 32,
          "juz": 26,
          "manzil": 7,
          "page": 519,
          "ruku": 454,
          "hizbQuarter": 208,
          "sajda": false
        },
        {
          "number": 4663,
          "text": "مَّنْ خَشِىَ ٱلرَّحْمَٰنَ بِٱلْغَيْبِ وَجَآءَ بِقَلْبٍۢ مُّنِيبٍ",
          "numberInSurah": 33,
          "juz": 26,
          "manzil": 7,
          "page": 519,
          "ruku": 454,
          "hizbQuarter": 208,
          "sajda": false
        },
        {
          "number": 4664,
          "text": "ٱدْخُلُوهَا بِسَلَٰمٍۢ ۖ ذَٰلِكَ يَوْمُ ٱلْخُلُودِ",
          "numberInSurah": 34,
          "juz": 26,
          "manzil": 7,
          "page": 519,
          "ruku": 454,
          "hizbQuarter": 208,
          "sajda": false
        },
        {
          "number": 4665,
          "text": "لَهُم مَّا يَشَآءُونَ فِيهَا وَلَدَيْنَا مَزِيدٌۭ",
          "numberInSurah": 35,
          "juz": 26,
          "manzil": 7,
          "page": 519,
          "ruku": 454,
          "hizbQuarter": 208,
          "sajda": false
        },
        {
          "number": 4666,
          "text": "وَكَمْ أَهْلَكْنَا قَبْلَهُم مِّن قَرْنٍ هُمْ أَشَدُّ مِنْهُم بَطْشًۭا فَنَقَّبُوا۟ فِى ٱلْبِلَٰدِ هَلْ مِن مَّحِيصٍ",
          "numberInSurah": 36,
          "juz": 26,
          "manzil": 7,
          "page": 520,
          "ruku": 454,
          "hizbQuarter": 208,
          "sajda": false
        },
        {
          "number": 4667,
          "text": "إِنَّ فِى ذَٰلِكَ لَذِكْرَىٰ لِمَن كَانَ لَهُۥ قَلْبٌ أَوْ أَلْقَى ٱلسَّمْعَ وَهُوَ شَهِيدٌۭ",
          "numberInSurah": 37,
          "juz": 26,
          "manzil": 7,
          "page": 520,
          "ruku": 454,
          "hizbQuarter": 208,
          "sajda": false
        },
        {
          "number": 4668,
          "text": "وَلَقَدْ خَلَقْنَا ٱلسَّمَٰوَٰتِ وَٱلْأَرْضَ وَمَا بَيْنَهُمَا فِى سِتَّةِ أَيَّامٍۢ وَمَا مَسَّنَا مِن لُّغُوبٍۢ",
          "numberInSurah": 38,
          "juz": 26,
          "manzil": 7,
          "page": 520,
          "ruku": 454,
          "hizbQuarter": 208,
          "sajda": false
        },
        {
          "number": 4669,
          "text": "فَٱصْبِرْ عَلَىٰ مَا يَقُولُونَ وَسَبِّحْ بِحَمْدِ رَبِّكَ قَبْلَ طُلُوعِ ٱلشَّمْسِ وَقَبْلَ ٱلْغُرُوبِ",
          "numberInSurah": 39,
          "juz": 26,
          "manzil": 7,
          "page": 520,
          "ruku": 454,
          "hizbQuarter": 208,
          "sajda": false
        },
        {
          "number": 4670,
          "text": "وَمِنَ ٱلَّيْلِ فَسَبِّحْهُ وَأَدْبَٰرَ ٱلسُّجُودِ",
          "numberInSurah": 40,
          "juz": 26,
          "manzil": 7,
          "page": 520,
          "ruku": 454,
          "hizbQuarter": 208,
          "sajda": false
        },
        {
          "number": 4671,
          "text": "وَٱسْتَمِعْ يَوْمَ يُنَادِ ٱلْمُنَادِ مِن مَّكَانٍۢ قَرِيبٍۢ",
          "numberInSurah": 41,
          "juz": 26,
          "manzil": 7,
          "page": 520,
          "ruku": 454,
          "hizbQuarter": 208,
          "sajda": false
        },
        {
          "number": 4672,
          "text": "يَوْمَ يَسْمَعُونَ ٱلصَّيْحَةَ بِٱلْحَقِّ ۚ ذَٰلِكَ يَوْمُ ٱلْخُرُوجِ",
          "numberInSurah": 42,
          "juz": 26,
          "manzil": 7,
          "page": 520,
          "ruku": 454,
          "hizbQuarter": 208,
          "sajda": false
        },
        {
          "number": 4673,
          "text": "إِنَّا نَحْنُ نُحْىِۦ وَنُمِيتُ وَإِلَيْنَا ٱلْمَصِيرُ",
          "numberInSurah": 43,
          "juz": 26,
          "manzil": 7,
          "page": 520,
          "ruku": 454,
          "hizbQuarter": 208,
          "sajda": false
        },
        {
          "number": 4674,
          "text": "يَوْمَ تَشَقَّقُ ٱلْأَرْضُ عَنْهُمْ سِرَاعًۭا ۚ ذَٰلِكَ حَشْرٌ عَلَيْنَا يَسِيرٌۭ",
          "numberInSurah": 44,
          "juz": 26,
          "manzil": 7,
          "page": 520,
          "ruku": 454,
          "hizbQuarter": 208,
          "sajda": false
        },
        {
          "number": 4675,
          "text": "نَّحْنُ أَعْلَمُ بِمَا يَقُولُونَ ۖ وَمَآ أَنتَ عَلَيْهِم بِجَبَّارٍۢ ۖ فَذَكِّرْ بِٱلْقُرْءَانِ مَن يَخَافُ وَعِيدِ",
          "numberInSurah": 45,
          "juz": 26,
          "manzil": 7,
