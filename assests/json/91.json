          "ruku": 533,
          "hizbQuarter": 238,
          "sajda": false
        },
        {
          "number": 6049,
          "text": "وَٱلْأَرْضِ وَمَا طَحَىٰهَا",
          "numberInSurah": 6,
          "juz": 30,
          "manzil": 7,
          "page": 595,
          "ruku": 533,
          "hizbQuarter": 238,
          "sajda": false
        },
        {
          "number": 6050,
          "text": "وَنَفْسٍۢ وَمَا سَوَّىٰهَا",
