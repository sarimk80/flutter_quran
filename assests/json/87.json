      "numberOfAyahs": 19,
      "ayahs": [
        {
          "number": 5949,
          "text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ سَبِّحِ ٱسْمَ رَبِّكَ ٱلْأَعْلَى",
          "numberInSurah": 1,
          "juz": 30,
          "manzil": 7,
          "page": 591,
          "ruku": 529,
          "hizbQuarter": 237,
          "sajda": false
        },
        {
          "number": 5950,
          "text": "ٱلَّذِى خَلَقَ فَسَوَّىٰ",
          "numberInSurah": 2,
          "juz": 30,
          "manzil": 7,
          "page": 591,
          "ruku": 529,
          "hizbQuarter": 237,
          "sajda": false
        },
        {
          "number": 5951,
          "text": "وَٱلَّذِى قَدَّرَ فَهَدَىٰ",
          "numberInSurah": 3,
          "juz": 30,
          "manzil": 7,
          "page": 591,
          "ruku": 529,
          "hizbQuarter": 237,
          "sajda": false
        },
        {
          "number": 5952,
          "text": "وَٱلَّذِىٓ أَخْرَجَ ٱلْمَرْعَىٰ",
          "numberInSurah": 4,
          "juz": 30,
          "manzil": 7,
          "page": 591,
          "ruku": 529,
          "hizbQuarter": 237,
          "sajda": false
        },
        {
          "number": 5953,
          "text": "فَجَعَلَهُۥ غُثَآءً أَحْوَىٰ",
          "numberInSurah": 5,
          "juz": 30,
          "manzil": 7,
          "page": 591,
          "ruku": 529,
          "hizbQuarter": 237,
          "sajda": false
        },
        {
          "number": 5954,
          "text": "سَنُقْرِئُكَ فَلَا تَنسَىٰٓ",
          "numberInSurah": 6,
          "juz": 30,
          "manzil": 7,
          "page": 591,
          "ruku": 529,
          "hizbQuarter": 237,
          "sajda": false
        },
        {
          "number": 5955,
          "text": "إِلَّا مَا شَآءَ ٱللَّهُ ۚ إِنَّهُۥ يَعْلَمُ ٱلْجَهْرَ وَمَا يَخْفَىٰ",
          "numberInSurah": 7,
          "juz": 30,
          "manzil": 7,
          "page": 591,
          "ruku": 529,
          "hizbQuarter": 237,
          "sajda": false
        },
        {
          "number": 5956,
          "text": "وَنُيَسِّرُكَ لِلْيُسْرَىٰ",
          "numberInSurah": 8,
          "juz": 30,
          "manzil": 7,
          "page": 591,
          "ruku": 529,
          "hizbQuarter": 237,
          "sajda": false
        },
        {
          "number": 5957,
          "text": "فَذَكِّرْ إِن نَّفَعَتِ ٱلذِّكْرَىٰ",
          "numberInSurah": 9,
          "juz": 30,
          "manzil": 7,
          "page": 591,
          "ruku": 529,
          "hizbQuarter": 237,
          "sajda": false
        },
        {
          "number": 5958,
          "text": "سَيَذَّكَّرُ مَن يَخْشَىٰ",
          "numberInSurah": 10,
          "juz": 30,
          "manzil": 7,
          "page": 591,
          "ruku": 529,
          "hizbQuarter": 237,
          "sajda": false
        },
        {
          "number": 5959,
          "text": "وَيَتَجَنَّبُهَا ٱلْأَشْقَى",
          "numberInSurah": 11,
          "juz": 30,
          "manzil": 7,
          "page": 591,
          "ruku": 529,
          "hizbQuarter": 237,
          "sajda": false
        },
        {
          "number": 5960,
          "text": "ٱلَّذِى يَصْلَى ٱلنَّارَ ٱلْكُبْرَىٰ",
          "numberInSurah": 12,
          "juz": 30,
          "manzil": 7,
          "page": 591,
          "ruku": 529,
          "hizbQuarter": 237,
          "sajda": false
        },
        {
          "number": 5961,
          "text": "ثُمَّ لَا يَمُوتُ فِيهَا وَلَا يَحْيَىٰ",
          "numberInSurah": 13,
          "juz": 30,
          "manzil": 7,
          "page": 591,
          "ruku": 529,
          "hizbQuarter": 237,
          "sajda": false
        },
        {
          "number": 5962,
          "text": "قَدْ أَفْلَحَ مَن تَزَكَّىٰ",
          "numberInSurah": 14,
          "juz": 30,
          "manzil": 7,
          "page": 591,
          "ruku": 529,
          "hizbQuarter": 237,
          "sajda": false
        },
        {
          "number": 5963,
          "text": "وَذَكَرَ ٱسْمَ رَبِّهِۦ فَصَلَّىٰ",
          "numberInSurah": 15,
          "juz": 30,
          "manzil": 7,
          "page": 591,
          "ruku": 529,
          "hizbQuarter": 237,
          "sajda": false
        },
        {
          "number": 5964,
          "text": "بَلْ تُؤْثِرُونَ ٱلْحَيَوٰةَ ٱلدُّنْيَا",
          "numberInSurah": 16,
          "juz": 30,
          "manzil": 7,
          "page": 592,
          "ruku": 529,
          "hizbQuarter": 237,
          "sajda": false
        },
        {
          "number": 5965,
          "text": "وَٱلْءَاخِرَةُ خَيْرٌۭ وَأَبْقَىٰٓ",
          "numberInSurah": 17,
          "juz": 30,
          "manzil": 7,
          "page": 592,
          "ruku": 529,
          "hizbQuarter": 237,
          "sajda": false
        },
        {
          "number": 5966,
          "text": "إِنَّ هَٰذَا لَفِى ٱلصُّحُفِ ٱلْأُولَىٰ",
          "numberInSurah": 18,
          "juz": 30,
