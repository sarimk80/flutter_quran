  "data": [
    {
      "number": 44,
      "name": "سورة الدخان",
      "englishName": "Ad-Dukhaan",
      "englishNameTranslation": "The Smoke",
      "revelationType": "Meccan",
      "numberOfAyahs": 59,
      "ayahs": [
        {
          "number": 4415,
          "text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ حمٓ",
          "numberInSurah": 1,
          "juz": 25,
          "manzil": 6,
          "page": 496,
          "ruku": 431,
          "hizbQuarter": 198,
          "sajda": false
        },
        {
          "number": 4416,
          "text": "وَٱلْكِتَٰبِ ٱلْمُبِينِ",
          "numberInSurah": 2,
          "juz": 25,
          "manzil": 6,
          "page": 496,
          "ruku": 431,
          "hizbQuarter": 198,
          "sajda": false
        },
        {
          "number": 4417,
          "text": "إِنَّآ أَنزَلْنَٰهُ فِى لَيْلَةٍۢ مُّبَٰرَكَةٍ ۚ إِنَّا كُنَّا مُنذِرِينَ",
          "numberInSurah": 3,
          "juz": 25,
          "manzil": 6,
          "page": 496,
          "ruku": 431,
          "hizbQuarter": 198,
          "sajda": false
        },
        {
          "number": 4418,
          "text": "فِيهَا يُفْرَقُ كُلُّ أَمْرٍ حَكِيمٍ",
          "numberInSurah": 4,
          "juz": 25,
          "manzil": 6,
          "page": 496,
          "ruku": 431,
          "hizbQuarter": 198,
          "sajda": false
        },
        {
          "number": 4419,
          "text": "أَمْرًۭا مِّنْ عِندِنَآ ۚ إِنَّا كُنَّا مُرْسِلِينَ",
          "numberInSurah": 5,
          "juz": 25,
          "manzil": 6,
          "page": 496,
          "ruku": 431,
          "hizbQuarter": 198,
          "sajda": false
        },
        {
          "number": 4420,
          "text": "رَحْمَةًۭ مِّن رَّبِّكَ ۚ إِنَّهُۥ هُوَ ٱلسَّمِيعُ ٱلْعَلِيمُ",
          "numberInSurah": 6,
          "juz": 25,
          "manzil": 6,
          "page": 496,
          "ruku": 431,
          "hizbQuarter": 198,
          "sajda": false
        },
        {
          "number": 4421,
          "text": "رَبِّ ٱلسَّمَٰوَٰتِ وَٱلْأَرْضِ وَمَا بَيْنَهُمَآ ۖ إِن كُنتُم مُّوقِنِينَ",
          "numberInSurah": 7,
          "juz": 25,
          "manzil": 6,
          "page": 496,
          "ruku": 431,
          "hizbQuarter": 198,
          "sajda": false
        },
        {
          "number": 4422,
          "text": "لَآ إِلَٰهَ إِلَّا هُوَ يُحْىِۦ وَيُمِيتُ ۖ رَبُّكُمْ وَرَبُّ ءَابَآئِكُمُ ٱلْأَوَّلِينَ",
          "numberInSurah": 8,
          "juz": 25,
          "manzil": 6,
          "page": 496,
          "ruku": 431,
          "hizbQuarter": 198,
          "sajda": false
        },
        {
          "number": 4423,
          "text": "بَلْ هُمْ فِى شَكٍّۢ يَلْعَبُونَ",
          "numberInSurah": 9,
          "juz": 25,
          "manzil": 6,
          "page": 496,
          "ruku": 431,
          "hizbQuarter": 198,
          "sajda": false
        },
        {
          "number": 4424,
          "text": "فَٱرْتَقِبْ يَوْمَ تَأْتِى ٱلسَّمَآءُ بِدُخَانٍۢ مُّبِينٍۢ",
          "numberInSurah": 10,
          "juz": 25,
          "manzil": 6,
          "page": 496,
          "ruku": 431,
          "hizbQuarter": 198,
          "sajda": false
        },
        {
          "number": 4425,
          "text": "يَغْشَى ٱلنَّاسَ ۖ هَٰذَا عَذَابٌ أَلِيمٌۭ",
          "numberInSurah": 11,
          "juz": 25,
          "manzil": 6,
          "page": 496,
          "ruku": 431,
          "hizbQuarter": 198,
          "sajda": false
        },
        {
          "number": 4426,
          "text": "رَّبَّنَا ٱكْشِفْ عَنَّا ٱلْعَذَابَ إِنَّا مُؤْمِنُونَ",
          "numberInSurah": 12,
          "juz": 25,
          "manzil": 6,
          "page": 496,
          "ruku": 431,
          "hizbQuarter": 198,
          "sajda": false
        },
        {
          "number": 4427,
          "text": "أَنَّىٰ لَهُمُ ٱلذِّكْرَىٰ وَقَدْ جَآءَهُمْ رَسُولٌۭ مُّبِينٌۭ",
          "numberInSurah": 13,
          "juz": 25,
          "manzil": 6,
          "page": 496,
          "ruku": 431,
          "hizbQuarter": 198,
          "sajda": false
        },
        {
          "number": 4428,
          "text": "ثُمَّ تَوَلَّوْا۟ عَنْهُ وَقَالُوا۟ مُعَلَّمٌۭ مَّجْنُونٌ",
          "numberInSurah": 14,
          "juz": 25,
          "manzil": 6,
          "page": 496,
          "ruku": 431,
          "hizbQuarter": 198,
          "sajda": false
        },
        {
          "number": 4429,
          "text": "إِنَّا كَاشِفُوا۟ ٱلْعَذَابِ قَلِيلًا ۚ إِنَّكُمْ عَآئِدُونَ",
          "numberInSurah": 15,
          "juz": 25,
          "manzil": 6,
          "page": 496,
          "ruku": 431,
          "hizbQuarter": 198,
          "sajda": false
        },
        {
          "number": 4430,
          "text": "يَوْمَ نَبْطِشُ ٱلْبَطْشَةَ ٱلْكُبْرَىٰٓ إِنَّا مُنتَقِمُونَ",
          "numberInSurah": 16,
          "juz": 25,
          "manzil": 6,
          "page": 496,
          "ruku": 431,
          "hizbQuarter": 198,
          "sajda": false
        },
        {
          "number": 4431,
          "text": "۞ وَلَقَدْ فَتَنَّا قَبْلَهُمْ قَوْمَ فِرْعَوْنَ وَجَآءَهُمْ رَسُولٌۭ كَرِيمٌ",
          "numberInSurah": 17,
          "juz": 25,
          "manzil": 6,
          "page": 496,
          "ruku": 431,
          "hizbQuarter": 199,
          "sajda": false
        },
        {
          "number": 4432,
          "text": "أَنْ أَدُّوٓا۟ إِلَىَّ عِبَادَ ٱللَّهِ ۖ إِنِّى لَكُمْ رَسُولٌ أَمِينٌۭ",
          "numberInSurah": 18,
          "juz": 25,
          "manzil": 6,
          "page": 496,
          "ruku": 431,
          "hizbQuarter": 199,
          "sajda": false
        },
        {
          "number": 4433,
          "text": "وَأَن لَّا تَعْلُوا۟ عَلَى ٱللَّهِ ۖ إِنِّىٓ ءَاتِيكُم بِسُلْطَٰنٍۢ مُّبِينٍۢ",
          "numberInSurah": 19,
          "juz": 25,
          "manzil": 6,
          "page": 497,
          "ruku": 431,
          "hizbQuarter": 199,
          "sajda": false
        },
        {
          "number": 4434,
          "text": "وَإِنِّى عُذْتُ بِرَبِّى وَرَبِّكُمْ أَن تَرْجُمُونِ",
          "numberInSurah": 20,
          "juz": 25,
          "manzil": 6,
          "page": 497,
          "ruku": 431,
          "hizbQuarter": 199,
          "sajda": false
        },
        {
          "number": 4435,
          "text": "وَإِن لَّمْ تُؤْمِنُوا۟ لِى فَٱعْتَزِلُونِ",
          "numberInSurah": 21,
          "juz": 25,
          "manzil": 6,
          "page": 497,
          "ruku": 431,
          "hizbQuarter": 199,
          "sajda": false
        },
        {
          "number": 4436,
          "text": "فَدَعَا رَبَّهُۥٓ أَنَّ هَٰٓؤُلَآءِ قَوْمٌۭ مُّجْرِمُونَ",
          "numberInSurah": 22,
          "juz": 25,
          "manzil": 6,
          "page": 497,
          "ruku": 431,
          "hizbQuarter": 199,
          "sajda": false
        },
        {
          "number": 4437,
          "text": "فَأَسْرِ بِعِبَادِى لَيْلًا إِنَّكُم مُّتَّبَعُونَ",
          "numberInSurah": 23,
          "juz": 25,
          "manzil": 6,
          "page": 497,
          "ruku": 431,
          "hizbQuarter": 199,
          "sajda": false
        },
        {
          "number": 4438,
          "text": "وَٱتْرُكِ ٱلْبَحْرَ رَهْوًا ۖ إِنَّهُمْ جُندٌۭ مُّغْرَقُونَ",
          "numberInSurah": 24,
          "juz": 25,
          "manzil": 6,
          "page": 497,
          "ruku": 431,
          "hizbQuarter": 199,
          "sajda": false
        },
        {
          "number": 4439,
          "text": "كَمْ تَرَكُوا۟ مِن جَنَّٰتٍۢ وَعُيُونٍۢ",
          "numberInSurah": 25,
          "juz": 25,
          "manzil": 6,
          "page": 497,
          "ruku": 431,
          "hizbQuarter": 199,
          "sajda": false
        },
        {
          "number": 4440,
          "text": "وَزُرُوعٍۢ وَمَقَامٍۢ كَرِيمٍۢ",
          "numberInSurah": 26,
          "juz": 25,
          "manzil": 6,
          "page": 497,
          "ruku": 431,
          "hizbQuarter": 199,
          "sajda": false
        },
        {
          "number": 4441,
          "text": "وَنَعْمَةٍۢ كَانُوا۟ فِيهَا فَٰكِهِينَ",
          "numberInSurah": 27,
          "juz": 25,
          "manzil": 6,
          "page": 497,
          "ruku": 431,
          "hizbQuarter": 199,
          "sajda": false
        },
        {
          "number": 4442,
          "text": "كَذَٰلِكَ ۖ وَأَوْرَثْنَٰهَا قَوْمًا ءَاخَرِينَ",
          "numberInSurah": 28,
          "juz": 25,
          "manzil": 6,
          "page": 497,
          "ruku": 431,
          "hizbQuarter": 199,
          "sajda": false
        },
        {
          "number": 4443,
          "text": "فَمَا بَكَتْ عَلَيْهِمُ ٱلسَّمَآءُ وَٱلْأَرْضُ وَمَا كَانُوا۟ مُنظَرِينَ",
          "numberInSurah": 29,
          "juz": 25,
          "manzil": 6,
          "page": 497,
          "ruku": 431,
          "hizbQuarter": 199,
          "sajda": false
        },
        {
          "number": 4444,
          "text": "وَلَقَدْ نَجَّيْنَا بَنِىٓ إِسْرَٰٓءِيلَ مِنَ ٱلْعَذَابِ ٱلْمُهِينِ",
          "numberInSurah": 30,
          "juz": 25,
          "manzil": 6,
          "page": 497,
          "ruku": 432,
          "hizbQuarter": 199,
          "sajda": false
        },
        {
          "number": 4445,
          "text": "مِن فِرْعَوْنَ ۚ إِنَّهُۥ كَانَ عَالِيًۭا مِّنَ ٱلْمُسْرِفِينَ",
          "numberInSurah": 31,
          "juz": 25,
          "manzil": 6,
          "page": 497,
          "ruku": 432,
          "hizbQuarter": 199,
          "sajda": false
        },
        {
          "number": 4446,
          "text": "وَلَقَدِ ٱخْتَرْنَٰهُمْ عَلَىٰ عِلْمٍ عَلَى ٱلْعَٰلَمِينَ",
          "numberInSurah": 32,
          "juz": 25,
          "manzil": 6,
          "page": 497,
          "ruku": 432,
          "hizbQuarter": 199,
          "sajda": false
        },
        {
          "number": 4447,
          "text": "وَءَاتَيْنَٰهُم مِّنَ ٱلْءَايَٰتِ مَا فِيهِ بَلَٰٓؤٌۭا۟ مُّبِينٌ",
          "numberInSurah": 33,
          "juz": 25,
          "manzil": 6,
          "page": 497,
          "ruku": 432,
          "hizbQuarter": 199,
          "sajda": false
        },
        {
          "number": 4448,
          "text": "إِنَّ هَٰٓؤُلَآءِ لَيَقُولُونَ",
          "numberInSurah": 34,
          "juz": 25,
          "manzil": 6,
          "page": 497,
          "ruku": 432,
          "hizbQuarter": 199,
          "sajda": false
        },
        {
          "number": 4449,
          "text": "إِنْ هِىَ إِلَّا مَوْتَتُنَا ٱلْأُولَىٰ وَمَا نَحْنُ بِمُنشَرِينَ",
          "numberInSurah": 35,
          "juz": 25,
          "manzil": 6,
          "page": 497,
          "ruku": 432,
          "hizbQuarter": 199,
          "sajda": false
        },
        {
          "number": 4450,
          "text": "فَأْتُوا۟ بِـَٔابَآئِنَآ إِن كُنتُمْ صَٰدِقِينَ",
          "numberInSurah": 36,
          "juz": 25,
          "manzil": 6,
          "page": 497,
          "ruku": 432,
          "hizbQuarter": 199,
          "sajda": false
        },
        {
          "number": 4451,
          "text": "أَهُمْ خَيْرٌ أَمْ قَوْمُ تُبَّعٍۢ وَٱلَّذِينَ مِن قَبْلِهِمْ ۚ أَهْلَكْنَٰهُمْ ۖ إِنَّهُمْ كَانُوا۟ مُجْرِمِينَ",
          "numberInSurah": 37,
          "juz": 25,
          "manzil": 6,
          "page": 497,
          "ruku": 432,
          "hizbQuarter": 199,
          "sajda": false
        },
        {
          "number": 4452,
          "text": "وَمَا خَلَقْنَا ٱلسَّمَٰوَٰتِ وَٱلْأَرْضَ وَمَا بَيْنَهُمَا لَٰعِبِينَ",
          "numberInSurah": 38,
          "juz": 25,
          "manzil": 6,
          "page": 497,
          "ruku": 432,
          "hizbQuarter": 199,
          "sajda": false
        },
        {
          "number": 4453,
          "text": "مَا خَلَقْنَٰهُمَآ إِلَّا بِٱلْحَقِّ وَلَٰكِنَّ أَكْثَرَهُمْ لَا يَعْلَمُونَ",
          "numberInSurah": 39,
          "juz": 25,
          "manzil": 6,
          "page": 497,
          "ruku": 432,
          "hizbQuarter": 199,
          "sajda": false
        },
        {
          "number": 4454,
          "text": "إِنَّ يَوْمَ ٱلْفَصْلِ مِيقَٰتُهُمْ أَجْمَعِينَ",
          "numberInSurah": 40,
          "juz": 25,
          "manzil": 6,
          "page": 498,
          "ruku": 432,
          "hizbQuarter": 199,
          "sajda": false
        },
        {
          "number": 4455,
          "text": "يَوْمَ لَا يُغْنِى مَوْلًى عَن مَّوْلًۭى شَيْـًۭٔا وَلَا هُمْ يُنصَرُونَ",
          "numberInSurah": 41,
          "juz": 25,
          "manzil": 6,
          "page": 498,
          "ruku": 432,
          "hizbQuarter": 199,
          "sajda": false
        },
        {
          "number": 4456,
          "text": "إِلَّا مَن رَّحِمَ ٱللَّهُ ۚ إِنَّهُۥ هُوَ ٱلْعَزِيزُ ٱلرَّحِيمُ",
          "numberInSurah": 42,
          "juz": 25,
          "manzil": 6,
          "page": 498,
          "ruku": 432,
          "hizbQuarter": 199,
          "sajda": false
        },
        {
          "number": 4457,
          "text": "إِنَّ شَجَرَتَ ٱلزَّقُّومِ",
          "numberInSurah": 43,
          "juz": 25,
          "manzil": 6,
          "page": 498,
          "ruku": 433,
          "hizbQuarter": 199,
          "sajda": false
        },
        {
          "number": 4458,
          "text": "طَعَامُ ٱلْأَثِيمِ",
          "numberInSurah": 44,
          "juz": 25,
          "manzil": 6,
          "page": 498,
          "ruku": 433,
          "hizbQuarter": 199,
          "sajda": false
        },
        {
          "number": 4459,
          "text": "كَٱلْمُهْلِ يَغْلِى فِى ٱلْبُطُونِ",
          "numberInSurah": 45,
          "juz": 25,
          "manzil": 6,
          "page": 498,
          "ruku": 433,
          "hizbQuarter": 199,
          "sajda": false
        },
        {
          "number": 4460,
          "text": "كَغَلْىِ ٱلْحَمِيمِ",
          "numberInSurah": 46,
          "juz": 25,
          "manzil": 6,
          "page": 498,
          "ruku": 433,
          "hizbQuarter": 199,
          "sajda": false
        },
        {
          "number": 4461,
          "text": "خُذُوهُ فَٱعْتِلُوهُ إِلَىٰ سَوَآءِ ٱلْجَحِيمِ",
          "numberInSurah": 47,
          "juz": 25,
          "manzil": 6,
          "page": 498,
          "ruku": 433,
          "hizbQuarter": 199,
          "sajda": false
        },
        {
          "number": 4462,
          "text": "ثُمَّ صُبُّوا۟ فَوْقَ رَأْسِهِۦ مِنْ عَذَابِ ٱلْحَمِيمِ",
          "numberInSurah": 48,
          "juz": 25,
          "manzil": 6,
          "page": 498,
          "ruku": 433,
          "hizbQuarter": 199,
          "sajda": false
        },
        {
          "number": 4463,
          "text": "ذُقْ إِنَّكَ أَنتَ ٱلْعَزِيزُ ٱلْكَرِيمُ",
          "numberInSurah": 49,
          "juz": 25,
          "manzil": 6,
          "page": 498,
          "ruku": 433,
          "hizbQuarter": 199,
          "sajda": false
        },
        {
          "number": 4464,
          "text": "إِنَّ هَٰذَا مَا كُنتُم بِهِۦ تَمْتَرُونَ",
          "numberInSurah": 50,
          "juz": 25,
          "manzil": 6,
          "page": 498,
          "ruku": 433,
          "hizbQuarter": 199,
          "sajda": false
        },
        {
          "number": 4465,
          "text": "إِنَّ ٱلْمُتَّقِينَ فِى مَقَامٍ أَمِينٍۢ",
          "numberInSurah": 51,
          "juz": 25,
          "manzil": 6,
          "page": 498,
          "ruku": 433,
          "hizbQuarter": 199,
          "sajda": false
        },
