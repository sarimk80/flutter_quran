      "numberOfAyahs": 17,
      "ayahs": [
        {
          "number": 5932,
          "text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ وَٱلسَّمَآءِ وَٱلطَّارِقِ",
          "numberInSurah": 1,
          "juz": 30,
          "manzil": 7,
          "page": 591,
          "ruku": 528,
          "hizbQuarter": 236,
          "sajda": false
        },
        {
          "number": 5933,
          "text": "وَمَآ أَدْرَىٰكَ مَا ٱلطَّارِقُ",
          "numberInSurah": 2,
          "juz": 30,
          "manzil": 7,
          "page": 591,
          "ruku": 528,
          "hizbQuarter": 236,
          "sajda": false
        },
        {
          "number": 5934,
          "text": "ٱلنَّجْمُ ٱلثَّاقِبُ",
          "numberInSurah": 3,
          "juz": 30,
          "manzil": 7,
          "page": 591,
          "ruku": 528,
          "hizbQuarter": 236,
          "sajda": false
        },
        {
          "number": 5935,
          "text": "إِن كُلُّ نَفْسٍۢ لَّمَّا عَلَيْهَا حَافِظٌۭ",
          "numberInSurah": 4,
          "juz": 30,
          "manzil": 7,
          "page": 591,
          "ruku": 528,
          "hizbQuarter": 236,
          "sajda": false
        },
        {
          "number": 5936,
          "text": "فَلْيَنظُرِ ٱلْإِنسَٰنُ مِمَّ خُلِقَ",
          "numberInSurah": 5,
          "juz": 30,
          "manzil": 7,
          "page": 591,
          "ruku": 528,
          "hizbQuarter": 236,
          "sajda": false
        },
        {
          "number": 5937,
          "text": "خُلِقَ مِن مَّآءٍۢ دَافِقٍۢ",
          "numberInSurah": 6,
          "juz": 30,
          "manzil": 7,
          "page": 591,
          "ruku": 528,
          "hizbQuarter": 236,
          "sajda": false
        },
        {
          "number": 5938,
          "text": "يَخْرُجُ مِنۢ بَيْنِ ٱلصُّلْبِ وَٱلتَّرَآئِبِ",
          "numberInSurah": 7,
          "juz": 30,
          "manzil": 7,
          "page": 591,
          "ruku": 528,
          "hizbQuarter": 236,
          "sajda": false
        },
        {
          "number": 5939,
          "text": "إِنَّهُۥ عَلَىٰ رَجْعِهِۦ لَقَادِرٌۭ",
          "numberInSurah": 8,
          "juz": 30,
          "manzil": 7,
          "page": 591,
          "ruku": 528,
          "hizbQuarter": 236,
          "sajda": false
        },
        {
          "number": 5940,
          "text": "يَوْمَ تُبْلَى ٱلسَّرَآئِرُ",
          "numberInSurah": 9,
          "juz": 30,
          "manzil": 7,
          "page": 591,
          "ruku": 528,
          "hizbQuarter": 236,
          "sajda": false
        },
        {
          "number": 5941,
          "text": "فَمَا لَهُۥ مِن قُوَّةٍۢ وَلَا نَاصِرٍۢ",
          "numberInSurah": 10,
          "juz": 30,
          "manzil": 7,
          "page": 591,
          "ruku": 528,
          "hizbQuarter": 236,
          "sajda": false
        },
        {
          "number": 5942,
          "text": "وَٱلسَّمَآءِ ذَاتِ ٱلرَّجْعِ",
          "numberInSurah": 11,
          "juz": 30,
          "manzil": 7,
          "page": 591,
          "ruku": 528,
          "hizbQuarter": 236,
          "sajda": false
        },
        {
          "number": 5943,
          "text": "وَٱلْأَرْضِ ذَاتِ ٱلصَّدْعِ",
          "numberInSurah": 12,
          "juz": 30,
          "manzil": 7,
          "page": 591,
          "ruku": 528,
          "hizbQuarter": 236,
          "sajda": false
        },
        {
          "number": 5944,
          "text": "إِنَّهُۥ لَقَوْلٌۭ فَصْلٌۭ",
          "numberInSurah": 13,
          "juz": 30,
          "manzil": 7,
          "page": 591,
          "ruku": 528,
          "hizbQuarter": 236,
          "sajda": false
        },
        {
          "number": 5945,
          "text": "وَمَا هُوَ بِٱلْهَزْلِ",
          "numberInSurah": 14,
          "juz": 30,
          "manzil": 7,
          "page": 591,
          "ruku": 528,
          "hizbQuarter": 236,
          "sajda": false
        },
        {
          "number": 5946,
          "text": "إِنَّهُمْ يَكِيدُونَ كَيْدًۭا",
          "numberInSurah": 15,
          "juz": 30,
          "manzil": 7,
          "page": 591,
          "ruku": 528,
          "hizbQuarter": 236,
          "sajda": false
        },
        {
          "number": 5947,
          "text": "وَأَكِيدُ كَيْدًۭا",
